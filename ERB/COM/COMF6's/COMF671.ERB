@COM_ABLE671
;개별 설정 커멘드 필터
SIF DITEMTYPE:999:671 > 0
	RETURN 0
;조교자에게 페니스가 없으면 안 됨
SIF TALENT:PLAYER:120 == 0 && TALENT:PLAYER:121 == 0
	RETURN 0
;동물귀 이외 불가
SIF TALENT:116 == 0
	RETURN 0
;수면중은 불가
SIF EQUIP:14
	RETURN 0
;패닉중은 불가
SIF TFLAG:108 > 0
	RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
	RETURN 0
;줄사용중은 불가
SIF TEQUIP:40
	RETURN 0
;승마중은 안 됨
SIF TEQUIP:43
	RETURN 0
;조수가 곤착용중의 경우는 안 됨
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17
	RETURN 0
;00목욕탕 입욕중은 안 됨
SIF TEQUIP:55
	RETURN 0
;때 멈춤중은 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교중은 안 됨
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM671
PRINTL 꼬리 비비기

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
IF TFLAG:699 == 0
	CALL COM_ORDER
	;실행할 수 없다
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
TSTR:0 = 꼬리 비비기
CALL TRAIN_MESSAGE_COM671

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:조교자의 기교를 본다
IF ABL:PLAYER:12 == 0
	LOCAL = 1200
ELSEIF ABL:PLAYER:12 == 1
	LOCAL = 1700
ELSEIF ABL:PLAYER:12 == 2
	LOCAL = 2300
ELSEIF ABL:PLAYER:12 == 3
	LOCAL = 3000
ELSEIF ABL:PLAYER:12 == 4
	LOCAL = 3600
ELSE
	LOCAL = 4200
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)
;정액 중독을 본다
LOCAL = EJACULATIONCHECK_ABL_31_1(TARGET, LOCAL)

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 40
DOWNBASE:1 += 180

SOURCE:22 = 2000
SOURCE:31 = 600

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*40 + 100

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 450
	SOURCE:16 = 170
	SOURCE:32 = 150
	TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
	SOURCE:15 = 530
	SOURCE:16 = 350
	SOURCE:32 = 100
	TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
	SOURCE:15 = 630
	SOURCE:16 = 630
	SOURCE:32 = 75
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
	SOURCE:15 = 700
	SOURCE:16 = 1100
	SOURCE:32 = 45
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
	SOURCE:15 = 900
	SOURCE:16 = 1700
	SOURCE:32 = 15
	TIMES SOURCE:30 , 0.50
ELSE
	SOURCE:15 = 1000
	SOURCE:16 = 2500
	TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.30
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.50
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 0.80
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.20
ELSE
	TIMES SOURCE:15 , 2.00
	TIMES SOURCE:16 , 1.50
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	TIMES SOURCE:15 , 3.00

	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 4.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 500
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 3.00
	ELSEIF ABL:31 == 2
		SOURCE:24 = 1200
		TIMES SOURCE:16 , 4.00
		TIMES SOURCE:22, 2.50
	ELSEIF ABL:31 == 3
		SOURCE:24 = 3000
		TIMES SOURCE:16 , 6.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 4
		SOURCE:24 = 6000
		TIMES SOURCE:16 , 9.00
		TIMES SOURCE:22, 1.50
	ELSE
		SOURCE:24 = 12000
		TIMES SOURCE:16 , 15.00
		TIMES SOURCE:22, 1.00
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 23
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT

	;정액 더러움
	STAIN:7 |= 4
ENDIF

;대상의 머리카락의 더러운⇔조교자의 P의 더러움이 서로 이동
STAIN:7 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:7

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 7
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:50 += 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
	SOURCE:22 /= 2
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

RETURN 1

;--------------------------------------------------
;명령 거부의 메시지
;--------------------------------------------------
@DENIAL_MESSAGE_COM_671
CALL KOJO_DENIAL_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF ASSIPLAY
	PRINTFORML %CALLNAME:TARGET%에게 %CALLNAME:PLAYER%의 페니스를 꼬리로 비비게 했지만 거부했다.
ELSE
	PRINTFORML %CALLNAME:TARGET%에게 자신의 페니스를 꼬리로 비비게 했지만 거부했다.
ENDIF
PRINTW 좀 더 조교가 필요한 것 같다.

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM671
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 가까이 간 뒤 페니스를 꼬리에 묻고 비비기 시작했다.
PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 눈앞의 행위에 
IF TALENT:170 && ABL:31 >= 3
	PRINTFORMW \@(TEQUIP:42) ? 느슨한 표정으로 # 입을 벌리고\@ %CALLNAME:PLAYER%의 자위를 보고 있다.
ELSEIF TALENT:150 == 0 && TALENT:160 == 0 && MARK:9 > 1
	PRINTFORMW 굴욕과 분노의 시선으로 %조사선택(CALLNAME:PLAYER, "를")% 보고 있다.
ELSEIF ABL:31 == 0 || ABL:12 <= 1
	PRINTW 눈을 돌려버렸다….
ELSEIF ABL:31 <= 3 || ABL:12 <= 3
	PRINTFORMW 얼굴을 빨갛게 하며 %CALLNAME:PLAYER%의 자위를 보고 있다.
ELSEIF ASSIPLAY == 0 && (TALENT:152 || TALENT:153 || TALENT:162)
	PRINTL 유혹하듯 몸을 바짝 붙이면서 
	PRINTFORMW 언제라도 %CALLNAME:PLAYER%의 정액을 받을 수 있도록 페니스를 노려보고 있다….
ELSE
	PRINTFORMW 사정을 기대하는 것처럼 %CALLNAME:PLAYER%의 자위를 보고 있다.
ENDIF
PRINTL

;-------------------------------------------------
;고유의 실행 판정
;-------------------------------------------------
@COM_ORDER_671, ARG
LOCAL:99 = ARG
;ABL:욕망
IF ABL:11
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:11
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %ABLNAME:11%LV{LOCAL:99}({LOCAL:99})
ENDIF
;ABL:봉사 정신
IF ABL:13
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:13
	TFLAG:240 += LOCAL:99 * 4
	RESULTS = %RESULTS% %ABLNAME:13%LV{LOCAL:99}({LOCAL:99 * 4})
ENDIF
;ABL:정액 중독
IF ABL:31
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:31
	TFLAG:240 += LOCAL:99 * 3
	RESULTS = %RESULTS% %ABLNAME:31%LV{LOCAL:99}({LOCAL:99 * 3})
ENDIF

;쾌락 각인
IF MARK:1
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = MARK:1
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %MARKNAME:1%LV{LOCAL:99}({LOCAL:99})
ENDIF

;PALAM:욕정
GETPALAMLV PALAM:13, 5
LOCAL:2 = RESULT
IF LOCAL:2
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = LOCAL:2
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %PALAMNAME:13%LV{LOCAL:2}({LOCAL:99})
ENDIF

;부끄럼쟁이
IF TALENT:35
	RESULTS = %RESULTS% - 
	LOCAL:99 = 1
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:35%({LOCAL:99})
ENDIF
;남자 싫다
IF TALENT:37 && TALENT:PLAYER:120
	RESULTS = %RESULTS% - 
	LOCAL:99 = 12
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:37%({LOCAL:99})
;여자 증오
ELSEIF TALENT:38 && TALENT:PLAYER:120 == 0
	RESULTS = %RESULTS% - 
	LOCAL:99 = 12
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:38%({LOCAL:99})
ENDIF
;악취 둔감
IF TALENT:61
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 1
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:61%({LOCAL:99})
;악취 민감
ELSEIF TALENT:62
	RESULTS = %RESULTS% - 
	LOCAL:99 = 3
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:62%({LOCAL:99})
ENDIF
;헌신적
IF TALENT:63
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 6
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:63%({LOCAL:99})
ENDIF
;쾌감의 부정
IF TALENT:71
	RESULTS = %RESULTS% - 
	LOCAL:99 = 1
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:71%({LOCAL:99})
ENDIF
;연모
IF TALENT:150 && ASSIPLAY == 0
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 5
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:150%({LOCAL:99})
ENDIF

;조교자가 후타나리
IF TALENT:PLAYER:121
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 8
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %조사처리(CALLNAME:PLAYER,"가")% %TALENTNAME:121%({LOCAL:99})
ENDIF

;조교자의 P의 더러움
TFLAG:242 = COMORDER_STAIN_CALC(2, 0)

;서로 불결
IF TFLAG:242
	RESULTS = %RESULTS% - 
	TFLAG:240 -= TFLAG:242
	RESULTS = %RESULTS% 서로 불결
	;악취 둔감
	IF TALENT:61
		RESULTS = %RESULTS%, %TALENTNAME:61%
	ELSEIF TALENT:62
		RESULTS = %RESULTS%, %TALENTNAME:62%
	ENDIF
	RESULTS = %RESULTS%({TFLAG:242})
	LOCAL:99 = 1
ENDIF

