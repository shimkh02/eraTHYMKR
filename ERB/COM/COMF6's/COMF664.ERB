SIF TEQUIP:71
	RETURN 0
;촉수 조교중은 안 됨
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM664

;전회의 조교가 꼬리베개 한편, 플레이어의 기교 3이상 한편, 반발 각인 없음 한편, 호감도 500이상
;또는 전회의 조교가 꼬리베개, 라면 꼬리베개에
IF PREVCOM == 670 && ABL:PLAYER:12 >= 3 && MARK:9 == 0 && CFLAG:2 >= 500
	CALL EXCOM_ABLE672
	SIF RESULT == 1
		JUMP COM672
ENDIF

PRINTL 꼬리 파묻히기
TSTR:0 = 꼬리 파묻히기
CALL TRAIN_MESSAGE_COM664

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 5
DOWNBASE:1 += 30

SOURCE:10 = 90
SOURCE:11 = 110

;봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:10 = 30
	SOURCE:11 = 45
ELSEIF ABL:13 == 1
	SOURCE:10 = 70
	SOURCE:11 = 90
ELSEIF ABL:13 == 2
	SOURCE:10 = 120
	SOURCE:11 = 155
ELSEIF ABL:13 == 3
	SOURCE:10 = 250
	SOURCE:11 = 400
ELSEIF ABL:13 == 4
	SOURCE:10 = 350
	SOURCE:11 = 500
ELSE
	SOURCE:10 = 550
	SOURCE:11 = 700
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:10 , 0.80
	TIMES SOURCE:11 , 0.80
	SOURCE:18 = 10
	SOURCE:19 = 10
ELSEIF ABL:10 == 1
	TIMES SOURCE:10 , 1.00
	TIMES SOURCE:11 , 1.00
	SOURCE:18 = 60
	SOURCE:19 = 40
ELSEIF ABL:10 == 2
	TIMES SOURCE:10 , 1.20
	TIMES SOURCE:11 , 1.30
	SOURCE:18 = 120
	SOURCE:19 = 80
ELSEIF ABL:10 == 3
	TIMES SOURCE:10 , 1.40
	TIMES SOURCE:11 , 1.40
	SOURCE:18 = 240
	SOURCE:19 = 140
ELSEIF ABL:10 == 4
	TIMES SOURCE:10 , 1.50
	TIMES SOURCE:11 , 1.70
	SOURCE:18 = 360
	SOURCE:19 = 220
ELSE
	SOURCE:10 = SOURCE:10 * (ABL:10 * 10 + 120) / 100
	SOURCE:11 = SOURCE:11 * (ABL:10 * 15 + 125) / 100
	SOURCE:18 = 150 + (ABL:10 * 35)
	SOURCE:19 = 100 + (ABL:10 * 20)
ENDIF

;[괴뢰]와[정신 붕괴]와[괴조인격]을 가지지 않고,[연모]소유라면 애정 경험이 가산
SIF TALENT:150 && TALENT:167 == 0 && TALENT:168 == 0 && TALENT:169 == 0
	TCVAR:21 += 1

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF

RETURN 1

