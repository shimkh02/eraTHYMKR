TSTR:0 = %LOCALS%
CALL TRAIN_MESSAGE_COM629

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 500
ELSEIF ABL:12 == 1
	LOCAL = 1100
ELSEIF ABL:12 == 2
	LOCAL = 2000
ELSEIF ABL:12 == 3
	LOCAL = 3000
ELSEIF ABL:12 == 4
	LOCAL = 3900
ELSE
	LOCAL = 4600
ENDIF

;윤활을 본다
LOCAL = EJACULATIONCHECK_PALAM_10_5(TARGET, LOCAL)
;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF TALENT:PLAYER:120 || TALENT:PLAYER:121
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 45
DOWNBASE:1 += 190

SOURCE:10 = 650
SOURCE:12 = 350
SOURCE:22 = 1700
SOURCE:31 = 700

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*10 + 60

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 220
	SOURCE:16 = 120
	TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
	SOURCE:15 = 280
	SOURCE:16 = 210
	TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
	SOURCE:15 = 330
	SOURCE:16 = 280
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
	SOURCE:15 = 380
	SOURCE:16 = 380
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
	SOURCE:15 = 440
	SOURCE:16 = 540
	TIMES SOURCE:30 , 0.50
ELSE
	SOURCE:15 = 500
	SOURCE:16 = 850
	TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.70
	TIMES SOURCE:16 , 0.70
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.90
	TIMES SOURCE:16 , 0.90
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 1.40
	TIMES SOURCE:16 , 1.40
ELSE
	TIMES SOURCE:15 , 1.60
	TIMES SOURCE:16 , 1.60
ENDIF

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 20
	TIMES SOURCE:12 , 0.80
ELSEIF ABL:0 == 1
	SOURCE:0 = 80
	TIMES SOURCE:12 , 0.90
ELSEIF ABL:0 == 2
	SOURCE:0 = 350
	TIMES SOURCE:12 , 1.00
ELSEIF ABL:0 == 3
	SOURCE:0 = 750
	TIMES SOURCE:12 , 1.10
ELSEIF ABL:0 == 4
	SOURCE:0 = 1200
	TIMES SOURCE:12 , 1.20
ELSE
	SOURCE:0 = 1750
	TIMES SOURCE:12 , 1.30
ENDIF

;PALAM:윤활
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:0 , 0.30
	TIMES SOURCE:15 , 0.60
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:15 , 0.80
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:15 , 1.20
ELSEIF PALAM:10 < PALAMLV:5
	TIMES SOURCE:0 , 2.00
	TIMES SOURCE:15 , 1.40
ELSE
	TIMES SOURCE:0 , 2.50
	TIMES SOURCE:15 , 1.60
ENDIF

;조수의 ABL:C감각을 본다
IF ABL:ASSI:0 == 0
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.50
ELSEIF ABL:ASSI:0 == 1
	TIMES SOURCE:15 , 0.90
	TIMES SOURCE:16 , 0.70
ELSEIF ABL:ASSI:0 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:ASSI:0 == 3
	TIMES SOURCE:15 , 1.10
	TIMES SOURCE:16 , 1.20
ELSEIF ABL:ASSI:0 == 4
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.40
ELSE
	TIMES SOURCE:15 , 1.30
	TIMES SOURCE:16 , 1.70
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(MASTER)

;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	TIMES SOURCE:15 , 2.00

	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 1.50
		TIMES SOURCE:22, 1.40
	ELSEIF ABL:31 == 1
		SOURCE:24 = 200
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 2
		SOURCE:24 = 400
		TIMES SOURCE:16 , 2.50
		TIMES SOURCE:22, 0.80
	ELSEIF ABL:31 == 3
		SOURCE:24 = 700
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 0.50
	ELSEIF ABL:31 == 4
		SOURCE:24 = 1000
		TIMES SOURCE:16 , 4.00
		TIMES SOURCE:22, 0.20
	ELSE
		SOURCE:24 = 1500
		TIMES SOURCE:16 , 5.00
		TIMES SOURCE:22, 0.00
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 5
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
;노예의 V⇔조교자의 P의 불결이 이동
STAIN:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:3

;조수의 V⇔조교자의 P의 불결이 이동
STAIN:ASSI:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:ASSI:3

;노예의 V⇔조수의 V의 불결이 이동
STAIN:3 |= STAIN:ASSI:3
STAIN:ASSI:3 |= STAIN:3

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 7
ENDIF
IF TALENT:MASTER:120
	TCVAR:41 += 1
	TCVAR:ASSI:41 += 1
ENDIF
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
	SOURCE:22 /= 2

