	CALL COM_ORDER, 657
	;실행할 수 없다
	SIF RESULT == 0
		RETURN 0
ENDIF

TSTR:0 = 거미먹이 플레이
SELECTCOM = 657
CALL TRAIN_MESSAGE_COM657

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;조수의 ABL:기교를 본다
IF ABL:ASSI:12 == 0
	LOCAL = 50
ELSEIF ABL:ASSI:12 == 1
	LOCAL = 300
ELSEIF ABL:ASSI:12 == 2
	LOCAL = 800
ELSEIF ABL:ASSI:12 == 3
	LOCAL = 1500
ELSEIF ABL:ASSI:12 == 4
	LOCAL = 2000
ELSE
	LOCAL = 3200
ENDIF

;조수의 순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(ASSI, LOCAL)
;조수의 봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(ASSI, LOCAL)
;조수의 V감각을 본다
LOCAL = EJACULATIONCHECK_ABL_1_1(ASSI, LOCAL)
;조수의 마조끼를 본다
LOCAL = EJACULATIONCHECK_ABL_21_2(ASSI, LOCAL)
;조수의 V경험을 본다
LOCAL = EJACULATIONCHECK_EXP_0_1(ASSI, LOCAL)

;조교 대상의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(TARGET, LOCAL)

SIF TALENT:MASTER:120 || TALENT:MASTER:121
	BASE:MASTER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 0
DOWNBASE:1 += 280

SOURCE:11 = 1200
SOURCE:31 = 2800

;ABL:욕망을 본다
IF ABL:11 == 0
	SOURCE:19 = 75
	SOURCE:22 = 3200
ELSEIF ABL:11 == 1
	SOURCE:19 = 150
	SOURCE:22 = 3800
ELSEIF ABL:11 == 2
	SOURCE:19 = 300
	SOURCE:22 = 4600
ELSEIF ABL:11 == 3
	SOURCE:19 = 550
	SOURCE:22 = 5400
ELSEIF ABL:11 == 4
	SOURCE:19 = 800
	SOURCE:22 = 6200
ELSE
	SOURCE:19 = 1000
	SOURCE:22 = 7000
ENDIF


;ABL:봉사 정신을 본다
IF ABL:13 == 0
	TIMES SOURCE:11 , 0.10
	TIMES SOURCE:19 , 0.50
ELSEIF ABL:13 == 1
	TIMES SOURCE:11 , 0.40
	TIMES SOURCE:19 , 0.80
ELSEIF ABL:13 == 2
	TIMES SOURCE:11 , 0.70
	TIMES SOURCE:19 , 1.00
ELSEIF ABL:13 == 3
	TIMES SOURCE:11 , 1.00
	TIMES SOURCE:19 , 1.50
ELSEIF ABL:13 == 4
	TIMES SOURCE:11 , 1.60
	TIMES SOURCE:19 , 2.00
ELSE
	TIMES SOURCE:11 , 2.00
	TIMES SOURCE:19 , 2.50
ENDIF

LOCAL:1 = 1000
;겁쟁이
SIF TALENT:10
	TIMES LOCAL:1 , 2.50
;기장
SIF TALENT:12
	TIMES LOCAL:1 , 0.50
;감정결여
SIF TALENT:22
	TIMES LOCAL:1 , 0.20
;자위하기 쉽다
SIF TALENT:60
	TIMES SOURCE:11 , 2.50
SOURCE:25 += LOCAL:1

;조수가 처녀
IF TALENT:ASSI:0
	PRINTL <조수가 처녀>
	CALL LOST_VIRGIN, TARGET, PLAYER
	IF TALENT:ASSI:0 == 1
		TIMES SOURCE:22 , 20.00
		TIMES SOURCE:31 , 3.00
		IF (CFLAG:21 & 4) == 0
			TCVAR:50 += 1
			CFLAG:21 |= 4
		ENDIF
		IF ABL:10 < 2
			PRINTFORML 그리고 %ABLNAME:10%이 2가 되었다. 
			ABL:10 = 2
		ENDIF
	ELSE
		TIMES SOURCE:22 , 15.00
		TIMES SOURCE:31 , 2.50
	ENDIF
	TFLAG:5 += TALENT:ASSI:0
	TALENT:ASSI:0 = 0
	STAIN:ASSI:3 |= 32
ENDIF
TCVAR:ASSI:0 += 1
SIF TALENT:MASTER:120 || TALENT:MASTER:121 || TALENT:MASTER:1
	CFLAG:ASSI:173 += 1

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 1
		TIMES SOURCE:22, 1.20
	ELSEIF ABL:31 == 2
		TIMES SOURCE:22, 1.50
	ELSEIF ABL:31 == 3
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 4
		TIMES SOURCE:22, 2.50
	ELSE
		TIMES SOURCE:22, 3.00
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 6
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
IF TALENT:MASTER:120 || TALENT:MASTER:121
	;주인의 P⇔조수의 V의 불결이 이동
	STAIN:MASTER:2 |= STAIN:ASSI:3
	STAIN:ASSI:3 |= STAIN:MASTER:2
ENDIF

IF TALENT:MASTER:120 == 0 && TALENT:ASSI:120 == 0
	TCVAR:ASSI:12 += 10
ELSEIF TALENT:MASTER:120 && TALENT:ASSI:120
	TCVAR:ASSI:13 += 10
ENDIF
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

;처녀 상실의 후에 페니스를 뽑고 있을까
TFLAG:101 = (TFLAG:101 == 2) ?  1 # 0
RETURN 1

