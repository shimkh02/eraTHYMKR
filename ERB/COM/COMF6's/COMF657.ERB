@COM_ABLE657
RETURN 0

@EXCOM_ABLE657
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16384) == 0
	RETURN 0
;ヤマメのスカートの中は蜘蛛の脚という設定でないとダメ
SIF (FLAG:15 & 65536) == 0
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;조교자가 天然처녀であり, 조교 대상の前でまだ一度も助手を犯すを行ったことが無い場合, 
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && TALENT:PLAYER:0 == 1 && (CFLAG:21 & 4) == 0
	RETURN 0
;조수가 없으면 불가
SIF ASSI < 0
	RETURN 0
;主人じゃないとダメ
SIF ASSIPLAY
	RETURN 0
;조수가 ヤマメでないとダメ
SIF NO:ASSI != 58
	RETURN 0
;조수가 オトコだとダメ
SIF TALENT:ASSI:120
	RETURN 0
;소인체형 보유자는, 쌍방이 조교자와 조교 대상이라면 OK
;그 외의 경우에는 조수가 소인체형のとき주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:ASSI:127 && (TALENT:MASTER:56 == 0 && TALENT:PLAYER:127 == 0)
	RETURN 0
;主人にペニスがない, かつペニスバンドもないとダメ
SIF EXIST_BAR(MASTER) == 0 && ITEM:2 == 0 && NOITEM == 0
	RETURN 0
;縄使用中か순종이 3이상 없으면 안 됨
SIF TEQUIP:40 == 0 && ABL:10 < 4
	RETURN 0
;蜘蛛の巣にかけていないとダメ
SIF TEQUIP:40 != 7
	RETURN 0
;조수가 코스프레 중에는 불가
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 > 1
	RETURN 0
;야외 플레이 중이 아니면 불가
SIF TEQUIP:52 == 0
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;時間帯が夜でないとだめ
SIF TIME == 0
	RETURN 0
;対象と助手の相性が良くないとダメ
SIF RELATION:(NO:ASSI) <= 100
	RETURN 0
RETURN 1

