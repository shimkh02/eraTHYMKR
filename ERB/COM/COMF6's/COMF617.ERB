	SOURCE:11 += 500
	SOURCE:19 += 250
	LOCAL:1 = 100
ELSE
	SOURCE:11 += 800
	SOURCE:19 += 400
	LOCAL:1 = 300
ENDIF

SOURCE:0 += LOCAL:1

SIF EXP:0 >= EXPLV:3
	SOURCE:1 += LOCAL:1

;TALENT:연모를 본다
IF TALENT:150
	TIMES SOURCE:11 , 3.00
	TIMES SOURCE:17 , 2.00
	TIMES SOURCE:24 , 2.00
ENDIF

TCVAR:0 += 1
IF PLAYER == MASTER
	TCVAR:42 += 1
	SIF TALENT:MASTER:120 || TALENT:MASTER:121 || TALENT:MASTER:1
		CFLAG:173 += 1
ENDIF

