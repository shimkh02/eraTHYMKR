;=============================================================================
;거미집 애널섹스
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
;더미 함수(커맨드를 직접 선택할 수는 없음)
@COM_ABLE656
RETURN 0

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM656
PRINTL 거미집 애널섹스
TSTR:0 = 거미집 항문간
SELECTCOM = 656
CALL TRAIN_MESSAGE_COM656

DOWNBASE:0 += 5
DOWNBASE:1 += 50

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 75
ELSEIF ABL:12 == 1
	LOCAL = 300
ELSEIF ABL:12 == 2
	LOCAL = 900
ELSEIF ABL:12 == 3
	LOCAL = 1800
ELSEIF ABL:12 == 4
	LOCAL = 2400
ELSE
	LOCAL = 3600
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 A감각을 본다
LOCAL = EJACULATIONCHECK_ABL_2_1(PLAYER, LOCAL)
;조교자의 A경험을 본다
LOCAL = EJACULATIONCHECK_EXP_1_1(PLAYER, LOCAL)

;조교 대상의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(TARGET, LOCAL)

SIF TALENT:120 || TALENT:121
	BASE:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:1 += 280

SOURCE:30 = 3000
SOURCE:22 = 2000
SOURCE:31 = 4800

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:22 , 0.80
	SOURCE:23 = 100
ELSEIF ABL:10 == 1
	TIMES SOURCE:22 , 0.90
	SOURCE:23 = 200
ELSEIF ABL:10 == 2
	TIMES SOURCE:22 , 1.00
	SOURCE:23 = 350
ELSEIF ABL:10 == 3
	TIMES SOURCE:22 , 1.10
	SOURCE:23 = 500
ELSEIF ABL:10 == 4
	TIMES SOURCE:22 , 1.20
	SOURCE:23 = 800
ELSE
	TIMES SOURCE:22 , 1.30
	SOURCE:23 = 1400
ENDIF

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 800
	SOURCE:15 = 1600
ELSEIF ABL:0 == 1
	SOURCE:0 = 1400
	SOURCE:15 = 1900
ELSEIF ABL:0 == 2
	SOURCE:0 = 2000
	SOURCE:15 = 2300
ELSEIF ABL:0 == 3
	SOURCE:0 = 2500
	SOURCE:15 = 2700
ELSEIF ABL:0 == 4
	SOURCE:0 = 2900
	SOURCE:15 = 3100
ELSE
	SOURCE:0 = 3200
	SOURCE:15 = 3500
ENDIF

;ABL:기교를 본다
IF ABL:PLAYER:12 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 0.50
ELSEIF ABL:PLAYER:12 == 1
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:PLAYER:12 == 2
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:PLAYER:12 == 3
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:PLAYER:12 == 4
	TIMES SOURCE:0 , 2.50
	TIMES SOURCE:15 , 2.50
ELSE
	TIMES SOURCE:0 , 4.00
	TIMES SOURCE:15 , 4.00
ENDIF

TCVAR:PLAYER:1 += 1
SIF ASSIPLAY == 0 && (TALENT:120 || TALENT:121 || TALENT:1)
	CFLAG:176 += 1

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(TARGET)

;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	TIMES SOURCE:15 , 3.00

	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:0 , 2.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 50
		TIMES SOURCE:0 , 2.50
		TIMES SOURCE:22, 1.60
	ELSEIF ABL:31 == 2
		SOURCE:24 = 150
		TIMES SOURCE:0 , 3.00
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 3
		SOURCE:24 = 300
		TIMES SOURCE:0 , 4.50
		TIMES SOURCE:22, 0.70
	ELSEIF ABL:31 == 4
		SOURCE:24 = 600
		TIMES SOURCE:0 , 6.00
		TIMES SOURCE:22, 0.40
	ELSE
		SOURCE:24 = 1200
		TIMES SOURCE:0 , 8.00
		TIMES SOURCE:22, 0.10
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 9
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;노예 사정 판정이 끝난 플래그
	TFLAG:705 = 1
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

IF TALENT:120 || TALENT:121
	;노예의 P⇔조교자의 A의 불결이 이동
	STAIN:2 |= STAIN:PLAYER:4
	STAIN:PLAYER:4 |= STAIN:2
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 4
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 4
ENDIF
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

;처녀 상실의 후에 페니스를 뽑고 있을까
TFLAG:101 = (TFLAG:101 == 2) ?  1 # 0
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM656
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
;전회와 이번 조교자가 같아, 전회의 조교가 역어널 강간이나 거미집 항문간이며, 뽑지 않는 플래그가 있는 경우
IF TFLAG:102 && (PREVCOM == 35 || PREVCOM == 656) && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))
	PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 페니스를 꽉 문 채로, 집요하게 허리를 흔들었다….
ELSE
	PRINTFORM %조사선택(CALLNAME:PLAYER, "는")% 
	SIF TALENT:PLAYER:81
		PRINT 가학적인 미소를 띄워, 
	PRINTFORM 거미집에게 붙어있는 %CALLNAME:TARGET%의 위에 올라타, 
	SIF PALAM:10 >= PALAMLV:4
		PRINT 점액 투성이가 된 
	PRINTL 항문으로 천천히 물건을 삼켜 갔다….
ENDIF
PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
IF PALAM:17 >= PALAMLV:3
	PRINTFORMW 어떻게든 거미집에서 탈출하려고 날뛰고 있다….
ELSEIF PALAM:13 >= PALAMLV:3 && PALAM:12 >= PALAMLV:3
	PRINTFORMW 거미집에 걸려있다는 상황에 욕정해 버리고 말았다….
ELSE
	PRINTFORMW 가만히 있다….
ENDIF
