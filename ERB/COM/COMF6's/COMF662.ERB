;=============================================================================
;꼬리 애널 삽입
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE662
;개별 설정 커멘드 필터
SIF DITEMTYPE:999:662 > 0
	RETURN 0
;동물귀 이외 불가
SIF TALENT:116 == 0
	RETURN 0
;A계 확장 커멘드를 유효하게 하지 않으면 안 됨
SIF (FLAG:15 & 4) == 0
	RETURN 0
;조수 조교 혹은 순애 모드가 무효의 경우는, A조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 2 && ((FLAG:12 & 32) == 0 || ASSIPLAY)
	RETURN 0
;윤활 부족해, 순종 4 미만·레즈비언기분 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새디즘이나 새디즘기분 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:10 < 4 || (TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:22 < 4)) && TALENT:ASSI:81 == 0 && ABL:ASSI:20 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
ENDIF
;A계 장착도구 사용중은 불가
SIF TEQUIP:25 || TEQUIP:26
	RETURN 0
;승마중은 안 됨
SIF TEQUIP:43
	RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕중은 안 됨
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;옴트프레이중은 안 됨
SIF TEQUIP:58
	RETURN 0
;곤착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:12 < 4
	RETURN 0
;촉수 조교중은 안 됨
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM662

PRINTL 꼬리 애널 삽입
TSTR:0 = 꼬리 애널 삽입
CALL TRAIN_MESSAGE_COM662

DOWNBASE:0 += 150
DOWNBASE:1 += 200

SOURCE:12 = 350
SOURCE:15 = 100
SOURCE:30 = 50
;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 50
	SOURCE:22 = 175
	SOURCE:25 = 1200
	SOURCE:30 = 1000
ELSEIF ABL:2 == 1
	SOURCE:2 = 100
	SOURCE:22 = 260
	SOURCE:25 = 600
	SOURCE:30 = 500
ELSEIF ABL:2 == 2
	SOURCE:2 = 350
	SOURCE:22 = 450
	SOURCE:25 = 450
	SOURCE:30 = 300
ELSEIF ABL:2 == 3
	SOURCE:2 = 700
	SOURCE:18 = 100
	SOURCE:22 = 900
	SOURCE:25 = 200
	SOURCE:30 = 150
ELSEIF ABL:2 == 4
	SOURCE:2 = 1500
	SOURCE:18 = 500
	SOURCE:22 = 1000
	SOURCE:25 = 100
	SOURCE:30 = 100
ELSE
	SOURCE:2 = 2000
	SOURCE:18 = 1000
	SOURCE:22 = 1500
	SOURCE:30 = 50
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	TIMES SOURCE:18 , 0.00
	TIMES SOURCE:22 , 0.20
	TIMES SOURCE:25 , 4.00
	TIMES SOURCE:30 , 4.00
	SOURCE:20 = 30000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:18 , 0.50
	TIMES SOURCE:22 , 0.50
	TIMES SOURCE:25 , 2.00
	TIMES SOURCE:30 , 2.00
	SOURCE:20 = 18000
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:18 , 1.00
	TIMES SOURCE:22 , 0.80
	TIMES SOURCE:25 , 1.00
	TIMES SOURCE:30 , 1.00
	SOURCE:20 = 8000
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:18 , 1.20
	TIMES SOURCE:22 , 1.00
	TIMES SOURCE:25 , 0.50
	TIMES SOURCE:30 , 0.50
	SOURCE:20 = 3000
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.80
	TIMES SOURCE:18 , 1.50
	TIMES SOURCE:22 , 1.10
	TIMES SOURCE:25 , 0.25
	TIMES SOURCE:30 , 0.25
	SOURCE:20 = 700
ELSE
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:18 , 1.75
	TIMES SOURCE:22 , 1.30
	TIMES SOURCE:25 , 0.10
	TIMES SOURCE:30 , 0.10
	SOURCE:20 = 100
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:20 += 700
	TIMES SOURCE:20 , 2.00
	TIMES SOURCE:25 , 2.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.20
	SOURCE:20 += 200
	TIMES SOURCE:20 , 1.60
	TIMES SOURCE:25 , 1.45
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:20 , 0.90
	TIMES SOURCE:25 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
	TIMES SOURCE:20 , 0.70
	TIMES SOURCE:25 , 0.50
ELSE
	TIMES SOURCE:2 , 2.20
	TIMES SOURCE:20 , 0.10
	TIMES SOURCE:25 , 0.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.30
	TIMES SOURCE:20 , 2.00
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.70
	TIMES SOURCE:20 , 1.45
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:20 , 0.90
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.80
	TIMES SOURCE:20 , 0.70
ELSE
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:20 , 0.30
	TIMES SOURCE:30 , 0.10
ENDIF

;A민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
ENDIF

RETURN 1

