;=============================================================================
;꼬리 삽입
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE661
;개별 설정 커멘드 필터
SIF DITEMTYPE:999:661 > 0
	RETURN 0
;동물귀 이외 불가
SIF TALENT:116 == 0
	RETURN 0
;V계 확장 커멘드를 유효하게 하지 않으면 안 됨
SIF (FLAG:15 & 2) == 0
	RETURN 0
;V조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 1
	RETURN 0
;대상의 V확장 경험이 0 때 이상계 조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 4 && EXP:32 == 0
	RETURN 0
;조수 조교 혹은 순애 모드가 무효의 경우는, V조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 1 && ((FLAG:12 & 32) == 0 || ASSIPLAY)
	RETURN 0
;남자라면 안 됨
SIF TALENT:120
	RETURN 0
;윤활 부족해, 순종 4 미만·레즈비언기분 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새디즘이나 새디즘기분 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:10 < 4 || (TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:22 < 4)) && TALENT:ASSI:81 == 0 && ABL:ASSI:20 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
ENDIF
;승마중은 안 됨
SIF TEQUIP:43
	RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕중은 안 됨
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;옴트프레이중은 안 됨
SIF TEQUIP:58
	RETURN 0
;곤착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:12 < 4
	RETURN 0
;촉수 조교중은 안 됨
SIF TEQUIP:90
	RETURN 0
;V계 장착도구 사용중은 불가
SIF TEQUIP:20 || TEQUIP:21
	RETURN 0
;A계 장착도구 사용중의 경우 조교자의 기교 4이상 필수
SIF (TEQUIP:25 || TEQUIP:26) && ABL:PLAYER:12 < 4
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM661
PRINTL 꼬리 삽입
TSTR:0 = 꼬리 삽입
CALL TRAIN_MESSAGE_COM661

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 50
DOWNBASE:1 += 100

SOURCE:12 = 300
SOURCE:15 = 300

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 1
	SOURCE:20 += 2500
	SOURCE:22 = 175
ELSEIF ABL:1 == 1
	SOURCE:1 = 10
	SOURCE:20 += 1000
	SOURCE:22 = 250
ELSEIF ABL:1 == 2
	SOURCE:1 = 300
	SOURCE:20 += 600
	SOURCE:22 = 450
ELSEIF ABL:1 == 3
	SOURCE:1 = 650
	SOURCE:20 += 300
	SOURCE:22 = 750
ELSEIF ABL:1 == 4
	SOURCE:1 = 1500
	SOURCE:22 = 1200
ELSE
	SOURCE:1 = 2000
	SOURCE:22 = 1800
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.10
	TIMES SOURCE:20 , 2.00
	TIMES SOURCE:22 , 0.20
	SOURCE:20 = 300
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.30
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 0.50
	SOURCE:20 = 180
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 0.90
	TIMES SOURCE:20 , 1.10
	TIMES SOURCE:22 , 0.80
	SOURCE:20 = 80
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.10
	TIMES SOURCE:20 , 1.00
	TIMES SOURCE:22 , 1.00
	SOURCE:20 = 30
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:20 , 0.80
	TIMES SOURCE:22 , 1.20
	SOURCE:20 = 0
ELSE
	TIMES SOURCE:1 , 1.90
	TIMES SOURCE:20 , 0.50
	TIMES SOURCE:22 , 1.50
	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:20 += 2500
	TIMES SOURCE:20 , 5.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:1 , 0.20
	SOURCE:20 += 1000
	TIMES SOURCE:20 , 1.50
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:20 , 0.75
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:20 , 0.20
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 0.20
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 0.50
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 1.70
ELSE
	TIMES SOURCE:1 , 2.00
ENDIF

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:103
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
ELSEIF TALENT:102
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	TIMES SOURCE:22 , 2.00

TCVAR:0 += 1
RETURN 1
