;=============================================================================
;오나홀
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE16
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:16 > 0
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용중의 경우는 안 됨
SIF TEQUIP:59 == 17
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;해제는 언제라도 가능
SIF TEQUIP:30 == 2
	RETURN 1
;조교 대상으로 페니스가 없으면 안 됨
SIF EXIST_BAR(TARGET) == 0
	RETURN 0
;아이템을 가지고 있는지, 아이템 없음 설정이 아니면 안 됨
SIF ITEM:16 == 0 && NOITEM == 0
	RETURN 0
;조교 대상이 [소인 체형]의 경우는 주인이 [금단의 지식] 소유가 아니면 안 됨
SIF TALENT:127 && TALENT:MASTER:56 == 0
	RETURN 0
;ASSI의 경우, 기교 3이상이 아니면 사용할 수 없다.
IF ASSIPLAY
	SIF ABL:ASSI:12 < 3
		RETURN 0
ENDIF
;다른 C계 장착도구 사용중은 불가
SIF TEQUIP:30 && TEQUIP:30 != 2
	RETURN 0
;콘돔 장착 중에는 불가
SIF TEQUIP:31
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;-------------------------------------------------------
;오나홀(SELECTCOM=16), 촉수 페니스 고문(SELECTCOM=505)
;-------------------------------------------------------
;촉수 페니스 고문은 COMF500.ERB로부터 JUMP 해 온다.
@COM16
IF TEQUIP:90
	PRINTL 촉수 페니스 고문
	TSTR:0 = 촉수 페니스 고문
ELSE
	TSTR:7 = 오나홀
	IF FLAG:1100 & 1
		;技師パッチ有効なら特殊な名前チェック
		FLAG:1110 = 16
		CALL NAMECHECK_CUSTOMTOOL
	ELSE
		TSTR:8 = %TSTR:7%
		TSTR:9 = %TSTR:7%
	ENDIF
	PRINTFORML %TSTR:8%
	TSTR:0 = 오나홀
ENDIF
CALL TRAIN_MESSAGE_COM16

DOWNBASE:0 += 10
DOWNBASE:1 += 80

SOURCE:12 = 120
SOURCE:31 = 70

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 200
ELSEIF ABL:0 == 1
	SOURCE:0 = 400
ELSEIF ABL:0 == 2
	SOURCE:0 = 900
ELSEIF ABL:0 == 3
	SOURCE:0 = 1600
ELSEIF ABL:0 == 4
	SOURCE:0 = 2400
ELSE
	SOURCE:0 = 3000
ENDIF

IF (FLAG:1100 & 1) && TFLAG:90 == 0
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 16
	FLAG:1111 = 1
	FLAG:1112 = SOURCE:0
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:0 = FLAG:1120
ENDIF

;오나홀의 착탈
IF TEQUIP:30
	TEQUIP:30 = 0
ELSE
	IF TEQUIP:90
		STAIN:3 |= 2
		STAIN:3 |= 4
		TEQUIP:30 = 2
	ENDIF
	TEQUIP:30 = 2
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF
SIF PLAYER == MASTER && TEQUIP:30 == 2
	TCVAR:41 += (TALENT:120) ?  1 # 2

;커맨드 속성：촉수
IF TEQUIP:90
	TFLAG:98 = 8
;커맨드 속성：도구(조교자)
ELSE
	TFLAG:98 = 1
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;-------------------------------------------------
;오나홀 장비중
;-------------------------------------------------
@EQUIP_COM16
IF TEQUIP:90
	PRINTL <촉수 페니스 고문 중>
ELSE
	TSTR:7 = 오나홀
	IF FLAG:1100 & 1
		;技師パッチ有効なら特殊な名前チェック
		FLAG:1110 = 16
		CALL NAMECHECK_CUSTOMTOOL
	ENDIF
	PRINTFORML <%TSTR:7% 장비 중>
ENDIF

DOWNBASE:0 += 5
DOWNBASE:1 += 20

;ABL:C감각을 본다
IF ABL:0 == 0
	LOCAL = 40
ELSEIF ABL:0 == 1
	LOCAL = 120
ELSEIF ABL:0 == 2
	LOCAL = 250
ELSEIF ABL:0 == 3
	LOCAL = 450
ELSEIF ABL:0 == 4
	LOCAL = 600
ELSE
	LOCAL = 750
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

IF (FLAG:1100 & 1) && TEQUIP:90 == 0
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 16
	FLAG:1111 = 0
	FLAG:1112 = LOCAL
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL = FLAG:1120
ENDIF

SOURCE:0 += LOCAL
SOURCE:12 += 50
SOURCE:18 += 50
;촉수 경험 플래그
SIF TEQUIP:90
	TFLAG:90 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM16
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:90
	IF TEQUIP:30
		IF ABL:35 > 2
			PRINTFORML 촉수가 %CALLNAME:TARGET%의 페니스를 해방했다.
			PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 아직 모자란다는 표정을 지었다….
		ELSEIF MARK:9 > 1
			PRINTFORML 촉수는 %CALLNAME:TARGET%의 페니스를 해방했다.
			PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 촉수에 의해 더럽혀진 페니스를 곤란한 표정으로 바라보고 있다….
		ELSE
			PRINTFORMW 촉수는 %CALLNAME:TARGET%의 페니스를 해방했다….
		ENDIF
	ELSE
		IF ABL:35 > 2
			PRINTFORML 불쾌하게 구불구불 휜 촉수가 %CALLNAME:TARGET%의 페니스에 감겨 비벼대기 시작했다.
			PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 그 자극에 \@(TEQUIP:42) ? # 환희의 신음을 흘리며 \@ 전신을 떨고 있다….
		ELSEIF MARK:9 > 1
			PRINTFORML 불쾌하게 구불구불 휜 촉수가 %CALLNAME:TARGET%의 페니스에 감겨 비벼대기 시작했다.
			PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 페니스가 자극에 반응할 때마다 분한 것 같은 표정을 하면서도 필사적으로 견디려 하고 있었다….
		ELSE
			PRINTFORMW 불쾌하게 구불구불 휜 촉수가 %CALLNAME:TARGET%의 페니스에 감겨 비벼대기 시작했다….
		ENDIF
	ENDIF
ELSE
	IF TEQUIP:30
		IF MARK:9 > 1
			PRINTFORMW %조사선택(CALLNAME:PLAYER, "가")% 손을 뻗기 전에, %조사선택(CALLNAME:TARGET, "는")% 페니스로부터 %조사선택(TSTR:7, "를")% 빼 던져버렸다.
		ELSEIF TALENT:153 && ASSIPLAY == 0
			PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 페니스로부터 %조사선택(TSTR:7, "를")% 뺐다.
			PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 다음엔 어디를 괴롭혀줄지 기대하면서 %조사선택(CALLNAME:PLAYER, "를")% 뜨거운 시선으로 바라보았다.
		ELSE
			PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 페니스로부터 %조사선택(TSTR:7, "를")% 뺐다.
		ENDIF
	ELSE
		IF MARK:9 > 1
			PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% 싫어서 버둥대는 %조사선택(CALLNAME:TARGET, "를")% 억눌렀다.
			PRINTFORMW 그리고, %CALLNAME:TARGET%의 페니스를 %TSTR:9%에 삽입했다….
		ELSE
			PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 페니스를 %TSTR:9%에 삽입했다….
		ENDIF
	ENDIF
ENDIF
