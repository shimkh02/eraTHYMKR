	TIMES SOURCE:1 , 3.00
	TIMES SOURCE:11 , 3.60
	TIMES SOURCE:32 , 1.00
ENDIF

;플레이어의 ABL:기교를 본다
IF ABL:PLAYER:12 == 0
	SOURCE:11 += 100
	SOURCE:19 += 0
