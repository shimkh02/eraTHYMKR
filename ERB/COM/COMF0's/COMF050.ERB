SIF COMMON_BODYSIZE_CHECK2(PLAYER, TARGET) > -2
	RETURN 0
;조교자에게 페니스가 없으면 안 됨
SIF EXIST_BAR(PLAYER) == 0
	RETURN 0
;수면중은 불가
SIF EQUIP:14
	RETURN 0
;패닉중은 불가
SIF TFLAG:108 > 0
	RETURN 0
