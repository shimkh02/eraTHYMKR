;커맨드 본체
;--------------------------------------------------
@COM50
PRINTL 전신잡

;-------------------------------------------------
;실행할 수 있는가 판정
;-------------------------------------------------
IF TFLAG:699 == 0
	CALL COM_ORDER
	;실행할 수 없다
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
TSTR:0 = 전신잡
CALL TRAIN_MESSAGE_COM050

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 450
ELSEIF ABL:12 == 1
	LOCAL = 1000
ELSEIF ABL:12 == 2
	LOCAL = 1600
ELSEIF ABL:12 == 3
	LOCAL = 2200
ELSEIF ABL:12 == 4
	LOCAL = 2700
ELSE
	LOCAL = 3200
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF TALENT:PLAYER:120 || TALENT:PLAYER:121
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 30
DOWNBASE:1 += 100

SOURCE:31 = 400

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*10 + 60

;ABL:Ｃ감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 20
ELSEIF ABL:0 == 1
	SOURCE:0 = 120
ELSEIF ABL:0 == 2
	SOURCE:0 = 640
ELSEIF ABL:0 == 3
	SOURCE:0 = 1200
ELSEIF ABL:0 == 4
	SOURCE:0 = 2300
ELSE
	SOURCE:0 = 3400
ENDIF

;ABL:Ｂ감각을 본다
IF ABL:3 == 0
	SOURCE:3 = 20
ELSEIF ABL:3 == 1
	SOURCE:3 = 120
ELSEIF ABL:3 == 2
	SOURCE:3 = 640
ELSEIF ABL:3 == 3
	SOURCE:3 = 1200
ELSEIF ABL:3 == 4
	SOURCE:3 = 2300
ELSE
	SOURCE:3 = 3400
ENDIF

;ABL:봉사정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 250
	SOURCE:16 = 50
	TIMES SOURCE:30 , 4.00
	SOURCE:22 = 40
ELSEIF ABL:13 == 1
	SOURCE:15 = 300
	SOURCE:16 = 100
	TIMES SOURCE:30 , 2.50
	SOURCE:22 = 160
ELSEIF ABL:13 == 2
	SOURCE:15 = 350
	SOURCE:16 = 200
	TIMES SOURCE:30 , 1.50
	SOURCE:22 = 700
ELSEIF ABL:13 == 3
	SOURCE:15 = 400
	SOURCE:16 = 300
	TIMES SOURCE:30 , 1.00
	SOURCE:22 = 1300
ELSEIF ABL:13 == 4
	SOURCE:15 = 450
	SOURCE:16 = 500
	TIMES SOURCE:30 , 0.50
	SOURCE:22 = 2200
ELSE
	SOURCE:15 = 500
	SOURCE:16 = 750
	TIMES SOURCE:30 , 0.10
	SOURCE:22 = 3100
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.50
	TIMES SOURCE:22 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.80
	TIMES SOURCE:22 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
	TIMES SOURCE:22 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.20
	TIMES SOURCE:22 , 1.50
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
	TIMES SOURCE:22 , 2.50
ELSE
	TIMES SOURCE:15 , 2.00
	TIMES SOURCE:16 , 2.00
	TIMES SOURCE:22 , 4.00
ENDIF

;도착적
SIF TALENT:80
	TIMES SOURCE:16 , 1.80

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

IF TFLAG:91
;射精している
	TIMES SOURCE:15 , 3.00

	;ABL:정액중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 200
		TIMES SOURCE:16 , 2.50
		TIMES SOURCE:22, 1.60
	ELSEIF ABL:31 == 2
		SOURCE:24 = 600
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 3
		SOURCE:24 = 1500
		TIMES SOURCE:16 , 4.00
		TIMES SOURCE:22, 0.70
	ELSEIF ABL:31 == 4
		SOURCE:24 = 3000
		TIMES SOURCE:16 , 5.00
		TIMES SOURCE:22, 0.40
	ELSE
		SOURCE:24 = 6000
		TIMES SOURCE:16 , 6.00
		TIMES SOURCE:22, 0.10
	ENDIF
ENDIF

;대량 사정
IF TFLAG:91 == 2
	TIMES SOURCE:24 , 2.00
	TIMES SOURCE:16 , 1.50

	TCVAR:PLAYER:3 += 2
	TCVAR:4 += 3
	PRINTL 대량사정

	;精液汚れ
	STAIN:PLAYER:2 |= 4
	STAIN:1 |= 4
	STAIN:2 |= 4
	STAIN:3 |= 4
	STAIN:5 |= 4
	STAIN:6 |= 4
	STAIN:7 |= 4

	;手で射精させたフラグ
	TFLAG:12 = TFLAG:91
;通常の射精
ELSEIF TFLAG:91 == 1
	TCVAR:PLAYER:3 += 1
	TCVAR:4 += 1
	PRINTL 사정

	;精液汚れ
	STAIN:PLAYER:2 |= 4
	STAIN:1 |= 4
	STAIN:2 |= 4
	STAIN:3 |= 4
	STAIN:5 |= 4
	STAIN:6 |= 4
	STAIN:7 |= 4

	;手で射精させたフラグ
	TFLAG:12 = TFLAG:91
ENDIF
BASE:PLAYER:2 = LIMIT(BASE:PLAYER:2 - EJAC*(TFLAG:91), 0, EJAC-1)

;노예의 体⇔조교자의 Ｐ의 불결이 이동
STAIN:1 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:1
STAIN:2 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:2
STAIN:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:3
STAIN:5 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:5
STAIN:6 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:6
STAIN:7 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:7

;LUNATIC以上は射精するたびに体力が減少
CALL EXHAUST_EJAC, PLAYER, TFLAG:91

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 4
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 4
ENDIF
;奉仕快楽経験入手判定
TFLAG:100 |= 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
	SOURCE:22 /= 2

RETURN 1

