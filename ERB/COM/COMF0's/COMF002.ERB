@COM_ABLE2
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:2 > 0
	RETURN 0
;A조교 필터가 오프되지 않았으면 불가
SIF FLAG:2 & 2
	RETURN 0
;윤활 부족해, 순종 4 미만·레즈끼(BL끼) 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새드나 새드끼 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:10 < 4 || ((TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:22 < 4) || (TALENT:120 && TALENT:ASSI:120 && ABL:ASSI:23 < 4))) && TALENT:ASSI:81 == 0 && ABL:ASSI:20 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
ENDIF
;그 조교중에 관장 플레이＋배설이 있었을 경우, 조교자가 더러운 무시 혹은 악취 둔감을 가지지 않으면 실행할 수 없다
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:12 < 4
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM2
PRINTL 애널 애무
TSTR:0 = 애널 애무
CALL TRAIN_MESSAGE_COM002

DOWNBASE:0 += 20
DOWNBASE:1 += 100

SOURCE:12 = 850
SOURCE:31 = 400

;ABL:Ａ감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 20
	SOURCE:22 = 300
ELSEIF ABL:2 == 1
	SOURCE:2 = 75
	SOURCE:22 = 350
ELSEIF ABL:2 == 2
	SOURCE:2 = 300
	SOURCE:22 = 400
ELSEIF ABL:2 == 3
	SOURCE:2 = 700
	SOURCE:22 = 650
ELSEIF ABL:2 == 4
	SOURCE:2 = 1100
	SOURCE:22 = 1000
ELSE
	SOURCE:2 = 1500
	SOURCE:22 = 1500
ENDIF

;EXP:Ａ경험을 본다
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.20
	TIMES SOURCE:22 , 0.20
	SOURCE:20 = 500
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:22 , 0.50
	SOURCE:20 = 400
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:22 , 1.00
	SOURCE:20 = 300
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:22 , 1.20
	SOURCE:20 = 200
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:22 , 1.60
	SOURCE:20 = 100
ELSE
	TIMES SOURCE:2 , 1.80
	TIMES SOURCE:22 , 1.80
	SOURCE:20 = 50
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.10
	TIMES SOURCE:22 , 0.10
	TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.20
	TIMES SOURCE:22 , 0.20
	TIMES SOURCE:20 , 2.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:22 , 1.00
	TIMES SOURCE:20 , 0.50
ELSE
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.30
	TIMES SOURCE:22 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:22 , 1.30
ELSE
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:22 , 1.60
ENDIF

;Ａ민감둔감을 본다
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀에다 정조관념
IF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3
ELSEIF TALENT:0 && TALENT:30
	SOURCE:22 /= 2
ENDIF

TCVAR:1 += 1

IF TEQUIP:90
	;노예의 Ａ에 촉수의 더러움이 붙는다
	STAIN:4 |= 2
	STAIN:4 |= 4
ELSE
	;노예의 Ａ⇔조교자의 손가락의 불결이 이동
	STAIN:4 |= STAIN:PLAYER:1
	STAIN:PLAYER:1 |= STAIN:4
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 2
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 5
ENDIF
RETURN 1

