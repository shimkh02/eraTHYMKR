@COM_ABLE44
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:44 > 0
	RETURN 0
;A조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 2
	RETURN 0
;대상이 고양이혀라면 고양이 페라 패치 유효하지 않으면 안 됨
SIF TALENT:54 && (FLAG:14 & 16) == 0
	RETURN 0
;고양이혀라면 기교 5·봉사 정신 4·순종 3이 필요
SIF TALENT:54 && (ABL:12 < 5 || ABL:13 < 4 || ABL:10 < 3)
	RETURN 0
;수면중은 불가
SIF EQUIP:14
	RETURN 0
;실신중은 불가
SIF TFLAG:899 > 0
	RETURN 0
;밧줄 사용 중에는 목분리시만 가능
SIF TEQUIP:40 && TALENT:230 == 0
	RETURN 0
;거미집이 결여되어 있을 때는 안 됨
SIF TEQUIP:40 == 7
	RETURN 0
;볼 개그 사용중은 불가
SIF TEQUIP:42
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43 && TALENT:230 == 0
	RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;조수가 훈도시 착용중의 경우 기교 4이상 필수
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17 && ABL:12 < 4
	RETURN 0
;나체정식 중에는 안 됨
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM44
PRINTL 애널 강제 핥기

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
IF TFLAG:699 == 0
	CALL COM_ORDER
	;실행할 수 없다
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
TSTR:0 = 애널 강제 핥기
CALL TRAIN_MESSAGE_COM044

;-------------------------------------------------
;사정 게이지 체크(후타나리의 경우)
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 100
ELSEIF ABL:12 == 1
	LOCAL = 300
ELSEIF ABL:12 == 2
	LOCAL = 500
ELSEIF ABL:12 == 3
	LOCAL = 800
ELSEIF ABL:12 == 4
	LOCAL = 1200
ELSE
	LOCAL = 1600
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;TALENT:혀사용을 본다
SIF TALENT:52
	TIMES LOCAL , 2.00
	
;TALENT:고양이혀을 본다
SIF TALENT:54
	TIMES LOCAL , 0.70

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF TALENT:PLAYER:120 || TALENT:PLAYER:121
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:1 += 100

SOURCE:22 = 1800
SOURCE:31 = 5000

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*80 + 50

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 420
	SOURCE:16 = 150
	TIMES SOURCE:22 , 0.80
	TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
	SOURCE:15 = 500
	SOURCE:16 = 300
	TIMES SOURCE:22 , 0.90
	TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
	SOURCE:15 = 580
	SOURCE:16 = 600
	TIMES SOURCE:22 , 1.00
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
	SOURCE:15 = 660
	SOURCE:16 = 900
	TIMES SOURCE:22 , 1.10
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
	SOURCE:15 = 740
	SOURCE:16 = 1500
	TIMES SOURCE:22 , 1.20
	TIMES SOURCE:30 , 0.50
ELSE
	SOURCE:15 = 820
	SOURCE:16 = 2200
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:30 , 0.10
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES SOURCE:22 , 0.80
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:21 == 1
	TIMES SOURCE:22 , 0.90
	TIMES SOURCE:30 , 1.20
ELSEIF ABL:21 == 2
	TIMES SOURCE:22 , 1.00
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:21 == 3
	TIMES SOURCE:22 , 1.20
	TIMES SOURCE:30 , 0.80
ELSEIF ABL:21 == 4
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:30 , 0.60
ELSE
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:30 , 0.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	SOURCE:11 = 20
ELSEIF PALAM:13 < PALAMLV:2
	SOURCE:11 = 50
ELSEIF PALAM:13 < PALAMLV:3
	SOURCE:11 = 100
ELSEIF PALAM:13 < PALAMLV:4
	SOURCE:11 = 200
ELSE
	SOURCE:11 = 400
ENDIF

;TALENT:헌신적을 본다
SIF TALENT:63
	TIMES SOURCE:11 , 2.00

;TALENT:도착적을 본다
IF TALENT:80
	TIMES SOURCE:11 , 1.50
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:30 , 0.70
	TIMES SOURCE:31 , 0.20
ENDIF

;TALENT:연모/망신/친애를 본다
IF (TALENT:150 || TALENT:151 || TALENT:152) && ASSIPLAY == 0
	TIMES SOURCE:11 , 2.50
	TIMES SOURCE:22 , 1.20
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 2.50
	TIMES SOURCE:16 , 2.50
ELSE
	TIMES SOURCE:15 , 4.00
	TIMES SOURCE:16 , 4.00
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	TIMES SOURCE:15 , 3.00

	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 200
		TIMES SOURCE:16 , 2.50
		TIMES SOURCE:22, 1.60
	ELSEIF ABL:31 == 2
		SOURCE:24 = 500
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 3
		SOURCE:24 = 1200
		TIMES SOURCE:16 , 4.50
		TIMES SOURCE:22, 0.70
	ELSEIF ABL:31 == 4
		SOURCE:24 = 2500
		TIMES SOURCE:16 , 6.00
		TIMES SOURCE:22, 0.40
	ELSE
		SOURCE:24 = 5000
		TIMES SOURCE:16 , 8.00
		TIMES SOURCE:22, 0.10
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 7
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

TCVAR:PLAYER:1 += 1

;노예의 입⇔조교자의 A의 불결이 이동
STAIN:0 |= STAIN:PLAYER:4
STAIN:PLAYER:4 |= STAIN:0

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 3
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 3
ENDIF
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

RETURN 1

