@COM20

;전회의 조교가 둘이서 범하는 것이라고 하고 둘이서 범하는에
TFLAG:132 = 0
IF PREVCOM == 610
	CALL EXCOM_ABLE610
	IF RESULT == 1
		TFLAG:132 = 1
		JUMP COM610
	ENDIF
;조교자가 전회:조수로 이번:주인인가, 전회:주인으로 이번:조수 때
ELSEIF (ASSIPLAY && TFLAG:133 == 0) || (ASSIPLAY == 0 && TFLAG:133)
	;전회의 조교가 후배위 애널인가 펠라치오나 이라마치오 때 둘이서 범하는에
	IF PREVCOM == 31 || PREVCOM == 41 || PREVCOM == 230
		CALL EXCOM_ABLE610
		SIF RESULT == 1
			JUMP COM610
	ENDIF
ENDIF

;전의 플레이가 정상위 또한 조교자의 기교 3이상의 경우, 삽입 G스포 꾸짖어 또는 삽입 자궁구 고문에 파생
;V감각이 높게 욕정하고 있는 만큼 자궁구에 가기 쉽다
IF FLAG:15 & 2
	IF PREVCOM == 20 && ABL:PLAYER:12 > 2 && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))
		;랜덤 배제시
		IF FLAG:14 & 8
			PRINTL 어디를 괴롭혀줄까?
			PRINTL [0] - G스팟
			PRINTL [1] - 자궁구
			$INPUT_LOOP
			INPUT
			IF RESULT == 0
				JUMP COM614
			ELSEIF RESULT == 1
				JUMP COM615
			ELSE
				GOTO INPUT_LOOP
			ENDIF
		ELSE
			;PALAM:욕정을 본다
			IF PALAM:13 < PALAMLV:1
				LOCAL = 1
			ELSEIF PALAM:13 < PALAMLV:2
				LOCAL = 2
			ELSEIF PALAM:13 < PALAMLV:3
				LOCAL = 3
			ELSEIF PALAM:13 < PALAMLV:4
				LOCAL = 4
			ELSE
				LOCAL = 5
			ENDIF
			LOCAL:1 = RAND:11
			LOCAL *= LOCAL:1
			LOCAL:1 = RAND:11
			LOCAL += (LOCAL:1) *ABL:1
			;LOCAL의 값은 0~100
			;정상위라면 40이상으로 자궁구
			IF LOCAL >= 40
				JUMP COM615
			ELSE
				JUMP COM614
			ENDIF
		ENDIF
	ENDIF
ENDIF

PRINTL 정상위
TSTR:0 = 정상위
CALL TRAIN_MESSAGE_COM020

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
