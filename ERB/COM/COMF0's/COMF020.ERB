SIF TEQUIP:60
	RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:98 <= 2
	RETURN 0
;강시는 불가
SIF TALENT:226 == 1
	RETURN 0
RETURN 1

