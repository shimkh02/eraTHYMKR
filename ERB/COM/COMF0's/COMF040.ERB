ELSEIF ABL:12 == 1
	LOCAL = 1000
ELSEIF ABL:12 == 2
	LOCAL = 1600
ELSEIF ABL:12 == 3
	LOCAL = 2200
ELSEIF ABL:12 == 4
	LOCAL = 2700
ELSE
	LOCAL = 3200
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
