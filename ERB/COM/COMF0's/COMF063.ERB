@EQUIP_COM63
PRINTL <좌약 효과 발휘 중>

DOWNBASE:0 += 50
DOWNBASE:1 += 30

LOCAL:1 = 300
LOCAL:2 = 100

;ABL:Ａ감각을 본다
IF ABL:2 == 0
	LOCAL = 5
ELSEIF ABL:2 == 1
	LOCAL = 10
ELSEIF ABL:2 == 2
	LOCAL = 20
ELSEIF ABL:2 == 3
	LOCAL = 50
ELSEIF ABL:2 == 4
	LOCAL = 100
ELSE
	LOCAL = 180
ENDIF

;Ａ민감둔감을 본다
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:104
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.50
ELSEIF TALENT:105
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
ENDIF
;약독내성
IF TALENT:44
	TIMES LOCAL , 0.50
	TIMES LOCAL:1 , 0.50
	TIMES LOCAL:2 , 1.50
ENDIF

SOURCE:2 += LOCAL
SOURCE:22 += LOCAL:1
SOURCE:31 += LOCAL:2

TCVAR:1 += 1

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 2
ENDIF
RETURN 1

