;조교자가 정조무구애
ELSEIF TALENT:PLAYER:31
	RESULTS = %RESULTS% + 
	LOCAL:99 = 5
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %조사처리(CALLNAME:PLAYER,"가")% 정조무구애({LOCAL:99})
	LOCAL:99 = 1
ENDIF

;조교자의 ABL:기교
IF ABL:PLAYER:12
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:PLAYER:12
