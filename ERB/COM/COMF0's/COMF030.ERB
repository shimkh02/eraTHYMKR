;커맨드 본체
;--------------------------------------------------
@COM30

;전회의 조교가 둘이서 범하는 것이라고 하고 둘이서 범하는에
TFLAG:132 = 0
IF PREVCOM == 610
	CALL EXCOM_ABLE610
	IF RESULT == 1
		TFLAG:132 = 1
		JUMP COM610
	ENDIF
;조교자가 전회:조수로 이번:주인인가, 전회:주인으로 이번:조수 때
ELSEIF (ASSIPLAY && TFLAG:133 == 0) || (ASSIPLAY == 0 && TFLAG:133)
	;전회의 조교가 펠라치오나 이라마치오 때는 둘이서 범하는에
	IF PREVCOM == 41 || PREVCOM == 230
		CALL EXCOM_ABLE610
		SIF RESULT == 1
			JUMP COM610
	ENDIF
ENDIF

PRINTL 정상위 애널
TSTR:0 = 정상위 애널
CALL TRAIN_MESSAGE_COM030

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 1500
ELSEIF ABL:12 == 1
	LOCAL = 1600
ELSEIF ABL:12 == 2
	LOCAL = 1800
ELSEIF ABL:12 == 3
	LOCAL = 2000
ELSEIF ABL:12 == 4
	LOCAL = 2400
ELSE
	LOCAL = 3000
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;욕망을 본다
LOCAL = EJACULATIONCHECK_ABL_11_1(TARGET, LOCAL)
;윤활을 본다
LOCAL = EJACULATIONCHECK_PALAM_10_1(TARGET, LOCAL)

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF TALENT:PLAYER:120 || TALENT:PLAYER:121
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 80
DOWNBASE:1 += 120

SOURCE:12 = 400

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 10
	SOURCE:11 = 10
	SOURCE:22 = 100
ELSEIF ABL:2 == 1
	SOURCE:2 = 30
	SOURCE:11 = 30
	SOURCE:22 = 500
ELSEIF ABL:2 == 2
	SOURCE:2 = 400
	SOURCE:11 = 150
	SOURCE:22 = 1200
ELSEIF ABL:2 == 3
	SOURCE:2 = 900
	SOURCE:11 = 300
	SOURCE:22 = 2400
ELSEIF ABL:2 == 4
	SOURCE:2 = 1600
	SOURCE:11 = 500
	SOURCE:22 = 3600
ELSE
	SOURCE:2 = 2100
	SOURCE:11 = 900
	SOURCE:22 = 5000
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:20 = 20000
	DOWNBASE:0 += 50
	DOWNBASE:1 += 100
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.30
	SOURCE:20 = 12000
	DOWNBASE:0 += 40
	DOWNBASE:1 += 80
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:20 = 5000
	DOWNBASE:0 += 30
	DOWNBASE:1 += 60
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:20 = 1800
	DOWNBASE:0 += 20
	DOWNBASE:1 += 40
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:20 = 1000
	DOWNBASE:0 += 10
	DOWNBASE:1 += 20
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:20 = 600
	DOWNBASE:0 += 0
	DOWNBASE:1 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 10000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 3600
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 1200
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 200
ELSE
	TIMES SOURCE:2 , 1.80
	SOURCE:20 += 100
ENDIF

;조교자가[남자]
IF TALENT:PLAYER:120
	TIMES SOURCE:2 , 2.50
;조교자가[후타나리]
ELSEIF TALENT:PLAYER:121
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.60
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;정조관념
IF TALENT:30
	IF EXP:1 == 0
		TIMES SOURCE:11 , 1.20
		SOURCE:32 = 500
	ELSE
		TIMES SOURCE:11 , 1.20
		SOURCE:32 = 0
	ENDIF
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:22 , 1.20
ELSE
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:22 , 1.40
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:11 , 0.60
	TIMES SOURCE:32 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:11 , 0.80
	TIMES SOURCE:32 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:11 , 1.00
	TIMES SOURCE:32 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:11 , 1.20
	TIMES SOURCE:32 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:11 , 1.40
	TIMES SOURCE:32 , 0.60
ELSE
	TIMES SOURCE:11 , 1.60
	TIMES SOURCE:32 , 0.30
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
ENDIF

TCVAR:1 += 2
SIF ASSIPLAY == 0 && (TALENT:MASTER:120 || TALENT:MASTER:121 || TALENT:MASTER:1)
	CFLAG:174 += 2

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
	IF ABL:10 >= 3 && ABL:11 >= 3 && FLAG:10
		PRINTFORML %조사선택(CALLNAME:TARGET, "는")% %CALLNAME:PLAYER%의 허리에 다리를 감고 꽉 눌렀다….
		PRINTL
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 2
	;사정처 선택의 가능·불가
	TFLAG:704 = 1
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
;노예의 A⇔조교자의 P의 불결이 이동
STAIN:4 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:4

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 7
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0
	TFLAG:50 += (ABL:2 >= 3) ?  2 # 1

;처녀 상실의 후에 페니스를 뽑고 있을까
TFLAG:101 = (TFLAG:101 == 2) ?  1 # 0
RETURN 1

