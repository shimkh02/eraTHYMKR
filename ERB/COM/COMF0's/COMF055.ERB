CALL TRAIN_MESSAGE_COM055

;-------------------------------------------------
;사정 게이지 체크(후타나리의 경우)
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 100
ELSEIF ABL:12 == 1
	LOCAL = 300
ELSEIF ABL:12 == 2
	LOCAL = 500
ELSEIF ABL:12 == 3
	LOCAL = 800
ELSEIF ABL:12 == 4
	LOCAL = 1200
ELSE
	LOCAL = 1600
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조교자의 Ｃ감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF TALENT:PLAYER:120 || TALENT:PLAYER:121
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:1 += 100

SOURCE:12 = 500
SOURCE:22 = 1200
SOURCE:31 = 1200

;위쪽에서 계산한 더러움 데이터
SOURCE:30 = TFLAG:242*70 + 40

;ABL:봉사정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 200
	SOURCE:16 = 50
	TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
	SOURCE:15 = 250
	SOURCE:16 = 100
	TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
	SOURCE:15 = 300
	SOURCE:16 = 150
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
	SOURCE:15 = 400
	SOURCE:16 = 300
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
	SOURCE:15 = 550
	SOURCE:16 = 600
	TIMES SOURCE:30 , 0.60
ELSE
	SOURCE:15 = 750
	SOURCE:16 = 1000
	TIMES SOURCE:30 , 0.30
ENDIF

;ABL:새드끼를 본다
IF ABL:20 == 0
	SOURCE:18 = 420
	TIMES SOURCE:30 , 1.20
ELSEIF ABL:20 == 1
	SOURCE:18 = 500
	TIMES SOURCE:30 , 1.10
ELSEIF ABL:20 == 2
	SOURCE:18 = 580
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:20 == 3
	SOURCE:18 = 660
	TIMES SOURCE:30 , 0.90
ELSEIF ABL:20 == 4
	SOURCE:18 = 740
	TIMES SOURCE:30 , 0.80
ELSE
	SOURCE:18 = 820
	TIMES SOURCE:30 , 0.70
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	SOURCE:11 = 20
	TIMES SOURCE:18 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
	SOURCE:11 = 50
	TIMES SOURCE:18 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
	SOURCE:11 = 100
	TIMES SOURCE:18 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	SOURCE:11 = 200
	TIMES SOURCE:18 , 1.30
ELSE
	SOURCE:11 = 400
	TIMES SOURCE:18 , 1.60
ENDIF

;TALENT:헌신적을 본다
SIF TALENT:63
	TIMES SOURCE:11 , 2.00

;TALENT:도착적을 본다
IF TALENT:80
	TIMES SOURCE:11 , 1.50
	TIMES SOURCE:12 , 1.50
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:30 , 0.70
	TIMES SOURCE:31 , 0.20
ENDIF

;TALENT:연모/망신/친애를 본다
IF (TALENT:150 || TALENT:151 || TALENT:152) && ASSIPLAY == 0
	TIMES SOURCE:11 , 2.50
	TIMES SOURCE:22 , 1.20
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.40
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.60
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 0.90
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 2.50
	TIMES SOURCE:16 , 1.50
ELSE
	TIMES SOURCE:15 , 4.00
	TIMES SOURCE:16 , 2.00
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)

;사정시의 처리
IF TFLAG:91 >= 1
	;コマンドごとに固有の処理はこの下に書く
	TIMES SOURCE:15 , 3.00

	;ABL:정액중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 300
		TIMES SOURCE:16 , 2.50
		TIMES SOURCE:22, 1.60
	ELSEIF ABL:31 == 2
		SOURCE:24 = 600
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 3
		SOURCE:24 = 1500
		TIMES SOURCE:16 , 4.50
		TIMES SOURCE:22, 0.70
	ELSEIF ABL:31 == 4
		SOURCE:24 = 3000
		TIMES SOURCE:16 , 6.00
		TIMES SOURCE:22, 0.40
	ELSE
		SOURCE:24 = 6000
		TIMES SOURCE:16 , 8.00
		TIMES SOURCE:22, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 3
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

TCVAR:PLAYER:1 += 1

;노예의 손가락⇔조교자의 Ａ의 불결이 이동
STAIN:1 |= STAIN:PLAYER:4
STAIN:PLAYER:4 |= STAIN:1

;同性ならレズ（ＢＬ）経験が入る
IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 4
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 6
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && EXP:20 >= EXPLV:3
	TFLAG:50 += 1
;奉仕快楽経験入手判定
TFLAG:100 |= 1

RETURN 1

