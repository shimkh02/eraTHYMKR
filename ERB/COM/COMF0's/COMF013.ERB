@COM_ABLE13
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:13 > 0
	RETURN 0
;A조교 필터가 오프가 되지 않으면 안 됨
SIF FLAG:2 & 2
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;한증과 암반 욕 이외의00목욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용중의 경우 조교자의 기교 4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:12 < 4
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;해제는 언제라도 가능
SIF TEQUIP:25 == 1
	RETURN 1
;아이템을 가지고 있는지, 아이템 없음 설정이 아니면 안 됨
SIF ITEM:9 == 0 && NOITEM == 0
	RETURN 0
;그 조교중에 관장 플레이＋배설이 있었을 경우, 조교자가 더러운 무시 혹은 악취 둔감을 가지지 않으면 실행할 수 없다
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
	RETURN 0
;조교 대상이 [소인 체형]의 경우는 주인이 [금단의 지식] 소유가 아니면 안 됨
SIF TALENT:127 && TALENT:MASTER:56 == 0
	RETURN 0
;윤활 부족해, 순종 4 미만·레즈끼(BL끼) 4 미만의 ASSI의 경우, 조교 대상이 동성이라면 실행할 수 없다(새드나 새드끼 Lv3 이상, 또는 망신이나 복종이 있다면 OK)
IF PALAM:10 < PALAMLV:3 && ASSIPLAY
	SIF (ABL:ASSI:10 < 4 || ((TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:22 < 4) || (TALENT:120 && TALENT:ASSI:120 && ABL:ASSI:23 < 4))) && TALENT:ASSI:81 == 0 && ABL:ASSI:20 < 3 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
;윤활이 너무 낮으면(자) 사용할 수 없다(조교자가 새디즘 또는 드 S 또는 새드끼 Lv3 이상이라면 OK)
ELSEIF PALAM:10 < PALAMLV:2 && TALENT:PLAYER:81 == 0 && TALENT:PLAYER:177 == 0 && ABL:PLAYER:20 < 3
	RETURN 0
ENDIF
;다른 A계 장착도구 사용중은 불가
SIF (TEQUIP:25 && TEQUIP:25 != 1) || TEQUIP:26
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;----------------------------------------------------------
;애널 바이브(SELECTCOM=13), 촉수 애널 삽입(SELECTCOM=502)
;----------------------------------------------------------
;촉수 애널 삽입은 COMF500.ERB로부터 JUMP 해 온다.
@COM13
IF TEQUIP:90
	PRINTL 애널 촉수
	TSTR:0 = 애널 촉수
ELSE
	TSTR:7 = 애널 바이브
	IF FLAG:1100 & 1
		;技師パッチ有効なら特殊な名前チェック
		FLAG:1110 = 9
		CALL NAMECHECK_CUSTOMTOOL
	ELSE
		TSTR:8 = %TSTR:7%
		TSTR:9 = %TSTR:7%
	ENDIF
	PRINTFORML %TSTR:8%
	TSTR:0 = 애널 바이브
ENDIF

DOWNBASE:0 += 60
DOWNBASE:1 += 150

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 80
	SOURCE:22 = 300
ELSEIF ABL:2 == 1
	SOURCE:2 = 250
	SOURCE:22 = 800
ELSEIF ABL:2 == 2
	SOURCE:2 = 600
	SOURCE:22 = 1400
ELSEIF ABL:2 == 3
	SOURCE:2 = 1000
	SOURCE:22 = 1800
ELSEIF ABL:2 == 4
	SOURCE:2 = 1300
	SOURCE:22 = 2100
ELSE
	SOURCE:2 = 1700
	SOURCE:22 = 2400
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:20 = 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:20 = 300
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:20 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:20 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:20 = 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 120
ELSE
	TIMES SOURCE:2 , 1.80
	SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSE
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
IF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3
ELSEIF TALENT:0 && TALENT:30
	SOURCE:22 /= 2
ENDIF

IF (FLAG:1100 & 1) && TEQUIP:90 == 0
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 9
	FLAG:1111 = 0
	FLAG:1112 = SOURCE:2
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:2 = FLAG:1120
ENDIF

CALL TRAIN_MESSAGE_COM13

;애널 바이브의 착탈
IF TEQUIP:25
	IF TEQUIP:90
		SIF TEQUIP:94
			CALL KANTYO_EGGPLAY_OUT
	ENDIF
	TEQUIP:25 = 0
ELSE
	IF TEQUIP:90
		STAIN:4 |= 2
		STAIN:4 |= 4
		STAIN:4 |= 64
		;애널의 직후라면 효과 증강
		IF PREVCOM == 45
			TIMES SOURCE:2 , 1.50
			TIMES SOURCE:22 , 2.50
			SOURCE:24 += 4000
			SIF TALENT:173
				SOURCE:24 += 4000
		ENDIF
	ENDIF
	TEQUIP:25 = 1
ENDIF

TCVAR:1 += 2

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF

;커맨드 속성：촉수
IF TEQUIP:90
	TFLAG:98 = 8
;커맨드 속성：도구(조교자)
ELSE
	TFLAG:98 = 1
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;-------------------------------------------------
;애널 바이브 삽입중
;-------------------------------------------------
