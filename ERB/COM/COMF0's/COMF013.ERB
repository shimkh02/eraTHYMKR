	TIMES LOCAL , 1.40
	LOCAL:2 += 120
ELSE
	TIMES LOCAL , 1.80
	LOCAL:2 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:2 , 2.00

;처녀로 정조관념
IF TALENT:0 == 1 && TALENT:30
	LOCAL:1 /= 3
ELSEIF TALENT:0 && TALENT:30
	LOCAL:1 /= 2
ENDIF

IF (FLAG:1100 & 1) && TEQUIP:90 == 0
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 9
	FLAG:1111 = 0
	FLAG:1112 = LOCAL
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL = FLAG:1120
ENDIF

SOURCE:2 += LOCAL
SOURCE:20 += LOCAL:2
SOURCE:22 += LOCAL:1

;자위 실행시는 경험＋1
IF SELECTCOM == 7 || SELECTCOM == 124 || SELECTCOM == 125 || SELECTCOM == 127 || SELECTCOM == 182 || SELECTCOM == 205 || SELECTCOM == 628
	TCVAR:1 += 2
ELSE
	TCVAR:1 += 1
ENDIF

;촉수 경험 플래그
SIF TEQUIP:90
	TFLAG:90 += 1

RETURN 1

