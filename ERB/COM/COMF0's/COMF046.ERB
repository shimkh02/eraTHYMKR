CALL TRAIN_MESSAGE_COM046

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 10
DOWNBASE:1 += 200

SOURCE:22 = 1400
SOURCE:31 = 1000

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*20 + 300

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 400
	SOURCE:16 = 50
	TIMES SOURCE:22 , 0.80
	TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
	SOURCE:15 = 480
	SOURCE:16 = 100
	TIMES SOURCE:22 , 0.90
	TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
	SOURCE:14 = 400
	SOURCE:15 = 550
	SOURCE:16 = 200
	TIMES SOURCE:22 , 1.00
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
	SOURCE:14 = 600
	SOURCE:15 = 620
	SOURCE:16 = 300
	TIMES SOURCE:22 , 1.10
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
	SOURCE:14 = 800
	SOURCE:15 = 700
	SOURCE:16 = 500
	TIMES SOURCE:22 , 1.20
	TIMES SOURCE:30 , 0.50
ELSE
	SOURCE:14 = 1000
	SOURCE:15 = 800
	SOURCE:16 = 750
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:30 , 0.10
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES SOURCE:22 , 0.80
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:21 == 1
	TIMES SOURCE:22 , 0.90
	TIMES SOURCE:30 , 1.20
ELSEIF ABL:21 == 2
	TIMES SOURCE:22 , 1.00
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:21 == 3
	TIMES SOURCE:22 , 1.20
	TIMES SOURCE:30 , 0.80
ELSEIF ABL:21 == 4
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:30 , 0.60
ELSE
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:30 , 0.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	SOURCE:11 = 20
ELSEIF PALAM:13 < PALAMLV:2
	SOURCE:11 = 50
ELSEIF PALAM:13 < PALAMLV:3
	SOURCE:11 = 100
ELSEIF PALAM:13 < PALAMLV:4
	SOURCE:11 = 200
ELSE
	SOURCE:11 = 400
ENDIF

;TALENT:헌신적을 본다
SIF TALENT:63
	TIMES SOURCE:11 , 2.00

;TALENT:도착적을 본다
IF TALENT:80
	TIMES SOURCE:11 , 1.50
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:30 , 0.70
	TIMES SOURCE:31 , 0.20
ENDIF

;TALENT:연모/망신/친애를 본다
IF (TALENT:150 || TALENT:151 || TALENT:152) && ASSIPLAY == 0
	TIMES SOURCE:11 , 2.50
	TIMES SOURCE:22 , 1.20
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
ELSE
	TIMES SOURCE:15 , 2.00
	TIMES SOURCE:16 , 2.00
ENDIF

;대상의 입의 더러운⇔조교자의 다리의 더러움이 서로 이동
STAIN:0 |= STAIN:PLAYER:6
STAIN:PLAYER:6 |= STAIN:0

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 7
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:50 += 1
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

RETURN 1

