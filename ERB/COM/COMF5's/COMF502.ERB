@COM_ABLE502
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:502 > 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;그밖의 Ａ계 촉수責め中は不可
SIF TEQUIP:25 && TEQUIP:25 != 1
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM502
JUMP COM13
RETURN 1
