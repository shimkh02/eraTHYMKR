;=============================================================================
;촉수 산란
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE533
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:533 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수調教中じゃないとはダメ
SIF TEQUIP:90 == 0
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;조교자는 촉수사역LV4이상 필요
SIF ABL:PLAYER:98 < 4
	RETURN 0
;남자라면 안 됨
SIF TALENT:120
	RETURN 0
;妊娠中なら不可
SIF TALENT:140
	RETURN 0
;懐卵中はダメ
SIF TALENT:145
	RETURN 0
;Ｖ확장경험이 少ないと卵は追加不可能
IF EXP:32 < 1
	LOCAL = 0
ELSEIF EXP:32 < 4
	LOCAL = 1
ELSEIF EXP:32 < 20
	LOCAL = 2
ELSEIF EXP:32 < 50
	LOCAL = 3
ELSEIF EXP:32 < 200
	LOCAL = 4
ELSEIF EXP:32 < 500
	LOCAL = 5
ELSE
	LOCAL = 6
ENDIF
SIF TEQUIP:93 >= LOCAL
	RETURN 0
;촉수가 邪魔で膣内産卵できない
SIF TEQUIP:20 == 3
	RETURN 0
;幽霊膣鏡삽입中は不可
SIF TEQUIP:21
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM533
PRINTL 촉수 산란
TSTR:0 = 촉수 산란
CALL TRAIN_MESSAGE_COM533

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

LOCAL:1 = 70
LOCAL:2 = 80

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 1.40
ELSEIF ABL:35 == 1
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:2 , 1.20
ELSEIF ABL:35 == 2
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:2 , 1.00
ELSEIF ABL:35 == 3
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 0.80
ELSEIF ABL:35 == 4
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
ELSE
	TIMES LOCAL:1 , 0.50
	TIMES LOCAL:2 , 0.40
ENDIF

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:2

SOURCE:12 = 1800
SOURCE:30 = 2400

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 80
ELSEIF ABL:1 == 1
	SOURCE:1 = 250
ELSEIF ABL:1 == 2
	SOURCE:1 = 600
ELSEIF ABL:1 == 3
	SOURCE:1 = 1000
ELSEIF ABL:1 == 4
	SOURCE:1 = 1300
ELSE
	SOURCE:1 = 1700
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	SOURCE:20 = 2000
	SOURCE:22 = 200
	SOURCE:31 = 1000
	SOURCE:32 = 2000
ELSEIF ABL:21 == 1
	SOURCE:20 = 1600
	SOURCE:22 = 500
	SOURCE:31 = 1000
	SOURCE:32 = 1000
ELSEIF ABL:21 == 2
	SOURCE:20 = 1200
	SOURCE:22 = 800
	SOURCE:31 = 1000
	SOURCE:32 = 500
ELSEIF ABL:21 == 3
	SOURCE:20 = 800
	SOURCE:22 = 1200
	SOURCE:31 = 1000
	SOURCE:32 = 100
ELSEIF ABL:21 == 4
	SOURCE:20 = 600
	SOURCE:22 = 1500
	SOURCE:31 = 1000
	SOURCE:32 = 0
ELSE
	SOURCE:20 = 400
	SOURCE:22 = 2000
	SOURCE:31 = 1000
	SOURCE:32 = 0
ENDIF

;촉수 중독
IF ABL:35 == 0
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:35 == 1
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:35 == 2
	TIMES SOURCE:30 , 0.75
ELSEIF ABL:35 == 3
	TIMES SOURCE:30 , 0.50
ELSEIF ABL:35 == 4
	TIMES SOURCE:30 , 0.25
ELSE
	TIMES SOURCE:30 , 0.10
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:1 , 0.40
	SOURCE:20 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	SOURCE:20 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:20 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	SOURCE:20 += 120
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 1.10
ELSE
	TIMES SOURCE:1 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.20
ELSE
	TIMES SOURCE:1 , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:103
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:102
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF
;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3

;알플러그의 착탈
SIF TEQUIP:20 == 0
	TEQUIP:20 = 1
TEQUIP:93 += 1

TCVAR:0 += 10

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 1
STAIN:4 |= 2
STAIN:4 |= 4
STAIN:4 |= 64
RETURN 1

;--------------------------------------------------
;촉수질내 산란중
;--------------------------------------------------
@EQUIP_COM533
PRINTL <촉수 질 내 산란 중>
CALL EQUIP_MESSAGE_COM533

LOCAL:3 = 110
LOCAL:4 = 90

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:3 , 1.00
	TIMES LOCAL:4 , 1.40
ELSEIF ABL:35 == 1
	TIMES LOCAL:3 , 0.90
	TIMES LOCAL:4 , 1.20
ELSEIF ABL:35 == 2
	TIMES LOCAL:3 , 0.80
	TIMES LOCAL:4 , 1.00
ELSEIF ABL:35 == 3
	TIMES LOCAL:3 , 0.70
	TIMES LOCAL:4 , 0.80
ELSEIF ABL:35 == 4
	TIMES LOCAL:3 , 0.60
	TIMES LOCAL:4 , 0.60
ELSE
	TIMES LOCAL:3 , 0.50
	TIMES LOCAL:4 , 0.40
ENDIF

DOWNBASE:0 += LOCAL:3
DOWNBASE:1 += LOCAL:4

;ABL:V감각을 본다
IF ABL:1 == 0
	LOCAL = 80
	LOCAL:1 = 300
ELSEIF ABL:1 == 1
	LOCAL = 250
	LOCAL:1 = 800
ELSEIF ABL:1 == 2
	LOCAL = 600
	LOCAL:1 = 1400
ELSEIF ABL:1 == 3
	LOCAL = 1000
	LOCAL:1 = 1800
ELSEIF ABL:1 == 4
	LOCAL = 1300
	LOCAL:1 = 2100
ELSE
	LOCAL = 1700
	LOCAL:1 = 2400
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES LOCAL , 0.50
	LOCAL:2 = 2000
ELSEIF EXP:0 < EXPLV:2
	TIMES LOCAL , 1.00
	LOCAL:2 = 300
ELSEIF EXP:0 < EXPLV:3
	TIMES LOCAL , 1.10
	LOCAL:2 = 50
ELSEIF EXP:0 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 10
ELSEIF EXP:0 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL , 0.40
	LOCAL:2 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL , 0.80
	LOCAL:2 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES LOCAL , 1.40
	LOCAL:2 += 120
ELSE
	TIMES LOCAL , 1.80
	LOCAL:2 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:2 , 2.00

;V민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:103
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:102
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

SOURCE:1 += LOCAL
SOURCE:22 += LOCAL:1
SOURCE:20 += LOCAL:2
SOURCE:31 += LOCAL:1 *2
SOURCE:23 += LOCAL:1

TCVAR:0 += 3
;촉수 경험 플래그
SIF TEQUIP:90
	TFLAG:90 += 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM533
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:93
	PRINTFORMW 촉수는 알을 품고 있는 %CALLNAME:TARGET%의 자궁에 추가로 알을 낳아 붙였다.
ELSEIF TEQUIP:20 == 2
	PRINTFORML %CALLNAME:TARGET%의 질을 괴롭히던 촉수는 한번 몸을 부르르 떨더니,
	PRINTW 자궁구를 비틀어 열고 그 안에 알을 토해냈다.
ELSEIF TEQUIP:20 == 1
	PRINTFORML %CALLNAME:TARGET%의 성기에 기어들어 비벼대고 있던 촉수는
	PRINTW 갑자기 안쪽으로 돌진해, 자궁구를 향해 알을 내뱉었다.
ELSE
	PRINTFORMW 엄청나게 굵은 촉수가 %CALLNAME:TARGET%의 비순에 기어들어 잠시 비벼대더니, 자궁의 안쪽에 알을 낳아 붙였다….
ENDIF

;--------------------------------------------------
;장착시 메시지
;--------------------------------------------------
@EQUIP_MESSAGE_COM533
CALL KOJO_EQUIP_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 자궁에 촉수의 알을 품은 채로, 촉수로 질구를 막힌 상태가 되어있다.
PRINTL
IF ABL:21 == 0
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 격통에 소리도 나오지 않는 것 같다….
ELSEIF ABL:21 == 1
	PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 비지땀을 흘리고 
	SIF TEQUIP:40 == 0
		PRINT 크게 부푼 복부를 손으로 쓰다듬고 
	PRINTW 있다.
ELSEIF ABL:21 == 2
	PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 괴로운 듯한 표정을 하면서도, 얼굴을 붉게 물들이곤 크게 부풀어 오른 자신의 배를 
	IF TEQUIP:40
		PRINTW 지긋이 보고 있다.
	ELSE
		PRINTW 문지르고 있다.
	ENDIF
ELSEIF ABL:21 == 3
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 배란감에 시달리면서도, 때때로 황홀한 표정을 짓고 있다.
ELSEIF ABL:21 == 4
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 넋을 잃은 표정을 되어 배란감과 복통을 즐기고 있다.
ELSE
	PRINTFORML 크게 부풀어 오른 %CALLNAME:TARGET%의 배 안에서는
	PRINTL 호흡에 맞추어 데굴데굴 알이 굴러다니는 것 같다.
	PRINTFORMW 알이 자궁의 벽을 긁어대고 있는지 %조사선택(CALLNAME:TARGET, "는")% 음란한 비명을 질러대고 있다….
ENDIF
