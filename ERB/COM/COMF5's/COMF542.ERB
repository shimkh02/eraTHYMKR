	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:20 += 1000
	TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 300
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:20 , 0.20
ELSE
	TIMES SOURCE:2 , 1.80
	TIMES SOURCE:20 , 0.10
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;정조관념
IF TALENT:30
	TIMES SOURCE:15 , 0.60
	SOURCE:32 = 1000
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:15 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:15 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:15 , 1.50
ELSE
	TIMES SOURCE:2 , 1.50
	TIMES SOURCE:15 , 1.80
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.50
	TIMES SOURCE:15 , 0.60
	TIMES SOURCE:32 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:32 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:32 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:32 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:15 , 1.40
	TIMES SOURCE:32 , 0.60
ELSE
	TIMES SOURCE:2 , 2.00
	TIMES SOURCE:15 , 1.60
	TIMES SOURCE:32 , 0.30
ENDIF

SOURCE:24 = LOCAL:2

TCVAR:1 += 1

;장내(어널)의 더러움이 리셋
STAIN:4 = 8

;커맨드 속성：촉수
TFLAG:98 = 8
