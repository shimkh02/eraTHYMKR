@COM_ABLE542
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:542 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;조교자는 촉수사역LV2이상 필요
SIF ABL:PLAYER:98 < 2
	RETURN 0
;촉수가 [特殊ホルモン分泌]を得ていないとダメ
SIF (FLAG:75 & 1024) == 0
	RETURN 0
;기계면 불가
SIF TALENT:206
	RETURN 0
;애널촉수中じゃないとダメ
SIF TEQUIP:25 == 0
	RETURN 0
;그밖의 Ａ계 촉수責め中は不可
SIF TEQUIP:25 > 2
	RETURN 0
;卵が邪魔で腸内洗浄できない
SIF TEQUIP:94
	RETURN 0
RETURN 1

