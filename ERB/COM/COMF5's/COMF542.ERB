	LOCAL:3 += 1
SIF STAIN:4 & 4
	LOCAL:3 += 1
SIF STAIN:4 & 8
	LOCAL:3 += 1
SIF STAIN:4 & 16
	LOCAL:3 += 1

SOURCE:16 = LOCAL:2
