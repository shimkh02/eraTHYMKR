IF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.60
	SOURCE:20 = 400
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 = 60
ELSEIF EXP:1 < EXPLV:4
