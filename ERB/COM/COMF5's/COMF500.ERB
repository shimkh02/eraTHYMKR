LOCAL:1 = 100
;EXP:촉수 경험을 본다
IF EXP:53 < EXPLV:1
	TIMES LOCAL:1 , 3.00
ELSEIF EXP:53 < EXPLV:2
	TIMES LOCAL:1 , 2.50
ELSEIF EXP:53 < EXPLV:3
	TIMES LOCAL:1 , 2.00
ELSEIF EXP:53 < EXPLV:4
	TIMES LOCAL:1 , 1.00
ELSEIF EXP:53 < EXPLV:5
	TIMES LOCAL:1 , 0.80
ELSE
	TIMES LOCAL:1 , 0.60
ENDIF

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:1 , 1.00
ELSEIF ABL:35 == 1
	TIMES LOCAL:1 , 0.90
ELSEIF ABL:35 == 2
	TIMES LOCAL:1 , 0.80
ELSEIF ABL:35 == 3
	TIMES LOCAL:1 , 0.70
ELSEIF ABL:35 == 4
	TIMES LOCAL:1 , 0.60
ELSE
	TIMES LOCAL:1 , 0.50
ENDIF

;감정결여
SIF TALENT:22
	TIMES LOCAL:1 , 0.60
;기생
SIF TALENT:146
	TIMES LOCAL:1 , 0.70
;회란
SIF TALENT:145
	TIMES LOCAL:1 , 0.80
;촉수 이식
SIF CFLAG:41
	TIMES LOCAL:1 , 0.75

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:1 * 2
SOURCE:25 += LOCAL:1 * 10

;겁쟁이
SIF TALENT:10
	TIMES LOCAL:1 , 2.00

SOURCE:19 += 2000
SOURCE:27 += LOCAL:1 * 2
SOURCE:30 += LOCAL:1 * 10
SOURCE:31 += LOCAL:1 * 5

TIMES SOURCE:0 , 2.00
TIMES SOURCE:1 , 2.00
TIMES SOURCE:2 , 2.00
TIMES SOURCE:3 , 2.00
TIMES SOURCE:22 , 1.80

CALL ADD_SOURCE_27

;-------------------------------------------------
;촉수 사정 체크
;-------------------------------------------------
IF TFLAG:998 != 0
	LOCAL = 0
	;ABL:기교를 본다
	IF ABL:12 == 0
		LOCAL = 500
	ELSEIF ABL:12 == 1
		LOCAL = 600
	ELSEIF ABL:12 == 2
		LOCAL = 800
	ELSEIF ABL:12 == 3
		LOCAL = 1000
	ELSEIF ABL:12 == 4
		LOCAL = 1400
	ELSE
		LOCAL = 2000
	ENDIF

	;ABL:순종을 본다
	IF ABL:10 == 0
		TIMES LOCAL , 0.80
	ELSEIF ABL:10 == 1
		TIMES LOCAL , 0.90
	ELSEIF ABL:10 == 2
		TIMES LOCAL , 1.00
	ELSEIF ABL:10 == 3
		TIMES LOCAL , 1.10
	ELSEIF ABL:10 == 4
		TIMES LOCAL , 1.20
	ELSE
		TIMES LOCAL , 1.30
	ENDIF

	;PALAM:욕정을 본다
	IF PALAM:13 < PALAMLV:1
		TIMES LOCAL , 1.00
	ELSEIF PALAM:13 < PALAMLV:2
		TIMES LOCAL , 1.10
	ELSEIF PALAM:13 < PALAMLV:3
		TIMES LOCAL , 1.20
	ELSEIF PALAM:13 < PALAMLV:4
		TIMES LOCAL , 1.30
	ELSEIF PALAM:13 < PALAMLV:5
		TIMES LOCAL , 1.40
	ELSE
		TIMES LOCAL , 1.50
	ENDIF

	;촉수 삽입외
	SIF TEQUIP:20
		TIMES LOCAL , 1.50
	;촉수 관통
	IF TEQUIP:25 == 3
		TIMES LOCAL , 1.20
	;촉수 관장
	ELSEIF TEQUIP:25 == 7
		TIMES LOCAL , 1.30
	;촉수 어널 삽입
	ELSEIF TEQUIP:25 == 1
		TIMES LOCAL , 1.50
	ENDIF
	;촉수 페니스 고문
	IF TEQUIP:30 == 2
		TIMES LOCAL , 1.10
	;촉수 방전 페니스 고문
	ELSEIF TEQUIP:30 == 6
		TIMES LOCAL , 1.50
	;촉수 클리 고문외
	ELSEIF TEQUIP:30
		TIMES LOCAL , 1.20
	ENDIF
	;촉수 착유
	IF TEQUIP:35 == 2
		TIMES LOCAL , 1.30
	;촉수 유두 고문
	ELSEIF TEQUIP:35 == 1
		TIMES LOCAL , 1.20
	ENDIF
	;촉수 카테터
	SIF TEQUIP:32
		TIMES LOCAL , 1.30
	;촉수 긴박
	SIF TEQUIP:40
		TIMES LOCAL , 1.20
	;촉수구욕
	SIF TEQUIP:42
		TIMES LOCAL , 1.50
	;촉수 최면
	SIF TEQUIP:92
		TIMES LOCAL , 1.10
	;촉수[정액 방출량 강화]
	SIF FLAG:15 & 4096 && FLAG:75 & 16384
		TIMES LOCAL , 1.80

	TFLAG:997 += LOCAL
	SOURCE:27 += LOCAL / 100

	EJAC = TFLAG:998

	IF  TFLAG:997 > EJAC * 4 && FLAG:15 & 4096 && FLAG:75 & 16384
		TFLAG:91 = 3
	ELSEIF  TFLAG:997 > EJAC * 2
		TFLAG:91 = 2
	ELSEIF TFLAG:997 > EJAC
		TFLAG:91 = 1
	ELSE
		TFLAG:91 = 0
	ENDIF

	;초대형량 사정
	IF TFLAG:91 == 3
		TCVAR:4 += 7
		SOURCE:27 += 1000
		PRINTL 촉수 초 대량 사정
		TFLAG:90 += 1

		TFLAG:997 -= EJAC*4
	;대량 사정
	ELSEIF TFLAG:91 == 2
		TCVAR:4 += 3
		SOURCE:27 += 500
		PRINTL 촉수 대량 사정

		TFLAG:997 -= EJAC*2
	;통상의 사정
	ELSEIF TFLAG:91 == 1
		TCVAR:4 += 1
		SOURCE:27 += 100
		PRINTL 촉수 사정

		TFLAG:997 -= EJAC
	ENDIF
	;촉수가 사정한 플래그
	TFLAG:37 = TFLAG:91
	;입에 정액을 마신 플래그
	SIF TEQUIP:42
		TFLAG:3 += TFLAG:91
	IF TFLAG:91
		TFLAG:90 += 1
		SIF TFLAG:997 >= EJAC
			TFLAG:997 = EJAC-1
	ENDIF
ENDIF

SIF EXP:53 == 0
	TCVAR:50 += 1

TFLAG:90 += 1
TCVAR:53 += TFLAG:90

RETURN 1

