;=============================================================================
;촉수 방광 세정
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE543
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:543 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:98 < 3
	RETURN 0
;촉수가 [特殊ホルモン分泌]を得ていないとダメ
SIF (FLAG:75 & 1024) == 0
	RETURN 0
;기계면 불가
SIF TALENT:206
	RETURN 0
;촉수카테터中じゃないとダメ
SIF TEQUIP:32 != 1
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM543
PRINTL 촉수 방광 세정
TSTR:0 = 촉수 방광 세정
CALL TRAIN_MESSAGE_COM543

LOCAL:1 = 70
;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:1 , 1.00
	LOCAL:2 = 100
ELSEIF ABL:35 == 1
	TIMES LOCAL:1 , 0.90
	LOCAL:2 = 200
ELSEIF ABL:35 == 2
	TIMES LOCAL:1 , 0.80
	LOCAL:2 = 400
ELSEIF ABL:35 == 3
	TIMES LOCAL:1 , 0.70
	LOCAL:2 = 800
ELSEIF ABL:35 == 4
	TIMES LOCAL:1 , 0.60
	LOCAL:2 = 1200
ELSE
	TIMES LOCAL:1 , 0.50
	LOCAL:2 = 1800
ENDIF

DOWNBASE:0 += LOCAL:1 *2
DOWNBASE:1 += LOCAL:1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
LOCAL:3 = 1

SOURCE:16 = LOCAL:2
SOURCE:30 = 2000
SOURCE:19 = 700
SOURCE:31 = 7000
SOURCE:23 = 4000

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 60
	SOURCE:24 = 150
	SOURCE:22 = 150
ELSEIF ABL:0 == 1
	SOURCE:0 = 180
	SOURCE:24 = 250
	SOURCE:22 = 300
ELSEIF ABL:0 == 2
	SOURCE:0 = 420
	SOURCE:24 = 350
	SOURCE:22 = 600
ELSEIF ABL:0 == 3
	SOURCE:0 = 900
	SOURCE:24 = 500
	SOURCE:22 = 1000
ELSEIF ABL:0 == 4
	SOURCE:0 = 1600
	SOURCE:24 = 700
	SOURCE:22 = 1800
ELSE
	SOURCE:0 = 2000
	SOURCE:24 = 1000
	SOURCE:22 = 2300
ENDIF

;EXP:요도 경험을 본다
IF EXP:46 < EXPLV:2
	TIMES SOURCE:0 , 0.60
	SOURCE:20 = 400
ELSEIF EXP:46 < EXPLV:3
	TIMES SOURCE:0 , 1.00
	SOURCE:20 = 60
ELSEIF EXP:46 < EXPLV:4
	TIMES SOURCE:0 , 1.20
	SOURCE:20 = 12
ELSEIF EXP:46 < EXPLV:5
	TIMES SOURCE:0 , 1.30
	SOURCE:20 = 0
ELSE
	TIMES SOURCE:0 , 1.80
	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:0 , 0.10
	SOURCE:20 += 1000
	TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:0 , 0.40
	SOURCE:20 += 300
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:0 , 1.40
	TIMES SOURCE:20 , 0.20
ELSE
	TIMES SOURCE:0 , 1.80
	TIMES SOURCE:20 , 0.10
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;정조관념
IF TALENT:30
	TIMES SOURCE:24 , 0.60
	SOURCE:32 = 1000
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:24 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:24 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:24 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:24 , 1.50
ELSE
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:24 , 1.80
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:24 , 0.60
	TIMES SOURCE:32 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:24 , 0.80
	TIMES SOURCE:32 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:24 , 1.00
	TIMES SOURCE:32 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:24 , 1.20
	TIMES SOURCE:32 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:0 , 1.60
	TIMES SOURCE:24 , 1.40
	TIMES SOURCE:32 , 0.60
ELSE
	TIMES SOURCE:0 , 2.00
	TIMES SOURCE:24 , 1.60
	TIMES SOURCE:32 , 0.30
ENDIF

;방광 내압력 상승(방뇨 레벨 증가/뇨의가 재촉 되는 것으로 이뇨제 효과)
IF TALENT:45 == 0
	LOCAL:4 = MIN(1 + (EXP:6 / 10), 10)
	IF TEQUIP:12
		CFLAG:31 += LOCAL:4 *2
	ELSE
		CFLAG:31 += LOCAL:4
	ENDIF
ENDIF
IF TEQUIP:12 == 0
	PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 급격하게 요의가 생겼다.
	TEQUIP:12 = 1
ENDIF

TCVAR:46 += 1

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 1
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM543
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
PRINTFORML 촉수가 %CALLNAME:TARGET%의 요도를 통해 방광에 세정액을 쏟아냈다.
PRINTL 그리고 방광이 한계까지 부풀어 오른 순간, 반대로 안의 용액을 빨아내었다.
PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 세정액으로 몇 번이나 방광 안을 유린당하다 보니,
PRINTL 마치 방뇨하고 있는 것 같은 착각이 들었다.
PRINTL
PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
IF TALENT:175 && ABL:21 >= 5
	PRINTL 하복부의 둔통과 요의를 기뻐하고 있다….
	PRINTW 방광 내에 세정액이 들락날락 할 때마다, 새된 소리를 지르며 허리를 흔들고 있다….
ELSEIF ABL:21 >= 5
	PRINTL 대량의 세정액을 방광 내에 흘려 넣는 고문에도 지지 않고,
	PRINTW 쾌락으로 가득 찬 웃는 얼굴을 짓고 있다….
ELSEIF ABL:21 >= 3
	PRINTL 강제적으로 요의를 느끼게 되었지만,
	PRINTW 그것조차 쾌락인지 안타까운듯한 소리를 내며 몸부림 치고 있다….
ELSEIF ABL:21 >= 1
	PRINTL 간신히 쾌락을 느끼게 되어,
	PRINTW 음란한 신음소리를 내고 있다….
ELSE
	PRINTL 격렬한 요의와 하복부의 격통에 얼굴을 찡그리고,
	PRINTW 이마에는 굵은 비지땀을 흘리고 있다….
ENDIF
