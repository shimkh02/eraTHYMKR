;=============================================================================
;촉수 유선 삽입
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE526
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:526 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;一度も촉수乳腺삽입をしたことが無い場合, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && (CFLAG:24 & 2) == 0
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수調教中じゃないとはダメ
SIF TEQUIP:90 == 0
	RETURN 0
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:98 < 3
	RETURN 0
;촉수가 [極細촉수]を得ていないとダメ
SIF (FLAG:75 & 16) == 0
	RETURN 0
;남자라면 안 됨
SIF TALENT:120
	RETURN 0
;그밖의 Ｂ계 촉수責め中は不可
SIF TEQUIP:35 && TEQUIP:35 != 3
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM526
PRINTL 촉수 유선 삽입
TSTR:0 = 촉수 유선 삽입
CALL TRAIN_MESSAGE_COM526

IF TEQUIP:35 == 3
	DOWNBASE:0 += (TALENT:134) ?  225 # 450
	DOWNBASE:1 += (TALENT:134) ?  1250 # 2500
ELSE
	DOWNBASE:0 += (TALENT:134) ?  250 # 500
	DOWNBASE:1 += (TALENT:134) ?  1500 # 3000
ENDIF

;ABL:B감각을 본다
IF ABL:3 == 0
	SOURCE:3 = 50
ELSEIF ABL:3 == 1
	SOURCE:3 = 100
ELSEIF ABL:3 == 2
	SOURCE:3 = 200
ELSEIF ABL:3 == 3
	SOURCE:3 = 800
ELSEIF ABL:3 == 4
	SOURCE:3 = 1600
ELSE
	SOURCE:3 = 2800
ENDIF

;EXP:촉수 경험을 본다
IF EXP:53 < EXPLV:1
	SOURCE:20 = 10000
	SOURCE:30 = 10000
	SOURCE:31 = 10000
ELSEIF EXP:53 < EXPLV:2
	SOURCE:20 = 5000
	SOURCE:30 = 6500
	SOURCE:31 = 8000
ELSEIF EXP:53 < EXPLV:3
	SOURCE:20 = 3000
	SOURCE:30 = 4500
	SOURCE:31 = 6000
ELSEIF EXP:53 < EXPLV:4
	SOURCE:20 = 2000
	SOURCE:30 = 3000
	SOURCE:31 = 4000
	TIMES DOWNBASE:0 , 0.90
ELSEIF EXP:53 < EXPLV:5
	SOURCE:20 = 1000
	SOURCE:30 = 1500
	SOURCE:31 = 2000
	TIMES DOWNBASE:0 , 0.70
ELSE
	SOURCE:20 = 500
	SOURCE:30 = 750
	SOURCE:31 = 1000
	TIMES DOWNBASE:0 , 0.50
ENDIF

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	SOURCE:18 = 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.50
	TIMES SOURCE:30 , 2.00
	TIMES SOURCE:31 , 4.00
ELSEIF ABL:35 == 1
	SOURCE:18 = 60
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.80
	TIMES SOURCE:30 , 1.50
	TIMES SOURCE:31 , 1.80
	TIMES DOWNBASE:1 , 0.90
ELSEIF ABL:35 == 2
	SOURCE:18 = 180
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
	TIMES SOURCE:30 , 1.00
	TIMES SOURCE:31 , 1.00
	TIMES DOWNBASE:1 , 0.80
ELSEIF ABL:35 == 3
	SOURCE:18 = 360
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.20
	TIMES SOURCE:30 , 0.50
	TIMES SOURCE:31 , 0.70
	TIMES DOWNBASE:1 , 0.70
ELSEIF ABL:35 == 4
	SOURCE:18 = 640
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
	TIMES SOURCE:30 , 0.10
	TIMES SOURCE:31 , 0.20
	TIMES DOWNBASE:1 , 0.60
ELSE
	SOURCE:18 = 900
	TIMES SOURCE:15 , 2.00
	TIMES SOURCE:16 , 2.00
	TIMES SOURCE:30 , 0.00
	TIMES SOURCE:31 , 0.10
	TIMES DOWNBASE:1 , 0.50
ENDIF

;B민감·둔감
IF TALENT:106
	TIMES SOURCE:3 , 1.20
ELSEIF TALENT:107
	TIMES SOURCE:3 , 0.70
ENDIF
;절벽·빈유·큰 가슴·폭유
IF TALENT:110
	TIMES SOURCE:20 , 1.75
ELSEIF TALENT:111
	TIMES SOURCE:20 , 1.50
ELSEIF TALENT:112
	TIMES SOURCE:3 , 1.50
ELSEIF TALENT:113
	TIMES SOURCE:3 , 1.20
ENDIF
;음유
SIF TALENT:174
	TIMES SOURCE:3 , 2.50

IF TEQUIP:35 == 3
	SIF TALENT:134
		STAIN:5 |= 16
	TEQUIP:35 = 0
	;촉수 경험 플래그
	TFLAG:90 = 0
ELSE
	IF TALENT:134 == 0
		IF FLAG:75 & 1024 && PALAM:13 > PALAMLV:5
			SOURCE:25 += 3000
			TALENT:134 = 1
			CALL CALC_GAUGE3_SLAVE, TARGET, 1
		ENDIF
	ELSE
		TIMES SOURCE:20 , 0.60
	ENDIF
	;첫체험 때 이상 경험+1
	IF (CFLAG:24 & 2) == 0
		TCVAR:50 += 1
		CFLAG:24 |= 2
	ENDIF
	TCVAR:45 += 1
	TEQUIP:35 = 3
	;촉수 경험 플래그
	TFLAG:90 = 1
ENDIF

;커맨드 속성：촉수
TFLAG:98 = 8
RETURN 1

;--------------------------------------------------
;촉수 유선 삽입중
;--------------------------------------------------
@EQUIP_COM526
PRINTL <촉수 유선 삽입 중>

DOWNBASE:0 += 75
DOWNBASE:1 += 75

;ABL:B감각을 본다
IF ABL:3 == 0
	LOCAL = 20
ELSEIF ABL:3 == 1
	LOCAL = 60
ELSEIF ABL:3 == 2
	LOCAL = 100
ELSEIF ABL:3 == 3
	LOCAL = 250
ELSEIF ABL:3 == 4
	LOCAL = 400
ELSE
	LOCAL = 600
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL , 0.80
	SOURCE:24 += 10
	SOURCE:31 += 2500
ELSEIF ABL:35 == 1
	TIMES LOCAL , 0.90
	SOURCE:24 += 40
	SOURCE:31 += 1500
ELSEIF ABL:35 == 2
	TIMES LOCAL , 1.00
	SOURCE:24 += 100
	SOURCE:22 += 10
	SOURCE:31 += 500
ELSEIF ABL:35 == 3
	TIMES LOCAL , 1.10
	SOURCE:24 += 150
	SOURCE:22 += 40
	SOURCE:31 += 150
ELSEIF ABL:35 == 4
	TIMES LOCAL , 1.20
	SOURCE:24 += 300
	SOURCE:22 += 70
	SOURCE:31 += 10
ELSE
	TIMES LOCAL , 1.30
	SOURCE:24 += 450
	SOURCE:22 += 100
ENDIF

;EXP:촉수 경험을 본다
IF EXP:53 <= EXPLV:0
	TIMES LOCAL , 0.80
	SOURCE:20 += 500
ELSEIF EXP:53 <= EXPLV:1
	TIMES LOCAL , 1.00
	SOURCE:20 += 100
ELSEIF EXP:53 <= EXPLV:2
	TIMES LOCAL , 1.30
	SOURCE:20 += 50
ELSEIF EXP:53 <= EXPLV:3
	TIMES LOCAL , 1.60
	SOURCE:20 += 10
ELSEIF EXP:53 <= EXPLV:4
	TIMES LOCAL , 2.00
ELSE
	TIMES LOCAL , 2.25
ENDIF

;EXP:분유경험을 본다
IF EXP:5 <= EXPLV:0
	TIMES LOCAL , 0.80
ELSEIF EXP:5 <= EXPLV:1
	TIMES LOCAL , 1.00
ELSEIF EXP:5 <= EXPLV:2
	TIMES LOCAL , 1.30
ELSEIF EXP:5 <= EXPLV:3
	TIMES LOCAL , 1.60
ELSEIF EXP:5 <= EXPLV:4
	TIMES LOCAL , 2.00
ELSE
	TIMES LOCAL , 2.25
ENDIF

;B둔감과 B민감
IF TALENT:106
	TIMES LOCAL , 1.40
ELSEIF TALENT:107
	TIMES LOCAL , 0.60
ENDIF

;큰 가슴과 폭유, 빈유와 절벽
IF TALENT:113
	TIMES LOCAL , 1.20
ELSEIF TALENT:112
	TIMES LOCAL , 1.50
ELSEIF TALENT:111
	TIMES SOURCE:20 , 1.50
ELSEIF TALENT:110
	TIMES SOURCE:20 , 1.75
ENDIF
SOURCE:3 += LOCAL

;유선 개발 경험
TCVAR:45 += 1

STAIN:5 |= 64
;촉수 경험 플래그
TFLAG:90 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM526
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:35 == 3
	PRINTFORML 가느다란 촉수가 %CALLNAME:TARGET%의 유선에서 뽑혀졌다.
	IF TALENT:134
		PRINTFORMW 해방된 %CALLNAME:TARGET%의 유선에서 푸슛하고 모유가 흘러나왔다.
	ELSE
		PRINTFORMW 해방된 %CALLNAME:TARGET%의 유두가 꿈틀하고 움직였다.
	ENDIF
ELSE
	PRINTFORM 가느다란 촉수가 %CALLNAME:TARGET%의 
	IF TALENT:110
		PRINT 파리가 미끄러질 정도로 가파른 가슴에 
	ELSEIF TALENT:111
		PRINT 작은 가슴에 
	ELSEIF TALENT:112
		PRINT 풍부한 가슴에 
	ELSEIF TALENT:113
		PRINT 너무나도 풍만한 가슴에 
	ELSE
		PRINT 아름다운 형태의 가슴에 
	ENDIF
	PRINTL 가까워지자,
	PRINTL 유두를 통해 유선 안으로 침입을 시도했다….
	PRINTL
	IF TALENT:134 == 0
		IF FLAG:75 & 1024 && PALAM:13 > PALAMLV:5
			PRINTFORMW 촉수로부터 분비되고 있는 호르몬의 작용인가 %CALLNAME:TARGET%의 유선에서 모유가 흘러나오기 시작했다.
		ELSE
			PRINTFORMW 유방내의 이물감에 %CALLNAME:TARGET%의 이마에 비지땀이 맺히고 있다.
		ENDIF
	ELSE
		PRINTFORMW 모유때문에 침입이 용이하게 되었는지 %CALLNAME:TARGET%의 유선은 촉수를 받아들이고 있는 것 같다….
	ENDIF
ENDIF
