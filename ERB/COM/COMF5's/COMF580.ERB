	SOURCE:12 += 100
	SOURCE:25 += 500
	SOURCE:31 += 500
	TIMES LOCAL:1, 4.60
ELSE
	SOURCE:12 += 50
	SOURCE:25 += 900
	SOURCE:31 += 100
	TIMES LOCAL:1, 5.80
