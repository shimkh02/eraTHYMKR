;=============================================================================
;촉수 애널 산란
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE534
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:534 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수調教中じゃないとはダメ
SIF TEQUIP:90 == 0
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;조교자는 촉수사역LV4이상 필요
SIF ABL:PLAYER:98 < 4
	RETURN 0
;기계면 불가
SIF TALENT:206
	RETURN 0
;애널삽입以外のＡ계 촉수責め中は不可
SIF TEQUIP:25 && TEQUIP:25 != 1
	RETURN 0
;Ａ확장경험이 少ないと卵は追加不可能
IF EXP:33 < 1
	LOCAL = 0
ELSEIF EXP:33 < 4
	LOCAL = 1
ELSEIF EXP:33 < 20
	LOCAL = 2
ELSEIF EXP:33 < 50
	LOCAL = 3
ELSEIF EXP:33 < 200
	LOCAL = 4
ELSEIF EXP:33 < 500
	LOCAL = 5
ELSE
	LOCAL = 6
ENDIF
SIF TEQUIP:94 >= LOCAL
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM534
PRINTL 촉수 애널 산란
TSTR:0 = 촉수 어널 산란
CALL TRAIN_MESSAGE_COM534

LOCAL:1 = 65
LOCAL:2 = 70

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 1.40
ELSEIF ABL:35 == 1
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:2 , 1.20
ELSEIF ABL:35 == 2
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:2 , 1.00
ELSEIF ABL:35 == 3
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 0.80
ELSEIF ABL:35 == 4
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
ELSE
	TIMES LOCAL:1 , 0.50
	TIMES LOCAL:2 , 0.40
ENDIF

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:2

SOURCE:30 = 4000

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 80
	SOURCE:22 = 300
ELSEIF ABL:2 == 1
	SOURCE:2 = 250
	SOURCE:22 = 800
ELSEIF ABL:2 == 2
	SOURCE:2 = 600
	SOURCE:22 = 1400
ELSEIF ABL:2 == 3
	SOURCE:2 = 1000
	SOURCE:22 = 1800
ELSEIF ABL:2 == 4
	SOURCE:2 = 1300
	SOURCE:22 = 2100
ELSE
	SOURCE:2 = 1700
	SOURCE:22 = 2400
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	SOURCE:20 = 2000
	SOURCE:22 += 200
	SOURCE:31 = 1000
	SOURCE:32 = 2000
ELSEIF ABL:21 == 1
	SOURCE:20 = 1600
	SOURCE:22 += 500
	SOURCE:31 = 1000
	SOURCE:32 = 1000
ELSEIF ABL:21 == 2
	SOURCE:20 = 1200
	SOURCE:22 += 800
	SOURCE:31 = 1000
	SOURCE:32 = 500
ELSEIF ABL:21 == 3
	SOURCE:20 = 800
	SOURCE:22 += 1200
	SOURCE:31 = 1000
	SOURCE:32 = 100
ELSEIF ABL:21 == 4
	SOURCE:20 = 600
	SOURCE:22 += 1500
	SOURCE:31 = 1000
	SOURCE:32 = 0
ELSE
	SOURCE:20 = 400
	SOURCE:22 += 2000
	SOURCE:31 = 1000
	SOURCE:32 = 0
ENDIF

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:35 == 1
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:35 == 2
	TIMES SOURCE:30 , 0.75
ELSEIF ABL:35 == 3
	TIMES SOURCE:30 , 0.50
ELSEIF ABL:35 == 4
	TIMES SOURCE:30 , 0.25
ELSE
	TIMES SOURCE:30 , 0.10
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 120
ELSE
	TIMES SOURCE:2 , 1.80
	SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSE
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

SOURCE:23 = SOURCE:22
;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3

;알플러그의 착탈
SIF TEQUIP:25 == 0
	TEQUIP:25 = 1
TEQUIP:94 += 1
STAIN:4 |= 2
STAIN:4 |= 4
STAIN:4 |= 64

TCVAR:1 += 15

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 1
RETURN 1

;--------------------------------------------------
;촉수장내 산란중
;--------------------------------------------------
@EQUIP_COM534
PRINTL <촉수장내 산란 중>
CALL EQUIP_MESSAGE_COM534

LOCAL:3 = 120
LOCAL:4 = 100

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:3 , 1.00
	TIMES LOCAL:4 , 1.40
ELSEIF ABL:35 == 1
	TIMES LOCAL:3 , 0.90
	TIMES LOCAL:4 , 1.20
ELSEIF ABL:35 == 2
	TIMES LOCAL:3 , 0.80
	TIMES LOCAL:4 , 1.00
ELSEIF ABL:35 == 3
	TIMES LOCAL:3 , 0.70
	TIMES LOCAL:4 , 0.80
ELSEIF ABL:35 == 4
	TIMES LOCAL:3 , 0.60
	TIMES LOCAL:4 , 0.60
ELSE
	TIMES LOCAL:3 , 0.50
	TIMES LOCAL:4 , 0.40
ENDIF

DOWNBASE:0 += LOCAL:3
DOWNBASE:1 += LOCAL:4

;ABL:A감각을 본다
IF ABL:2 == 0
	LOCAL = 80
	LOCAL:1 = 300
ELSEIF ABL:2 == 1
	LOCAL = 250
	LOCAL:1 = 800
ELSEIF ABL:2 == 2
	LOCAL = 600
	LOCAL:1 = 1400
ELSEIF ABL:2 == 3
	LOCAL = 1000
	LOCAL:1 = 1800
ELSEIF ABL:2 == 4
	LOCAL = 1300
	LOCAL:1 = 2100
ELSE
	LOCAL = 1700
	LOCAL:1 = 2400
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:1
	TIMES LOCAL , 0.50
	LOCAL:2 = 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES LOCAL , 1.00
	LOCAL:2 = 300
ELSEIF EXP:1 < EXPLV:3
	TIMES LOCAL , 1.10
	LOCAL:2 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL , 0.40
	LOCAL:2 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL , 0.80
	LOCAL:2 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES LOCAL , 1.40
	LOCAL:2 += 120
ELSE
	TIMES LOCAL , 1.80
	LOCAL:2 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:2 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	LOCAL:1 /= 3

SOURCE:2 += LOCAL
SOURCE:22 += LOCAL:1
SOURCE:20 += LOCAL:2
SOURCE:31 += LOCAL:1 *2
SOURCE:23 += LOCAL:1

TCVAR:1 += 3
;촉수 경험 플래그
SIF TEQUIP:90
	TFLAG:90 += 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM534
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:94
	PRINTFORMW 촉수는 %CALLNAME:TARGET%의 장내에 새로운 알을 낳아 붙였다.
ELSEIF TEQUIP:25
	PRINTFORML %CALLNAME:TARGET%의 직장을 괴롭히고 있던 촉수는 살짝 몸부림 하더니,
	PRINTW 장내의 안쪽을 향해 점액과 함께 알을 토해냈다.
ELSE
	PRINTFORMW 엄청나게 굵은 촉수가 %CALLNAME:TARGET%의 항문에 비집고 들어가, 장내에 알을 낳았다.
ENDIF

;--------------------------------------------------
;장착시 메시지
;--------------------------------------------------
@EQUIP_MESSAGE_COM534
CALL KOJO_EQUIP_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 항문에서 촉수의 알이 밀어 넣어져 항문이 막힌 채로, 계속 능욕당하고 있다.
PRINTL
IF ABL:21 == 0
	PRINTFORMW %CALLNAME:TARGET%의 얼굴은 장내의 이물감에 새파랗게 질렸다….
ELSEIF ABL:21 == 1
	PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 비지땀을 흘리면서도 이물감을 느끼지 않으려고, 억지로 
	IF TEQUIP:40
		PRINTW 허리를 좌우로 흔들고 있다.
	ELSE
		PRINTW 엉덩이를 쓰다듬고 있다.
	ENDIF
ELSEIF ABL:21 == 2
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 괴로운 듯한 표정을 하면서도, 얼굴을 홍조 시키고 엉덩이를 움찔움찔 떨고 있다….
ELSEIF ABL:21 == 3
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 배설감에 시달리면서도, 황홀한 표정을 짓고 있다.
ELSEIF ABL:21 == 4
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 넋을 잃은 표정이 되어, 알이 장내의 벽을 긁어대는 자극을 즐기고 있다.
ELSE
	PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 배설감을 마음껏 맛보고 있다.
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 배덕감이 가득한 웃음을 지으며, 무엇인가를 기대하는 눈으로 %조사선택(CALLNAME:MASTER, "를")% 올려보고 있다.
ENDIF
