;실행 판정
;………………………………………………
@COM_ABLE503
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:503 > 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;대상에게 페니스가 있다면 불가
SIF TALENT:1 || TALENT:120 || TALENT:121
	RETURN 0
;그밖의 Ｃ계 촉수 고문 중에는 불가
SIF TEQUIP:30 && TEQUIP:30 != 1
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM503
JUMP COM14
RETURN 1
