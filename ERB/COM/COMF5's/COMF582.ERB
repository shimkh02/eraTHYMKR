;=============================================================================
;촉수 유령질경
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE582
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:582 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16384) == 0
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:21 == 2
	RETURN 1
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:98 < 3
	RETURN 0
;촉수가 [YUYUKO촉수]を得ていないとダメ
SIF (FLAG:98 & 4) == 0
	RETURN 0
;幽々子を촉수生贄に捧げていない(=幽々子が何らかの形で存在している)とダメ
SIF (FLAG:97 & 4) == 0
	RETURN 0
;촉수삽입中はダメ
SIF TEQUIP:20
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;촉수생지를 통해서 하반신이 촉수와 일체화님을 소환,
;꼭 껴안으면서 유령 체질의 투명한 촉수를 삽입해 의사적인 촉수 쿠스코를 재현……이라고 하는 초의미불인 플레이
@COM582
PRINTL 촉수 유령질경
TSTR:0 = 촉수 유령질경
CALL TRAIN_MESSAGE_COM582

DOWNBASE:0 += 75
DOWNBASE:1 += 150

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 80
	SOURCE:19 = 0
ELSEIF ABL:1 == 1
	SOURCE:1 = 150
	SOURCE:19 = 150
ELSEIF ABL:1 == 2
	SOURCE:1 = 360
	SOURCE:19 = 500
ELSEIF ABL:1 == 3
	SOURCE:1 = 720
	SOURCE:19 = 800
ELSEIF ABL:1 == 4
	SOURCE:1 = 1000
	SOURCE:19 = 1200
ELSE
	SOURCE:1 = 1800
	SOURCE:19 = 2000
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	SOURCE:12 = 300
	SOURCE:22 = 600
	SOURCE:25 = 1800
ELSEIF ABL:14 == 1
	SOURCE:12 = 500
	SOURCE:22 = 500
	SOURCE:25 = 1000
ELSEIF ABL:14 == 2
	SOURCE:12 = 500
	SOURCE:22 = 400
	SOURCE:25 = 600
ELSEIF ABL:14 == 3
	SOURCE:12 = 500
	SOURCE:22 = 300
	SOURCE:25 = 300
ELSEIF ABL:14 == 4
	SOURCE:12 = 1000
	SOURCE:22 = 200
	SOURCE:25 = 200
ELSE
	SOURCE:12 = 1300
	SOURCE:22 = 100
	SOURCE:25 = 150
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES SOURCE:12 , 0.80
	SOURCE:31 = 1500
ELSEIF ABL:21 == 1
	TIMES SOURCE:12 , 1.10
	SOURCE:31 = 800
ELSEIF ABL:21 == 2
	TIMES SOURCE:12 , 1.30
	SOURCE:31 = 200
ELSEIF ABL:21 == 3
	TIMES SOURCE:12 , 1.50
ELSEIF ABL:21 == 4
	TIMES SOURCE:12 , 1.70
ELSE
	TIMES SOURCE:12 , 2.10
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:20 = 6500
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:20 = 2000
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:20 = 500
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:20 = 100
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:20 = 10
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:20 += 1000
	TIMES SOURCE:20 , 4.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:20 += 400
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:20 , 0.20
ELSE
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:25 , 1.20
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 0.90
	TIMES SOURCE:25 , 1.10
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:25 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 1.10
	TIMES SOURCE:25 , 0.90
ELSE
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:25 , 0.80
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:25 , 1.30
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.90
	TIMES SOURCE:25 , 1.20
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:25 , 1.10
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.10
	TIMES SOURCE:25 , 1.00
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:25 , 0.90
ELSE
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:25 , 0.80
ENDIF

;정조관념
SIF TALENT:30
	TIMES SOURCE:31 , 4.00
;도착적
IF TALENT:80
	TIMES SOURCE:12 , 1.50
	TIMES SOURCE:31 , 1.20
ENDIF
;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.50

;유들자와의 궁합 있고 없음으로 보정이 상하
LOCAL:1 = 20
IF RELATION:(LOCAL:1) > 0 && RELATION:(LOCAL:1) != 100
	SOURCE:12 *= RELATION:(LOCAL:1)
	SOURCE:12 /= 100
	SOURCE:22 *= RELATION:(LOCAL:1)
	SOURCE:22 /= 100
	;트라우마는 강제적으로 증가
	IF RELATION:(LOCAL:1) < 100
		SOURCE:25 *= (200-RELATION:(LOCAL:1))
	ELSE
		SOURCE:25 *= RELATION:(LOCAL:1)
	ENDIF
	SOURCE:25 /= 100
	SOURCE:31 *= RELATION:(LOCAL:1)
	SOURCE:31 /= 100
ENDIF

TCVAR:0 += 5

;촉수 유령질경의 착탈
IF TEQUIP:21
	TEQUIP:21 = 0
ELSE
	TEQUIP:21 = 2
ENDIF

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;-------------------------------------------------
;촉수질경삽입중
;-------------------------------------------------
@EQUIP_COM582
PRINTL <촉수질경삽입중>

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 10
DOWNBASE:1 += 40

;ABL:V감각을 본다
IF ABL:1 == 0
	LOCAL:1 = 50
	LOCAL:4 = 80
ELSEIF ABL:1 == 1
	LOCAL:1 = 100
	LOCAL:4 = 160
ELSEIF ABL:1 == 2
	LOCAL:1 = 150
	LOCAL:4 = 240
ELSEIF ABL:1 == 3
	LOCAL:1 = 200
	LOCAL:4 = 360
ELSEIF ABL:1 == 4
	LOCAL:1 = 300
	LOCAL:4 = 600
ELSE
	LOCAL:1 = 500
	LOCAL:4 = 960
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	SOURCE:22 += 300
	LOCAL:2 += 150
	LOCAL:4 += 1000
ELSEIF ABL:14 == 1
	SOURCE:22 += 250
	LOCAL:2 += 250
	LOCAL:4 += 800
ELSEIF ABL:14 == 2
	SOURCE:22 += 200
	LOCAL:2 += 300
	LOCAL:4 += 400
ELSEIF ABL:14 == 3
	SOURCE:22 += 150
	LOCAL:2 += 500
	LOCAL:4 += 200
ELSEIF ABL:14 == 4
	SOURCE:22 += 100
	LOCAL:2 += 700
	LOCAL:4 += 100
ELSE
	SOURCE:22 += 50
	LOCAL:2 += 1000
	LOCAL:4 += 50
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES LOCAL:2 , 0.80
	SOURCE:31 += 1500
ELSEIF ABL:21 == 1
	TIMES LOCAL:2 , 1.10
	SOURCE:31 += 800
ELSEIF ABL:21 == 2
	TIMES LOCAL:2 , 1.30
	SOURCE:31 += 200
ELSEIF ABL:21 == 3
	TIMES LOCAL:2 , 1.50
ELSEIF ABL:21 == 4
	TIMES LOCAL:2 , 1.70
ELSE
	TIMES LOCAL:2 , 2.10
ENDIF

;EXP:V경험을 본다
;처녀는 있을 수 없다
IF EXP:0 < EXPLV:2
	TIMES LOCAL:1 , 0.60
	LOCAL:3 = 180
ELSEIF EXP:0 < EXPLV:3
	TIMES LOCAL:1 , 1.00
	LOCAL:3 = 75
ELSEIF EXP:0 < EXPLV:4
	TIMES LOCAL:1 , 1.20
	LOCAL:3 = 20
ELSEIF EXP:0 < EXPLV:5
	TIMES LOCAL:1 , 1.40
	LOCAL:3 = 0
ELSE
	TIMES LOCAL:1 , 1.60
	LOCAL:3 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL:1 , 0.10
	LOCAL:3 += 400
	TIMES LOCAL:3 , 3.00
	LOCAL:4 += 1000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL:1 , 0.40
	LOCAL:3 += 150
	TIMES LOCAL:3 , 1.00
	LOCAL:4 += 200
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:3 , 0.50
	LOCAL:4 += 80
ELSEIF PALAM:10 < PALAMLV:4
	TIMES LOCAL:1 , 1.40
	TIMES LOCAL:3 , 0.20
ELSE
	TIMES LOCAL:1 , 1.80
	TIMES LOCAL:3 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:4 , 1.20
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:4 , 1.10
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:4 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL:1 , 1.10
	TIMES LOCAL:4 , 0.90
ELSE
	TIMES LOCAL:1 , 1.20
	TIMES LOCAL:4 , 0.80
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:4 , 1.30
ELSEIF ABL:10 == 1
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:4 , 1.20
ELSEIF ABL:10 == 2
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:4 , 1.10
ELSEIF ABL:10 == 3
	TIMES LOCAL:1 , 1.10
	TIMES LOCAL:4 , 1.00
ELSEIF ABL:10 == 4
	TIMES LOCAL:1 , 1.20
	TIMES LOCAL:4 , 0.90
ELSE
	TIMES LOCAL:1 , 1.30
	TIMES LOCAL:4 , 0.80
ENDIF

;프라이드 높다
SIF TALENT:16
	TIMES SOURCE:31 , 1.20
;무관심
IF TALENT:21
	TIMES LOCAL:2 , 0.80
	TIMES SOURCE:31 , 0.80
ENDIF
;정조관념
SIF TALENT:30
	TIMES SOURCE:31 , 2.00
;도착적
IF TALENT:80
	TIMES LOCAL:2 , 1.50
	TIMES SOURCE:31 , 1.20
ENDIF
;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:3 , 1.50

;유들자와의 궁합 있고 없음으로 보정이 상하
LOCAL = 20
IF RELATION:LOCAL > 0 && RELATION:LOCAL != 100
	LOCAL:1 *= RELATION:LOCAL
	LOCAL:1 /= 100
	LOCAL:2 *= RELATION:LOCAL
	LOCAL:2 /= 100
	LOCAL:3 *= RELATION:LOCAL
	LOCAL:3 /= 100
	LOCAL:4 *= RELATION:LOCAL
	LOCAL:4 /= 100
ENDIF

SOURCE:1 += LOCAL:1
SOURCE:12 += LOCAL:2
SOURCE:20 += LOCAL:3
SOURCE:25 += LOCAL:4

TCVAR:0 += 3

;유들자의 성별상, 조교 대상이♀의 때만 레즈비언 경험이 들어가는 사양에
SIF TALENT:120 == 0
	TCVAR:12 += 1

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2
;촉수 경험 플래그
TFLAG:90 += 1
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM582
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:21 == 2
	PRINTFORML %CALLNAME:PLAYER%의 명령에 따라, 유유코는 일단 %조사처리(CALLNAME:TARGET, "로")%부터 멀어졌다.
	PRINTFORML %CALLNAME:TARGET%의 질로부터 반투명의 촉수를 뽑아 냈다.
	PRINTFORML 유유코는 촉수에 묻어있는 %CALLNAME:TARGET%의 애액을 손가락으로 훑어올려,
	PRINTL 수상한 미소를 지으면서 할짝하고 혀끝으로 가볍게 핥았다.
	PRINTFORMW 공포로 떨고 있는 %조사선택(CALLNAME:TARGET, "를")% 거들떠보지도 않고, 그대로 유유코는 육벽의 저 편으로 사라져 갔다….
ELSE
	PRINTFORML %CALLNAME:PLAYER%의 명령에 반응해, 육벽의 안쪽에서 사람이 들어갈 수 있을 정도의 굵기를 자랑하는 촉수가 한 개 나타났다.
	PRINTFORML 촉수가 %CALLNAME:TARGET%에게 가까워지자 끄트머리가 갈라져, 안에서 하반신이 이형의 물건과 동화한 유유코가 병적으로 새햐얀 나신을 뽐내며 나타났다.
	PRINTFORML 유유코는 %CALLNAME:MASTER%에게 향해 우아하게 인사 하고, 그대로 %조사선택(CALLNAME:TARGET, "를")% 덮듯이 꼭 껴안아,
	PRINTFORML 영혼이 빠져나간 것 같은 눈동자를 가능게 했다고 생각한 순간, 전체가 창백하게 빛나고 있는 반투명인 촉수를 %CALLNAME:TARGET%의 질에 꽂아 넣었다.
	PRINTFORML 유령과 같이 반투명인 촉수는 %CALLNAME:TARGET%의 질 내의 모습을 마치 영사기처럼 벽에 비추고 있다.
	PRINTL 반투명의 물체에 압박받아 펄떡거리는 질 벽은, 촉수에서 나온 인광으로 비춰지고 있다.
	PRINTFORMW 유유코는 %CALLNAME:TARGET%의 공포심과 수치심을 돋우려는 듯, 그 모습을 상세하게 말하기 시작했다….
ENDIF
