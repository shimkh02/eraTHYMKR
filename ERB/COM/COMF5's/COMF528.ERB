;=============================================================================
;촉수 채뇨
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE528
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:528 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:98 < 3
	RETURN 0
;촉수가 [抗菌촉수]を得ていないとダメ
SIF (FLAG:75 & 64) == 0
	RETURN 0
;그밖의 촉수尿道責め中はダメ
SIF TEQUIP:32 && TEQUIP:32 != 2
	RETURN 0
;[オトコ]の場合, 촉수Ｃ責め中や도구装着もダメ
SIF TALENT:120 && (TEQUIP:30 || TEQUIP:31)
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM528
JUMP COM142
RETURN 1
