@COM_ABLE525
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:525 > 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4
	RETURN 0
;남자라면 안 됨
SIF TALENT:120
	RETURN 0
;조교자는 촉수사역LV2이상 필요
SIF ABL:PLAYER:98 < 2
	RETURN 0
;그밖의 Ｂ계 촉수責め中は不可
SIF TEQUIP:35 && TEQUIP:35 != 2
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
