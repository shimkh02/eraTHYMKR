;=============================================================================
;촉수 자궁 고문
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE521
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:521 > 0
	RETURN 0
;신사적인 촉수계 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;아이템進化の秘法を持っているか, 아이템なし設定でないとダメ
SIF ITEM:59 == 0 && NOITEM == 0
	RETURN 0
;촉수조교 중이 아니면 불가
SIF TEQUIP:90 == 0
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;조교자는 촉수사역LV3이상 필요
SIF ABL:PLAYER:98 < 3
	RETURN 0
;남자라면 안 됨
SIF TALENT:120
	RETURN 0
;妊娠中はダメ
SIF TALENT:140
	RETURN 0
;懐卵中はダメ
SIF TALENT:145
	RETURN 0
;촉수삽입中じゃないとダメ
SIF TEQUIP:20 == 0
	RETURN 0
;幽霊膣鏡삽입中は不可
SIF TEQUIP:21
	RETURN 0
;卵が邪魔で子宮責めできない
SIF TEQUIP:93
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;--------------------------------------------------
;촉수 자궁 고문
;--------------------------------------------------
;촉수 삽입으로부터의 재즈 악단적 취급
@COM521
PRINTL 촉수 자궁 고문
TSTR:0 = 촉수 자궁 고문
CALL TRAIN_MESSAGE_COM521

IF TEQUIP:20 == 3
	LOCAL:1 = 70
ELSEIF TEQUIP:20 == 2
	LOCAL:1 = 50
ELSE
	LOCAL:1 = 30
ENDIF

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:1 , 2.00
	LOCAL:2 = 0
ELSEIF ABL:35 == 1
	TIMES LOCAL:1 , 1.50
	LOCAL:2 = 50
ELSEIF ABL:35 == 2
	TIMES LOCAL:1 , 1.20
	LOCAL:2 = 120
ELSEIF ABL:35 == 3
	TIMES LOCAL:1 , 1.00
	LOCAL:2 = 350
ELSEIF ABL:35 == 4
	TIMES LOCAL:1 , 0.80
	LOCAL:2 = 900
ELSE
	TIMES LOCAL:1 , 0.60
	LOCAL:2 = 2000
ENDIF

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:1 *2

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
LOCAL:3 = 2

SOURCE:24 = 200
SOURCE:19 = 500
SOURCE:31 = 2000
SOURCE:32 = 500

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 10
	SOURCE:22 = 20
ELSEIF ABL:1 == 1
	SOURCE:1 = 150
	SOURCE:22 = 120
ELSEIF ABL:1 == 2
	SOURCE:1 = 600
	SOURCE:22 = 500
ELSEIF ABL:1 == 3
	SOURCE:1 = 1800
	SOURCE:22 = 1200
ELSEIF ABL:1 == 4
	SOURCE:1 = 2400
	SOURCE:22 = 1800
ELSE
	SOURCE:1 = 3200
	SOURCE:22 = 2400
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	TIMES SOURCE:22, 0.20
	SOURCE:20 = 300
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:22, 0.50
	SOURCE:20 = 160
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:22, 0.80
	SOURCE:20 = 100
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:22, 1.00
	SOURCE:20 = 60
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:22, 1.20
	SOURCE:20 = 20
ELSE
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:22, 1.50
	SOURCE:20 = 0
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;정조관념
IF TALENT:30
	IF TALENT:0 == 1
		TIMES SOURCE:22 , 0.60
		TIMES SOURCE:31, 5.00
		SOURCE:32 += 2000
	ELSE
		TIMES SOURCE:22 , 0.60
		SOURCE:32 += 1000
	ENDIF
;정조무구애
ELSEIF TALENT:31
	IF TALENT:0 == 1
		TIMES SOURCE:22 , 0.60
		SOURCE:32 += 300
	ENDIF
ELSE
	SIF TALENT:0 == 1
		SOURCE:32 += 3000
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:20 += 2000
	TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:1 , 0.20
	SOURCE:20 += 800
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:20 , 0.80
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:20 , 0.50
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:24 , 0.90
	TIMES SOURCE:19 , 0.90
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:24 , 1.00
	TIMES SOURCE:19 , 1.00
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 2.10
	TIMES SOURCE:24 , 1.10
	TIMES SOURCE:19 , 1.10
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 3.20
	TIMES SOURCE:24 , 1.20
	TIMES SOURCE:19 , 1.20
ELSE
	TIMES SOURCE:1 , 4.30
	TIMES SOURCE:24 , 1.30
	TIMES SOURCE:19 , 1.30
ENDIF

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:103
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:102
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

SOURCE:15 = LOCAL:2 /2
SOURCE:30 = LOCAL:1 *150
SOURCE:23 = LOCAL:2 /4

IF TEQUIP:20 == 2
	LOCAL:3 += 2
ELSEIF TEQUIP:20 == 3
	LOCAL:3 += 3
ENDIF
TEQUIP:20 = 3
TCVAR:0 += LOCAL:3
TCVAR:32 += 1

;커맨드 속성：촉수
TFLAG:98 = 8
;촉수 경험 플래그
TFLAG:90 = 2
RETURN 1

;--------------------------------------------------
;촉수 삽입중(자궁 고문)
;--------------------------------------------------
@EQUIP_COM521
PRINTL <촉수 자궁 고문 중>
CALL EQUIP_MESSAGE_COM521

IF TEQUIP:20 == 3
	LOCAL:4 = 35
ELSEIF TEQUIP:20 == 2
	LOCAL:4 = 25
ELSE
	LOCAL:4 = 15
ENDIF

;ABL:촉수 중독을 본다
IF ABL:35 == 0
	TIMES LOCAL:4 , 2.00
	LOCAL:3 = 0
ELSEIF ABL:35 == 1
	TIMES LOCAL:4 , 1.50
	LOCAL:3 = 25
ELSEIF ABL:35 == 2
	TIMES LOCAL:4 , 1.20
	LOCAL:3 = 60
ELSEIF ABL:35 == 3
	TIMES LOCAL:4 , 1.00
	LOCAL:3 = 180
ELSEIF ABL:35 == 4
	TIMES LOCAL:4 , 0.80
	LOCAL:3 = 450
ELSE
	TIMES LOCAL:4 , 0.60
	LOCAL:3 = 1000
ENDIF

DOWNBASE:0 += LOCAL:4
DOWNBASE:1 += LOCAL:4 *2

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;ABL:V감각을 본다
IF ABL:1 == 0
	LOCAL = 100
ELSEIF ABL:1 == 1
	LOCAL = 200
ELSEIF ABL:1 == 2
	LOCAL = 400
ELSEIF ABL:1 == 3
	LOCAL = 600
ELSEIF ABL:1 == 4
	LOCAL = 800
ELSE
	LOCAL = 1000
ENDIF

;EXP:V경험을 본다
;처녀는 있을 수 없다
IF EXP:0 < EXPLV:2
	TIMES LOCAL , 0.60
	LOCAL:1 = 150
ELSEIF EXP:0 < EXPLV:3
	TIMES LOCAL , 1.00
	LOCAL:1 = 20
ELSEIF EXP:0 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:1 = 0
ELSEIF EXP:0 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:1 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:1 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL , 0.10
	LOCAL:1 += 400
	TIMES LOCAL:1 , 3.00
	LOCAL:2 += 1000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL , 0.40
	LOCAL:1 += 150
	TIMES LOCAL:1 , 1.00
	LOCAL:2 += 200
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL , 1.00
	TIMES LOCAL:1 , 0.50
	LOCAL:2 += 80
ELSEIF PALAM:10 < PALAMLV:4
	TIMES LOCAL , 1.40
	TIMES LOCAL:1 , 0.20
ELSE
	TIMES LOCAL , 1.80
	TIMES LOCAL:1 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:1 , 2.00

;정조관념(처녀는 있을 수 없다)
SIF TALENT:30
	TIMES LOCAL:1, 3.00

SOURCE:1 += LOCAL/2
SOURCE:24 += LOCAL/4
SOURCE:20 += LOCAL:1 *3
SOURCE:30 += LOCAL:3 *50
SOURCE:31 += LOCAL:2
SOURCE:32 += LOCAL:2

TCVAR:0 += 3
;촉수 경험 플래그
TFLAG:90 += 2

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM521
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:20 == 2
	PRINTFORML %CALLNAME:TARGET%의 질 내의 껄끔거리는 천정을 더듬고 있던 촉수는
	PRINTFORML 갑자기 자궁구를 비틀어 열어 안쪽으로 돌진해, %CALLNAME:TARGET%의 자궁 내로 파고들었다.
	PRINTFORMW 촉수로 채워진 %CALLNAME:TARGET%의 하복부는 마치 임신하고 있는 것 같이 부풀어 오르고 있다….
ELSEIF TEQUIP:20 == 3
	PRINTFORMW 자궁에 자리잡고 있는 촉수는 %CALLNAME:TARGET%의 자궁벽 위를 덧쓰는 것처럼 돌기로 더듬었다.
ELSE
	PRINTFORML %CALLNAME:TARGET%의 성기에 기어들어 비벼대고 있던 촉수는 한층 더 안쪽으로 기어들어가,
	PRINTFORML 촉수는 %CALLNAME:TARGET%의 자궁구를 비틀어 열어 그대로 자궁 내에 비집고 메워갔다.
	PRINTFORMW 촉수로 가득 찬 %CALLNAME:TARGET%의 배는 마치 임산부와 같이 부풀어 올랐다….
ENDIF

;--------------------------------------------------
;장착시 메시지
;--------------------------------------------------
@EQUIP_MESSAGE_COM521
CALL KOJO_EQUIP_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
PRINTFORML %CALLNAME:TARGET%의 복부는 촉수로 채워져 크게 부풀어 오르고 있다….
