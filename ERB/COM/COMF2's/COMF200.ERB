@COM_ABLE200
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:200 > 0
	RETURN 0
;Ｂ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 8) == 0
	RETURN 0
;남자라면 안 됨
SIF TALENT:120
	RETURN 0
;ウェディングドレス · ゴスロリ着用中の場合기교4이상 필수
SIF (TEQUIP:59 == 19 || TEQUIP:59 == 20) && ABL:PLAYER:12 < 4
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM200
;모유 체질의 경우 착유로 난다. 그렇지만, 미마 님의 착유라면 조금 보고 싶을지도
SIF TALENT:134
	JUMP COM626

PRINTL 가슴 주무르기
TSTR:0 = 가슴 주무르기
CALL TRAIN_MESSAGE_COM200

DOWNBASE:0 += 5
DOWNBASE:1 += 50

SOURCE:15 = 30
SOURCE:30 = 10
SOURCE:12 = 50

;ABL:B감각을 본다
IF ABL:3 == 0
	SOURCE:3 = 10
	SOURCE:11 = 20
ELSEIF ABL:3 == 1
	SOURCE:3 = 50
	SOURCE:11 = 50
ELSEIF ABL:3 == 2
	SOURCE:3 = 250
	SOURCE:11 = 80
ELSEIF ABL:3 == 3
	SOURCE:3 = 600
	SOURCE:11 = 100
ELSEIF ABL:3 == 4
	SOURCE:3 = 1000
	SOURCE:11 = 120
ELSE
	SOURCE:3 = 1600
	SOURCE:11 = 200
ENDIF

;음유
IF TALENT:174
	TIMES SOURCE:11 , 1.40
	TIMES SOURCE:30 , 0.80
	TIMES SOURCE:3 , 1.40
ENDIF

;조교자가［유아］,［유치］
IF TALENT:PLAYER:130 || TALENT:PLAYER:131
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:11 , 1.20
ENDIF
;조교자가［유아 퇴행］
SIF TALENT:PLAYER:159
	TIMES SOURCE:11 , 1.20

;노예의 B⇔조교자의 손가락의 불결이 이동
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 5
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 5
ENDIF

RETURN 1

