@COM_ABLE230
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:230 > 0
	RETURN 0
;조교자에게 페니스가 없으면 불가
SIF EXIST_BAR(PLAYER) == 0
	RETURN 0
;[猫舌]だとダメ
SIF TALENT:54
	RETURN 0
;주인이 [금단의 지식]持ちじゃないとき, 体格差が2段階以内か조교자가 [금단의 지식]持ちじゃないとダメ
SIF TALENT:MASTER:56 == 0 && COMMON_BODYSIZE_CHECK(PLAYER, TARGET) == 0
	RETURN 0
;순종, 욕망의 합계가 6이상 없으면 안 됨
SIF ABL:10 + ABL:11 < 6
	RETURN 0
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;볼 개그 사용 중에는 불가
SIF TEQUIP:42
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;의사 선생님 플레이 중에는 불가
SIF TEQUIP:57
	RETURN 0
;조수가 훈도시 착용 중일 경우에는 불가
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17
	RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:98 <= 2
	RETURN 0
;촉수 관통중은 안됨
SIF TEQUIP:90 && TEQUIP:25 == 3
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM230
;전회의 조교가 둘이서 범하는 것이라고 하고 둘이서 범하는에
TFLAG:132 = 0
IF PREVCOM == 610
	CALL EXCOM_ABLE610
	IF RESULT == 1
		TFLAG:132 = 1
		JUMP COM610
	ENDIF
;조교자가 전회:조수로 이번:주인인가, 전회:주인으로 이번:조수 때
ELSEIF (ASSIPLAY && TFLAG:133 == 0) || (ASSIPLAY == 0 && TFLAG:133)
	;전회의 조교가 정상위나 후배위나 후배위 애널인가 둘이서 범하는 것 때는 둘이서 범하는에
	IF PREVCOM == 20 || PREVCOM == 21 || PREVCOM == 31 || PREVCOM == 610
		CALL EXCOM_ABLE610
		SIF RESULT == 1
			JUMP COM610
	ENDIF
ENDIF

;전회와 이번 조교자가 같아 새드끼 LV3 이상이나 욕망 LV3 이상으로 줄사용중이 아니고 전회의 조교가 이라마치오 때
IF PREVCOM == 230 && TEQUIP:40 == 0 && (ABL:PLAYER:20 > 2 || ABL:PLAYER:11 > 2) && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))
	PRINTL 딥스로트
ELSE
	PRINTL 이라마치오
ENDIF

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
IF TFLAG:699 == 0
	CALL COM_ORDER
	;실행할 수 없다
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
;전회와 이번 조교자가 같아 새드끼 LV3 이상이나 욕망 LV3 이상으로 줄사용중이 아니고 전회의 조교가 이라마치오 때
IF PREVCOM == 230 && TEQUIP:40 == 0 && (ABL:PLAYER:20 > 2 || ABL:PLAYER:11 > 2) && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))
	TSTR:0 = 딥스로트
ELSE
	TSTR:0 = 이라마치오
ENDIF
CALL TRAIN_MESSAGE_COM230

;-------------------------------------------------
;사정 게이지 체크
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 1200
ELSEIF ABL:12 == 1
	LOCAL = 1700
ELSEIF ABL:12 == 2
	LOCAL = 2300
ELSEIF ABL:12 == 3
	LOCAL = 3000
ELSEIF ABL:12 == 4
	LOCAL = 3600
ELSE
	LOCAL = 4200
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)
;정액 중독을 본다
LOCAL = EJACULATIONCHECK_ABL_31_1(TARGET, LOCAL)

;TALENT:혀사용을 본다
SIF TALENT:52
	TIMES LOCAL , 2.00
;TALENT:고양이혀을 본다(실행할 수 없지만 일단)
SIF TALENT:54
	TIMES LOCAL , 0.70

;조교자의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF TALENT:PLAYER:120 || TALENT:PLAYER:121
	BASE:PLAYER:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 200
DOWNBASE:1 += 150

SOURCE:20 = 200
SOURCE:22 = 1500
SOURCE:31 = 500
SOURCE:17 = 500

;위 쪽으로 계산한 더러운 데이터
SOURCE:30 = TFLAG:242*40 + 100

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:15 = 420
	SOURCE:16 = 150
	TIMES SOURCE:30 , 4.00
ELSEIF ABL:13 == 1
	SOURCE:15 = 500
	SOURCE:16 = 300
	TIMES SOURCE:30 , 2.50
ELSEIF ABL:13 == 2
	SOURCE:15 = 580
	SOURCE:16 = 600
	TIMES SOURCE:30 , 1.50
ELSEIF ABL:13 == 3
	SOURCE:15 = 660
	SOURCE:16 = 900
	TIMES SOURCE:30 , 1.00
ELSEIF ABL:13 == 4
	SOURCE:15 = 740
	SOURCE:16 = 1500
	TIMES SOURCE:30 , 0.50
ELSE
	SOURCE:15 = 820
	SOURCE:16 = 2200
	TIMES SOURCE:30 , 0.10
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:16 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
ELSE
	TIMES SOURCE:15 , 2.00
	TIMES SOURCE:16 , 2.00
ENDIF

;전회와 이번 조교자가 같아 새드끼 LV3 이상이나 욕망 LV3 이상으로 줄사용중이 아니고 전회의 조교가 이라마치오 때
IF PREVCOM == 230 && TEQUIP:40 == 0 && (ABL:PLAYER:20 > 2 || ABL:PLAYER:11 > 2) && ((ASSIPLAY && TFLAG:133) || (ASSIPLAY == 0 && TFLAG:133 == 0))
	TIMES DOWNBASE:0 , 1.10
	TIMES SOURCE:17 , 1.10
	TIMES SOURCE:16 , 1.10
	TIMES SOURCE:20 , 1.10
	TIMES SOURCE:31 , 1.10
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(PLAYER)
LOCAL:2 = TFLAG:91
;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	TIMES SOURCE:15 , 3.00

	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 4.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 500
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 3.00
	ELSEIF ABL:31 == 2
		SOURCE:24 = 1200
		TIMES SOURCE:16 , 4.00
		TIMES SOURCE:22, 2.50
	ELSEIF ABL:31 == 3
		SOURCE:24 = 3000
		TIMES SOURCE:16 , 6.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 4
		SOURCE:24 = 6000
		TIMES SOURCE:16 , 9.00
		TIMES SOURCE:22, 1.50
	ELSE
		SOURCE:24 = 12000
		TIMES SOURCE:16 , 15.00
		TIMES SOURCE:22, 1.00
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 4
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
TCVAR:22 += 1

;노예의 입⇔조교자의 P의 불결이 이동
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

;봉사 정신 LV2 이상, 기교 LV2 이상이라면 P의 더러움을 없는 취한다
;다만 대상이 고양이혀의 경우, 고양이 펠라 패치 유효해 게다가
;봉사 정신 LV4 이상, 기교 LV5, 순종 LV3 이상 없으면 않은 취하지 않는다
IF (TALENT:54 == 0 && ABL:13 >= 2 && ABL:12 >= 2) || (TALENT:54 && FLAG:14 & 16 && ABL:13 >= 4 && ABL:12 >= 5 && ABL:10 >=3)
	STAIN:PLAYER:2 = 2
	SIF LOCAL:2 >= 1
		TFLAG:21 = 1
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 7
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 7
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:50 += 1

;조교자가 후타나리
SIF TALENT:PLAYER:121
	SOURCE:22 /= 2
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

RETURN 1

