@COM_ABLE271
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:271 > 0
	RETURN 0
;조교자奉仕 · ウフフ系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 2048) == 0
	RETURN 0
;奴隷が새드持ちまたはドＳ持ちまたは새드끼Lv1이상이 아니면 불가
SIF TALENT:81 == 0 && TALENT:177 == 0 && ABL:20 < 1
	RETURN 0
;조교자가 고양이혀일 경우 고양이 펠라 패치 유효가 아니면 불가
SIF TALENT:PLAYER:54 && (FLAG:14 & 16) == 0
	RETURN 0
;조교자가 猫舌だと기교2필요
SIF TALENT:PLAYER:54 && ABL:PLAYER:12 < 2
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;볼 개그 사용 중에는 불가
SIF TEQUIP:42
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM271
PRINTL 다리 빨기
TSTR:0 = 다리 빨기
CALL TRAIN_MESSAGE_COM271

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 5
DOWNBASE:1 += 100

SOURCE:14 = 350
SOURCE:16 = 70
SOURCE:17 = 50
SOURCE:31 = 100

;ABL:조교자의 기교를 본다
IF ABL:PLAYER:12 == 0
	TIMES SOURCE:14 , 0.8
	TIMES SOURCE:16 , 0.8
	TIMES SOURCE:17 , 0.8
ELSEIF ABL:PLAYER:12 == 1
	TIMES SOURCE:14 , 1.0
	TIMES SOURCE:16 , 1.0
	TIMES SOURCE:17 , 1.0
ELSEIF ABL:PLAYER:12 == 2
	TIMES SOURCE:14 , 1.2
	TIMES SOURCE:16 , 1.1
	TIMES SOURCE:17 , 1.0
ELSEIF ABL:PLAYER:12 == 3
	TIMES SOURCE:14 , 1.5
	TIMES SOURCE:16 , 1.3
	TIMES SOURCE:17 , 1.1
ELSEIF ABL:PLAYER:12 == 4
	TIMES SOURCE:14 , 1.8
	TIMES SOURCE:16 , 1.5
	TIMES SOURCE:17 , 1.2
ELSE
	TIMES SOURCE:14 , 2.0
	TIMES SOURCE:16 , 1.8
	TIMES SOURCE:17 , 1.5
ENDIF

;조교자의 TALENT:혀사용을 본다
IF TALENT:PLAYER:52
	TIMES SOURCE:17 , 1.2
ENDIF
;조교자의 고양이혀을 본다
IF TALENT:PLAYER:54
	SOURCE:20 += 50
	SOURCE:21 += 10
ENDIF

;ABL:새드끼를 본다
IF ABL:20 <= 1
	TIMES SOURCE:14 , 0.8
	TIMES SOURCE:16 , 0.8
	TIMES SOURCE:17 , 0.7
ELSEIF ABL:20 == 2
	TIMES SOURCE:14 , 1.0
	TIMES SOURCE:16 , 1.0
	TIMES SOURCE:17 , 0.9
ELSEIF ABL:20 == 3
	TIMES SOURCE:14 , 1.4
	TIMES SOURCE:16 , 1.2
	TIMES SOURCE:17 , 1.0
ELSEIF ABL:20 == 4
	TIMES SOURCE:14 , 1.8
	TIMES SOURCE:16 , 1.5
	TIMES SOURCE:17 , 1.2
ELSE
	TIMES SOURCE:14 , 2.2
	TIMES SOURCE:16 , 2.0
	TIMES SOURCE:17 , 1.6
ENDIF

;ABL:조교자의 마조끼를 본다
IF ABL:PLAYER:21 == 0
	TIMES SOURCE:14 , 0.8
	TIMES SOURCE:16 , 0.8
	TIMES SOURCE:17 , 0.8
ELSEIF ABL:PLAYER:21 == 1
	TIMES SOURCE:14 , 1.0
	TIMES SOURCE:16 , 1.0
	TIMES SOURCE:17 , 1.0
ELSEIF ABL:PLAYER:21 == 2
	TIMES SOURCE:14 , 1.2
	TIMES SOURCE:16 , 1.1
	TIMES SOURCE:17 , 1.0
ELSEIF ABL:PLAYER:21 == 3
	TIMES SOURCE:14 , 1.5
	TIMES SOURCE:16 , 1.3
	TIMES SOURCE:17 , 1.1
ELSEIF ABL:PLAYER:21 == 4
	TIMES SOURCE:14 , 1.8
	TIMES SOURCE:16 , 1.5
	TIMES SOURCE:17 , 1.2
ELSE
	TIMES SOURCE:14 , 2.0
	TIMES SOURCE:16 , 1.8
	TIMES SOURCE:17 , 1.5
ENDIF

;조교자가 왕M
IF TALENT:PLAYER:178
	TIMES SOURCE:14 , 1.5
	TIMES SOURCE:16 , 1.5
	TIMES SOURCE:17 , 1.5
	SOURCE:18 = 240
;조교자가 마조
ELSEIF TALENT:PLAYER:82
	TIMES SOURCE:14 , 1.2
	TIMES SOURCE:16 , 1.2
	TIMES SOURCE:17 , 1.2
	SOURCE:18 = 120
ENDIF
;왕S
IF TALENT:177
	TIMES SOURCE:14 , 1.5
	TIMES SOURCE:16 , 1.5
	TIMES SOURCE:17 , 1.5
;새드
ELSEIF TALENT:81
	TIMES SOURCE:14 , 1.2
	TIMES SOURCE:16 , 1.2
	TIMES SOURCE:17 , 1.2
ENDIF

;상애
IF TALENT:153
	SOURCE:11 = 500
	TIMES SOURCE:31 , 0.8
;친애
ELSEIF TALENT:152
	SOURCE:11 = 200
;연모
ELSEIF TALENT:150
	SOURCE:11 = 50
ENDIF

;조교자의 입⇔대상의 다리의 더러움이 서로 이동
STAIN:6 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:6

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 3
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 3
ENDIF
RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM271
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
;고양이혀
IF TALENT:PLAYER:54
	PRINTFORMW %CALLNAME:TARGET%의 다리를 혀끝으로 조금씩 간질였다….
ELSE
	PRINTFORMW %CALLNAME:TARGET%의 다리를 상냥하게 핥았다….
ENDIF
