@COM_ABLE228
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:228 > 0
	RETURN 0
;촉수鼻穴삽입中だとダメ
SIF TEQUIP:44 == 2
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:44 == 1
	RETURN 1
;아이템을 가지고 있거나, 아이템 없음 설정이 아니면 불가
SIF ITEM:35 == 0 && NOITEM == 0
	RETURN 0
;ASSI일 경우, 기교3이상이 아니면 쓸 수 없다。
IF ASSIPLAY
	SIF ABL:ASSI:12 < 3
		RETURN 0
ENDIF
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;	마음대로 사용한 변수
;		TEQUIP:44 = 코훅 (1:코훅 장착, 2:촉수비혈삽입)
;								……(으)로서 사용할 수 있으면 좋겠다, 라고.
;
;……네?　목걸이도 없이 어떻게 코훅만으로 고정하고 있는지라는?
;그것은 반드시 마법의 힘이에요. 그러니까, 목걸이가 이미 야외 플레이용으로서
;사용되고 있으니 어쩔 수 없었다고 그런 일은 관계없는 것이에요.
;-------------------------------------------------
@COM228
TSTR:7 = 코훅
IF FLAG:1100 & 1
	;技師パッチ有効なら特殊な名前チェック
	FLAG:1110 = 35
	CALL NAMECHECK_CUSTOMTOOL
ELSE
	TSTR:8 = %TSTR:7%
	TSTR:9 = %TSTR:7%
ENDIF
PRINTFORML %TSTR:8%
TSTR:0 = 코훅
CALL TRAIN_MESSAGE_COM228

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
;긴박 경험이 높으면 소비 감소
IF EXP:31 < EXPLV:3
	DOWNBASE:0 += 80
	DOWNBASE:1 += 150
ELSEIF EXP:31 < EXPLV:4
	DOWNBASE:0 += 60
	DOWNBASE:1 += 100
ELSE
	DOWNBASE:0 += 40
	DOWNBASE:1 += 50
ENDIF

SOURCE:12 = 150
SOURCE:17 = 80
SOURCE:20 = 80
SOURCE:22 = 250
SOURCE:23 = 100
SOURCE:31 = 400
SOURCE:32 = 50

;ABL:마조끼를 본다
IF ABL:21 == 0
	LOCAL:1 = 50
	LOCAL:2 = 0
ELSEIF ABL:21 == 1
	LOCAL:1 = 150
	LOCAL:2 = 100
ELSEIF ABL:21 == 2
	LOCAL:1 = 300
	LOCAL:2 = 150
ELSEIF ABL:21 == 3
	LOCAL:1 = 500
	LOCAL:2 = 200
ELSEIF ABL:21 == 4
	LOCAL:1 = 750
	LOCAL:2 = 300
ELSE
	LOCAL:1 = 1000
	LOCAL:2 = 450
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 0.70
ELSEIF ABL:14 == 1
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:2 , 0.80
ELSEIF ABL:14 == 2
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 0.90
ELSEIF ABL:14 == 3
	TIMES LOCAL:1 , 1.10
	TIMES LOCAL:2 , 1.00
ELSEIF ABL:14 == 4
	TIMES LOCAL:1 , 1.30
	TIMES LOCAL:2 , 1.10
ELSE
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.20
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:2 , 0.70
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL:1 , 1.10
	TIMES LOCAL:2 , 1.00
ELSE
	TIMES LOCAL:1 , 1.20
	TIMES LOCAL:2 , 1.10
ENDIF

;TALENT:감정결여를 본다
IF TALENT:22
	TIMES LOCAL:1 , 0.50
	TIMES LOCAL:2 , 0.50
ENDIF
;TALENT:억압을 본다
IF TALENT:32
	TIMES LOCAL:1 , 0.20
	TIMES LOCAL:2 , 0.10
ENDIF
;TALENT:해방을 본다
SIF TALENT:33
	TIMES LOCAL:1 , 1.10
;TALENT:부끄럼쟁이을 본다
IF TALENT:35
	TIMES LOCAL:1 , 1.20
	TIMES LOCAL:2 , 0.80
;TALENT:수치 얇은을 본다
ELSEIF TALENT:36
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:2 , 0.80
ENDIF
;TALENT:도착적을 본다
IF TALENT:80
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.40
ENDIF
;TALENT:광기를 본다
IF TALENT:158
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 0.50
ENDIF
;TALENT:정신 붕괴를 본다
IF TALENT:167
	TIMES LOCAL:1 , 0.10
	TIMES LOCAL:2 , 0.10
ENDIF
;TALENT:왕M를 본다
IF TALENT:178
	TIMES LOCAL:1 , 2.00
	TIMES LOCAL:2 , 1.80
ENDIF

IF FLAG:1100 & 1
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 35
	FLAG:1111 = 1
	FLAG:1112 = LOCAL:1
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL:1 = FLAG:1120
	FLAG:1112 = LOCAL:2
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL:2 = FLAG:1120
ENDIF

SOURCE:12 += LOCAL:1
SOURCE:17 += LOCAL:1
SOURCE:22 += LOCAL:1
SOURCE:23 += LOCAL:1 / 4
SOURCE:24 += LOCAL:2
SOURCE:31 += LOCAL:1
SOURCE:32 += LOCAL:1 / 3

TCVAR:31 += 2
;SM교육 경험 플래그
SIF ASSIPLAY
	TFLAG:55 = 1

;커맨드 속성：도구(조교자)
SIF TEQUIP:90 == 0
	TFLAG:98 = 1

;코훅의 착탈
IF TEQUIP:44
	TEQUIP:44 = 0
ELSE
	TEQUIP:44 = 1
ENDIF
RETURN 1

