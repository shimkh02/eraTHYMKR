@COM_ABLE238
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:238 > 0
	RETURN 0
;강력 아이템 · 커맨드 무효시에는 불가
SIF (FLAG:13 & 2) == 0
	RETURN 0
;失神パッチ無効時もダメ
SIF FLAG:3999 == 0
	RETURN 0
;スタンガンを持っているか, 아이템なし設定でないとダメ
SIF ITEM:37 == 0 && NOITEM == 0
	RETURN 0
;助手の場合, 순종5か새드かドＳか狂気でないとダメ
IF ASSIPLAY
	SIF ABL:PLAYER:10 < 5 || (TALENT:PLAYER:81 == 0 && TALENT:PLAYER:177 == 0 && TALENT:PLAYER:158 == 0)
		RETURN 0
ENDIF
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;기계면 불가
SIF TALENT:206
	RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;시체는 안됨
SIF TALENT:226 == 1
	RETURN 0
RETURN 1

