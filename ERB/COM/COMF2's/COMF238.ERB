;기절시킬 뿐. 또한 기절중이라든지는 사용할 수 없다(라고 할까 효과를 알 수 있지 않아)
@COM238
TSTR:7 = 스턴건
IF FLAG:1100 & 1
	;技師パッチ有効なら特殊な名前チェック
	FLAG:1110 = 37
	CALL NAMECHECK_CUSTOMTOOL
ELSE
	TSTR:8 = %TSTR:7%
	TSTR:9 = %TSTR:7%
ENDIF
PRINTFORML %TSTR:8%
TSTR:0 = 스턴건
CALL TRAIN_MESSAGE_COM238

DOWNBASE:0 += 10
DOWNBASE:1 += 10

SOURCE:20 = 800
SOURCE:31 = 1000

IF (FLAG:1100 & 1) && TEQUIP:90 == 0
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 37
	FLAG:1111 = 2
	FLAG:1112 = SOURCE:20
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:20 = FLAG:1120
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF
;주인 경험 플래그
SIF ASSIPLAY == 0 && ABL:21 >= 3
	TFLAG:50 += 1
;SM교육 경험 플래그
SIF ASSIPLAY
	TFLAG:55 = 1

;이번 전기쇼크건 사용한 플래그
TFLAG:159 = 1

;커맨드 속성：도구(조교자)
SIF TEQUIP:90 == 0
	TFLAG:98 = 1
RETURN 1

