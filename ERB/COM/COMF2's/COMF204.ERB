@COM_ABLE204
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:204 > 0
	RETURN 0
;Ｂ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 8) == 0
	RETURN 0
;母乳体質じゃなきゃダメ
SIF TALENT:134 == 0
	RETURN 0
;Ｂ계 장착구 사용 중에는 불가
SIF TEQUIP:35 || TEQUIP:36
	RETURN 0
;ウェディングドレス · ゴスロリ着用中の場合기교4이상 필수
SIF (TEQUIP:59 == 19 || TEQUIP:59 == 20) && ABL:PLAYER:12 < 4
	RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:40
	RETURN 0
;애태우기 플레이 중에는 불가
SIF TEQUIP:68
	RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:98 <= 2
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM204
PRINTL 모유 마시기
TSTR:0 = 모유 마시기
CALL TRAIN_MESSAGE_COM204

DOWNBASE:0 += 10
DOWNBASE:1 += 100

SOURCE:12 = 100
SOURCE:15 = 60
SOURCE:16 = 40

;ABL:순종을 본다
IF ABL:10 == 0
	SOURCE:30 = 300
ELSEIF ABL:10 == 1
	SOURCE:30 = 200
ELSEIF ABL:10 == 2
	SOURCE:30 = 150
ELSEIF ABL:10 == 3
	SOURCE:30 = 100
ELSEIF ABL:10 == 4
	SOURCE:16 += 20
	SOURCE:30 = 50
ELSE
	SOURCE:16 += 100
ENDIF

;ABL:Ｂ감각을 본다
IF ABL:3 == 0
	SOURCE:3 = 20
	SOURCE:11 = 150
ELSEIF ABL:3 == 1
	SOURCE:3 = 100
	SOURCE:11 = 300
ELSEIF ABL:3 == 2
	SOURCE:3 = 500
	SOURCE:11 = 480
ELSEIF ABL:3 == 3
	SOURCE:3 = 1200
	SOURCE:11 = 600
ELSEIF ABL:3 == 4
	SOURCE:3 = 2000
	SOURCE:11 = 800
ELSE
	SOURCE:3 = 2800
	SOURCE:11 = 1000
ENDIF

;ABL:봉사정신을 본다
IF ABL:13 == 0
	TIMES SOURCE:11 , 0.60
	TIMES SOURCE:15 , 0.60
	TIMES SOURCE:30 , 1.40
	TIMES SOURCE:3 , 0.60
ELSEIF ABL:13 == 1
	TIMES SOURCE:11 , 1.00
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:30 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF ABL:13 == 2
	TIMES SOURCE:11 , 1.20
	TIMES SOURCE:15 , 1.20
	TIMES SOURCE:30 , 0.80
	TIMES SOURCE:3 , 1.20
ELSEIF ABL:13 == 3
	TIMES SOURCE:11 , 1.40
	TIMES SOURCE:15 , 1.40
	TIMES SOURCE:30 , 0.60
	TIMES SOURCE:3 , 1.40
ELSEIF ABL:13 == 4
	TIMES SOURCE:11 , 1.60
	TIMES SOURCE:15 , 1.60
	TIMES SOURCE:30 , 0.60
	TIMES SOURCE:3 , 1.60
ELSE
	TIMES SOURCE:11 , 1.60
	TIMES SOURCE:15 , 1.60
	TIMES SOURCE:30 , 0.40
	TIMES SOURCE:3 , 1.60
ENDIF
IF TALENT:134
	;ABL:분유중독을 본다
	IF ABL:34 == 0
		TIMES SOURCE:3 , 1.00
		TIMES SOURCE:11 , 1.00
	ELSEIF ABL:34 == 1
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:11 , 1.20
	ELSEIF ABL:34 == 2
		TIMES SOURCE:3 , 1.40
		TIMES SOURCE:11 , 1.40
	ELSEIF ABL:34 == 3
		TIMES SOURCE:3 , 1.50
		TIMES SOURCE:11 , 1.50
	ELSEIF ABL:34 == 4
		TIMES SOURCE:3 , 1.60
		TIMES SOURCE:11 , 1.60
	ELSE
		TIMES SOURCE:3 , 1.80
		TIMES SOURCE:11 , 1.80
	ENDIF
ENDIF

;TALENT:연모를 본다
IF TALENT:150
	TIMES SOURCE:11 , 1.60
	TIMES SOURCE:15 , 1.60
	TIMES SOURCE:30 , 0.40
	TIMES SOURCE:3 , 1.60
ELSE
	TIMES SOURCE:11 , 0.60
	TIMES SOURCE:15 , 0.60
	TIMES SOURCE:30 , 1.60
	TIMES SOURCE:3 , 0.60
ENDIF

;음유
IF TALENT:174
	TIMES SOURCE:11 , 1.40
	TIMES SOURCE:15 , 1.40
	TIMES SOURCE:3 , 1.40
ENDIF

;조교자의 TALENT:혀놀림을 본다
SIF TALENT:PLAYER:52
	TIMES SOURCE:3 , 1.40
;조교자의 TALENT:고양이혀를 본다
SIF TALENT:PLAYER:54
	TIMES SOURCE:3 , 0.90
;조교자가 ［유아］, ［유치］
IF TALENT:PLAYER:130 || TALENT:PLAYER:131
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:11 , 1.40
ENDIF
;조교자가 ［유아퇴행］
SIF TALENT:PLAYER:159
	TIMES SOURCE:11 , 1.20

;조교자가 대상의 친자식
IF (NO:PLAYER == 148 || NO:PLAYER == 149) && (CFLAG:PLAYER:3105 == CFLAG:TARGET:3109 || CFLAG:PLAYER:3107 == CFLAG:TARGET:3109)
	TIMES SOURCE:11 , 2.00
	TIMES SOURCE:15 , 1.80
	TIMES SOURCE:30 , 0.30
	TIMES SOURCE:3 , 1.70
;조교자가 대상의 의붓자식
ELSEIF (NO:PLAYER == 148 || NO:PLAYER == 149) && CFLAG:PLAYER:3106 == CFLAG:TARGET:3109
	TIMES SOURCE:11 , 1.60
	TIMES SOURCE:15 , 1.60
	TIMES SOURCE:30 , 0.40
	TIMES SOURCE:3 , 1.60
ENDIF

;노예의 Ｂ⇔조교자의 입의 불결이 이동
STAIN:5 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:5

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 5
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 5
ENDIF

RETURN 1

