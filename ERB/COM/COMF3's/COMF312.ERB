;=============================================================================
;더블 V 피스트
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE312
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:312 > 0
	RETURN 0
;조수가 없으면 불가
SIF ASSI < 0
	RETURN 0
;主人じゃないとダメ
SIF ASSIPLAY
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;Ｖ경험LVが1미만で, 조교자가 オトコではない場合
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && EXP:0 < EXPLV:1 && TALENT:PLAYER:120 == 0
	RETURN 0
;対象か조수가 オトコだとダメ
SIF TALENT:120 || TALENT:ASSI:120
	RETURN 0
;작은 체형なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
	SIF ABL:10 < 4 || ABL:21 < 4
		RETURN 0
ENDIF
;助手も작은 체형なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:ASSI:128
	SIF ABL:ASSI:10 < 4 || ABL:ASSI:21 < 4
		RETURN 0
ENDIF
;조교 대상か조수가 소인체형일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF (TALENT:127 || TALENT:ASSI:127) && TALENT:MASTER:56 == 0
	RETURN 0
;주인이 새드か狂気じゃないとできない。
SIF TALENT:MASTER:81 == 0 && TALENT:MASTER:158 == 0
	RETURN 0
;일정의 Ｖ경험이 필요([巨躯]の場合は軽減)
IF TALENT:129
	LOCAL = 75
ELSE
	LOCAL = 150
ENDIF
SIF EXP:0 < LOCAL
	RETURN 0
;助手にも일정의 Ｖ경험이 필요([巨躯]の場合は軽減)
IF TALENT:ASSI:129
	LOCAL = 75
ELSE
	LOCAL = 150
ENDIF
SIF EXP:ASSI:0 < LOCAL
	RETURN 0
;Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20 || TEQUIP:21
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;助手コスプレ中はダメ
SIF TEQUIP:ASSI:59
	RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;대상과 조수의 양쪽 모두의 V에 주인이 피스트 성교!
@COM312
PRINTL 더블 V 피스트
TSTR:0 = 더블 V 피스트
CALL TRAIN_MESSAGE_COM312

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 550
DOWNBASE:1 += 500
DOWNBASE:ASSI:0 += 550
DOWNBASE:ASSI:1 += 200


SOURCE:20 = 1200
SOURCE:12 = 1200
SOURCE:22 = 1200
SOURCE:31 = 2000
SOURCE:17 = 1200

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 2000
ELSEIF ABL:1 == 1
	SOURCE:1 = 2500
ELSEIF ABL:1 == 2
	SOURCE:1 = 3000
ELSEIF ABL:1 == 3
	SOURCE:1 = 3300
ELSEIF ABL:1 == 4
	SOURCE:1 = 3600
ELSE
	SOURCE:1 = 3800
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:20 += 20000
	SOURCE:18 += 2000
	SIF TALENT:PLAYER:120 == 0
		TCVAR:50 += 1
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:20 += 300
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:20 += 50
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:20 += 10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:20 += 0
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:20 += 0
ENDIF

;ABL:욕망을 본다
IF ABL:11 == 0
	SOURCE:19 = 100
	SOURCE:22 += 3200
ELSEIF ABL:11 == 1
	SOURCE:19 = 200
	SOURCE:22 += 3800
ELSEIF ABL:11 == 2
	SOURCE:19 = 350
	SOURCE:22 += 4600
ELSEIF ABL:11 == 3
	SOURCE:19 = 600
	SOURCE:22 += 5400
ELSEIF ABL:11 == 4
	SOURCE:19 = 850
	SOURCE:22 += 6200
ELSE
	SOURCE:19 = 1200
	SOURCE:22 += 7000
ENDIF

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	TIMES SOURCE:19 , 0.50
ELSEIF ABL:13 == 1
	TIMES SOURCE:19 , 0.80
ELSEIF ABL:13 == 2
	TIMES SOURCE:19 , 1.00
ELSEIF ABL:13 == 3
	TIMES SOURCE:19 , 1.50
ELSEIF ABL:13 == 4
	TIMES SOURCE:19 , 2.00
ELSE
	TIMES SOURCE:19 , 2.50
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

SIF EXP:32 == 0
	TCVAR:50 += 1
TCVAR:0 += 25
TCVAR:32 += 1

SIF TALENT:120 == 0 && TALENT:MASTER:120 == 0
	TCVAR:12 += 1

SIF EXP:ASSI:32 == 0
	TCVAR:ASSI:50 += 1
TCVAR:ASSI:0 += 25
TCVAR:ASSI:32 += 1

SIF TALENT:ASSI:120 == 0 && TALENT:MASTER:120 == 0
	TCVAR:ASSI:12 += 1

RETURN 1

