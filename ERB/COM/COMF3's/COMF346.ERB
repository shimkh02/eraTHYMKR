;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM346
PRINTL 밖에서 보낸다
TSTR:0 = 밖에서 보낸다
CALL TRAIN_MESSAGE_COM346

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 0
DOWNBASE:1 += 30

SOURCE:31 = 100
SOURCE:12 = 120

;봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:11 = 10
ELSEIF ABL:13 == 1
	SOURCE:11 = 30
ELSEIF ABL:13 == 2
	SOURCE:11 = 80
ELSEIF ABL:13 == 3
	SOURCE:11 = 150
ELSEIF ABL:13 == 4
	SOURCE:11 = 230
ELSE
	SOURCE:11 = 200 + (ABL:13 * 20)
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES SOURCE:11 , 0.80
	TIMES SOURCE:12 , 0.80
	SOURCE:19 = 10
	SOURCE:18 = 10
ELSEIF ABL:21 == 1
	TIMES SOURCE:11 , 1.00
	TIMES SOURCE:12 , 1.00
	SOURCE:19 = 40
	SOURCE:18 = 60
ELSEIF ABL:21 == 2
	TIMES SOURCE:11 , 1.30
	TIMES SOURCE:12 , 1.20
	SOURCE:19 = 80
	SOURCE:18 = 120
ELSEIF ABL:21 == 3
	TIMES SOURCE:11 , 1.40
	TIMES SOURCE:12 , 1.40
	SOURCE:19 = 140
	SOURCE:18 = 240
ELSEIF ABL:21 == 4
	TIMES SOURCE:11 , 1.70
	TIMES SOURCE:12 , 1.50
	SOURCE:19 = 220
	SOURCE:18 = 360
ELSE
	SOURCE:11 = SOURCE:11 * (ABL:21 * 15 + 125) / 100
	SOURCE:12 = SOURCE:12 * (ABL:21 * 10 + 120) / 100
	SOURCE:19 = 100 + (ABL:21 * 20)
	SOURCE:18 = 150 + (ABL:21 * 35)
ENDIF

RETURN 1

