@COM_ABLE372
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:372 > 0
	RETURN 0
;特殊風呂系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 1024) == 0
	RETURN 0
;風呂を改装していないとダメ
SIF (FLAG:74 & 4096) == 0
	RETURN 0
;一度も厄毒風呂に入ったことが無く, さらに異常경험이 5미만의 場合
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && (CFLAG:23 & 32) == 0 && EXP:50 < 5
	RETURN 0
;순종5미만의 ASSI일 경우, 実行できない(새드거나 새드끼Lv3이상か狂気, 또는 망신이나 복종이 있다면 ＯＫ)
IF ASSIPLAY
	SIF ABL:ASSI:10 < 5 && TALENT:ASSI:81 == 0 && ABL:ASSI:20 < 3 && TALENT:ASSI:158 == 0 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
ENDIF
;해제는 언제든 가능
SIF TEQUIP:55 == 14
	RETURN 1
;기계면 불가
SIF TALENT:206
	RETURN 0
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;전극사용 중에는 불가
SIF TEQUIP:20 == 4 || TEQUIP:25 == 4 || TEQUIP:30 == 4 || TEQUIP:30 == 6 || TEQUIP:32 == 4 || TEQUIP:35 == 4
	RETURN 0
;거미집에 걸려 있을 때는 불가
SIF TEQUIP:40 == 7
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;야외면 안 됨
SIF TEQUIP:52
	RETURN 0
;욕실 플레이中じゃ無いとダメ
SIF TEQUIP:53 != 1
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;그밖의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 14
	RETURN 0
;의사 선생님 플레이 중에는 불가
SIF TEQUIP:57
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;코스프레 중에는 불가
SIF TEQUIP:59 > 2
	RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;액이 찬 액체에 잠기게 해 있으므로 불쾌가 대량으로 들어간다
@COM372
PRINTL 액독목욕탕
TSTR:0 = 액독목욕탕
CALL TRAIN_MESSAGE_COM372

IF TEQUIP:55 == 14
	TEQUIP:55 = 0
	RETURN 1
ENDIF

;약에 들어 있는 독 내성이라면 소모가 적은
DOWNBASE:0 += (TALENT:44) ?  30 # 60
DOWNBASE:1 += (TALENT:44) ?  50 # 100

SOURCE:20 = 900
SOURCE:19 = 2000
SOURCE:21 = 1800

LOCAL:1 = 1200
LOCAL:2 = 8000
LOCAL:3 = 1800

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL:1 , 0.50
	TIMES LOCAL:2 , 2.75
	TIMES LOCAL:3 , 0.60
ELSEIF ABL:10 == 1
	TIMES LOCAL:1 , 0.75
	TIMES LOCAL:2 , 1.80
	TIMES LOCAL:3 , 0.80
ELSEIF ABL:10 == 2
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 1.00
	TIMES LOCAL:3 , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 0.80
	TIMES LOCAL:3 , 1.20
ELSEIF ABL:10 == 4
	TIMES LOCAL:1 , 2.00
	TIMES LOCAL:2 , 0.50
	TIMES LOCAL:3 , 1.50
ELSE
	TIMES LOCAL:1 , 2.50
	TIMES LOCAL:2 , 0.20
	TIMES LOCAL:3 , 1.80
ENDIF

;ABL:자위 중독, 정액 중독, 분유중독, 촉수 중독의 합계를 본다
LOCAL:4 = ABL:30 + ABL:31 + ABL:34 + ABL:35
IF LOCAL:4 == 0
	TIMES LOCAL:1 , 0.30
	TIMES LOCAL:2 , 2.00
	TIMES LOCAL:3 , 0.20
ELSEIF LOCAL:4 < 5
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 1.50
	TIMES LOCAL:3 , 0.50
ELSEIF LOCAL:4 < 10
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 1.00
	TIMES LOCAL:3 , 1.00
ELSEIF LOCAL:4 < 15
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 0.70
	TIMES LOCAL:3 , 1.20
ELSEIF LOCAL:4 < 20
	TIMES LOCAL:1 , 2.00
	TIMES LOCAL:2 , 0.50
	TIMES LOCAL:3 , 1.60
ELSE
	TIMES LOCAL:1 , 2.50
	TIMES LOCAL:2 , 0.20
	TIMES LOCAL:3 , 1.80
ENDIF

;약에 들어 있는 독 내성
IF TALENT:44
	TIMES LOCAL:1 , 0.20
	TIMES LOCAL:2 , 0.40
	TIMES LOCAL:3 , 0.30
ENDIF
;도착적
IF TALENT:80
	TIMES LOCAL:1 , 1.30
	TIMES LOCAL:2 , 0.80
	TIMES LOCAL:3 , 1.20
ENDIF
;마조
IF TALENT:82
	TIMES LOCAL:1 , 1.30
	TIMES LOCAL:2 , 0.50
	TIMES LOCAL:3 , 1.50
ENDIF

;EXP:이상 경험을 본다
IF EXP:50 < EXPLV:1
	TIMES LOCAL:3 , 2.50
	SOURCE:25 += 250
ELSEIF EXP:50 < EXPLV:2
	TIMES LOCAL:3 , 1.80
	SOURCE:25 += 100
ELSEIF EXP:50 < EXPLV:3
	TIMES LOCAL:3 , 1.00
	SOURCE:25 += 50
ELSEIF EXP:50 < EXPLV:4
	TIMES LOCAL:3 , 0.50
ELSEIF EXP:50 < EXPLV:5
	TIMES LOCAL:3 , 0.30
ELSE
	TIMES LOCAL:3 , 0.10
ENDIF
TEQUIP:55 = 14

SOURCE:24 = LOCAL:1
SOURCE:30 = LOCAL:2
SOURCE:23 = LOCAL:3

IF (CFLAG:23 & 32) == 0 && EXP:50 < 5
	TCVAR:50 += 1
	CFLAG:23 |= 32
ENDIF
RETURN 1

