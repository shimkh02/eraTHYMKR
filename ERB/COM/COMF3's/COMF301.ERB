	SOURCE:15 = 700
	SOURCE:16 = 600
	TIMES SOURCE:12 , 1.20
ELSE
	SOURCE:0 = 1750
	SOURCE:15 = 2000
	SOURCE:16 = 1000
	TIMES SOURCE:12 , 1.30
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:15 , 0.70
	TIMES SOURCE:16 , 0.75
	TIMES SOURCE:22 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.90
	TIMES SOURCE:22 , 0.60
ELSEIF ABL:12 == 2
