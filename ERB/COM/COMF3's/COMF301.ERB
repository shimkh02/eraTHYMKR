	CALL JUDGE_TRAIN_PAIR, TARGET, ASSI
	IF TFLAG:175 > 0
		CALL EXCOM_ABLE629
		SIF RESULT == 1
			JUMP COM629
	ENDIF
	TFLAG:157 = 0
ENDIF

