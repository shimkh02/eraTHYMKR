ELSEIF ABL:10 == 2
	SOURCE:18 = 60
ELSEIF ABL:10 == 3
	SOURCE:18 = 20
ELSEIF ABL:10 == 4
	SOURCE:18 = 0
ELSE
	SOURCE:18 = 0
ENDIF

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 20
	SOURCE:15 = 0
	SOURCE:16 = 0
	TIMES SOURCE:12 , 0.80
ELSEIF ABL:0 == 1
	SOURCE:0 = 80
	SOURCE:15 = 10
	SOURCE:16 = 50
	TIMES SOURCE:12 , 0.90
ELSEIF ABL:0 == 2
	SOURCE:0 = 350
	SOURCE:15 = 50
	SOURCE:16 = 100
	TIMES SOURCE:12 , 1.00
ELSEIF ABL:0 == 3
	SOURCE:0 = 750
