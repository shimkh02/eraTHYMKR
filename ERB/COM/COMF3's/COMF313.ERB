;=============================================================================
;더블 A 피스트
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE313
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:313 > 0
	RETURN 0
;조수가 없으면 불가
SIF ASSI < 0
	RETURN 0
;主人じゃないとダメ
SIF ASSIPLAY
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;작은 체형なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
	SIF ABL:10 < 4 || ABL:21 < 4
		RETURN 0
ENDIF
;助手も작은 체형なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:ASSI:128
	SIF ABL:ASSI:10 < 4 || ABL:ASSI:21 < 4
		RETURN 0
ENDIF
;조교 대상か조수가 소인체형일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF (TALENT:127 || TALENT:ASSI:127) && TALENT:MASTER:56 == 0
	RETURN 0
;주인이 새드か狂気じゃないとできない。
SIF TALENT:MASTER:81 == 0 && TALENT:MASTER:158 == 0
	RETURN 0
;일정의 Ａ경험이 필요([巨躯]の場合は軽減)
IF TALENT:129
	LOCAL = 75
ELSE
	LOCAL = 150
ENDIF
SIF EXP:1 < LOCAL
	RETURN 0
;助手にも일정의 Ａ경험이 필요([巨躯]の場合は軽減)
IF TALENT:ASSI:129
	LOCAL = 75
ELSE
	LOCAL = 150
ENDIF
SIF EXP:ASSI:1 < LOCAL
	RETURN 0
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
	RETURN 0
;쿠스코 채웠으면 불가
SIF TEQUIP:21
	RETURN 0
;Ａ계 장착구 사용 중에는 불가
SIF TEQUIP:25 || TEQUIP:26
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;助手コスプレ中はダメ
SIF TEQUIP:ASSI:59
	RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM313
IF (NO:PLAYER == 14 && CFLAG:PLAYER:0 == 0) || (TALENT:208 && TALENT:ASSI:208)
	TSTR:0 = 퍼펫 머펫
ELSE
	TSTR:0 = 더블 A 피스트
ENDIF
PRINTFORML %TSTR:0% 
CALL TRAIN_MESSAGE_COM313

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 550
DOWNBASE:1 += 550
DOWNBASE:ASSI:0 += 550
DOWNBASE:ASSI:1 += 250

SOURCE:20 = 1200
SOURCE:12 = 1200
SOURCE:22 = 1200
SOURCE:31 = 2000
SOURCE:17 = 1200

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 10
	SOURCE:11 = 10
	SOURCE:22 += 100
ELSEIF ABL:2 == 1
	SOURCE:2 = 30
	SOURCE:11 = 30
	SOURCE:22 += 700
ELSEIF ABL:2 == 2
	SOURCE:2 = 500
	SOURCE:11 = 100
	SOURCE:22 += 1500
ELSEIF ABL:2 == 3
	SOURCE:2 = 1000
	SOURCE:11 = 200
	SOURCE:22 += 3000
ELSEIF ABL:2 == 4
	SOURCE:2 = 1700
	SOURCE:11 = 450
	SOURCE:22 += 5000
ELSE
	SOURCE:2 = 2200
	SOURCE:11 = 750
	SOURCE:22 += 8000
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:20 += 20000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.30
	SOURCE:20 += 12000
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:20 += 5000
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 1800
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 1000
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:20 += 600
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 10000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 3600
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 1200
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 200
ELSE
	TIMES SOURCE:2 , 1.80
	SOURCE:20 += 100
ENDIF

;ABL:욕망을 본다
IF ABL:11 == 0
	SOURCE:19 = 100
	SOURCE:22 += 3200
ELSEIF ABL:11 == 1
	SOURCE:19 = 200
	SOURCE:22 += 3800
ELSEIF ABL:11 == 2
	SOURCE:19 = 350
	SOURCE:22 += 4600
ELSEIF ABL:11 == 3
	SOURCE:19 = 600
	SOURCE:22 += 5400
ELSEIF ABL:11 == 4
	SOURCE:19 = 850
	SOURCE:22 += 6200
ELSE
	SOURCE:19 = 1200
	SOURCE:22 += 7000
ENDIF


;ABL:봉사 정신을 본다
IF ABL:13 == 0
	TIMES SOURCE:19 , 0.50
ELSEIF ABL:13 == 1
	TIMES SOURCE:19 , 0.80
ELSEIF ABL:13 == 2
	TIMES SOURCE:19 , 1.00
ELSEIF ABL:13 == 3
	TIMES SOURCE:19 , 1.50
ELSEIF ABL:13 == 4
	TIMES SOURCE:19 , 2.00
ELSE
	TIMES SOURCE:19 , 2.50
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:22 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:22 , 1.20
ELSE
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:22 , 1.40
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3

SIF EXP:33 == 0
	TCVAR:50 += 1
TCVAR:1 += 25
TCVAR:33 += 1

SIF EXP:ASSI:33 == 0
	TCVAR:ASSI:50 += 1
TCVAR:ASSI:1 += 25
TCVAR:ASSI:33 += 1

RETURN 1

