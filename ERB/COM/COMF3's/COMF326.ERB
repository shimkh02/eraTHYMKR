@COM_ABLE326
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:326 > 0
	RETURN 0
;조교자奉仕 · ウフフ系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 2048) == 0
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;조교 대상이 オトコだとダメ
SIF TALENT:120
	RETURN 0
;윤활 부족이고, 순종4미만 · 레즈끼4미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(새드거나 새드끼Lv3이상か狂気, 또는 망신이나 복종이 있다면 ＯＫ)
IF PALAM:10 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:10 < 4 || (TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:22 < 4)) && TALENT:ASSI:81 == 0 && ABL:ASSI:20 < 3 && TALENT:ASSI:158 == 0 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
ENDIF
;ペニスバンドが無いとダメ
SIF ITEM:2 == 0 && NOITEM == 0
	RETURN 0
;Ｖ系装着도구 사용 중에는 불가
SIF TEQUIP:20 || TEQUIP:21
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;조수가 훈도시 착용 중일 경우 , 奴隷と助手の기교4이상 필수
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 == 17 && (ABL:ASSI:12 < 4 || ABL:12 < 4)
	RETURN 0
;훈도시 착용 중일 경우 조교자와 奴隷の쌍방이 기교4이상 필수
SIF TEQUIP:59 == 17 && (ABL:PLAYER:12 < 4 || ABL:12 < 4)
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM326
;쿠스코 삽입중 또는 전회의 조교가 G스포트 또는 손가락 삽입또한 플레이어의 기교 3이상이라면 G스포트 자극에
IF TEQUIP:21 || PREVCOM == 4 && ABL:PLAYER:12 >= 3 || PREVCOM == 600
	CALL EXCOM_ABLE600
	SIF RESULT == 1
		JUMP COM600
ENDIF

PRINTL 페니스밴드 삽입
TSTR:0 = 페니스밴드 삽입
CALL TRAIN_MESSAGE_COM326

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 60
DOWNBASE:1 += 120

SOURCE:11 = 100
SOURCE:12 = 800
SOURCE:31 = 500

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 30
	SOURCE:22 = 200
ELSEIF ABL:1 == 1
	SOURCE:1 = 120
	SOURCE:22 = 350
ELSEIF ABL:1 == 2
	SOURCE:1 = 350
	SOURCE:22 = 550
ELSEIF ABL:1 == 3
	SOURCE:1 = 700
	SOURCE:22 = 900
ELSEIF ABL:1 == 4
	SOURCE:1 = 1300
	SOURCE:22 = 1500
ELSE
	SOURCE:1 = 2000
	SOURCE:22 = 2300
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	TIMES SOURCE:22, 0.20
	SOURCE:20 = 300
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:22, 0.50
	SOURCE:20 = 180
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:22, 0.80
	SOURCE:20 = 80
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:22, 1.00
	SOURCE:20 = 30
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:22, 1.20
	SOURCE:20 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:22, 1.50
	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:20 += 700
	TIMES SOURCE:20 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:1 , 0.20
	SOURCE:20 += 200
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:20 , 0.80
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:20 , 0.50
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 0.50
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 1.20
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 1.50
ELSE
	TIMES SOURCE:1 , 1.80
ENDIF

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:103
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:102
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	TIMES SOURCE:22 , 2.00

TCVAR:0 += 1

;노예의 V⇔조교자의 입의 불결이 이동
STAIN:3 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:3

SIF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 7

RETURN 1

