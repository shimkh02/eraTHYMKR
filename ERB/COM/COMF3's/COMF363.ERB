	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;전극사용 중에는 불가
SIF TEQUIP:20 == 4 || TEQUIP:25 == 4 || TEQUIP:30 == 4 || TEQUIP:30 == 6 || TEQUIP:32 == 4 || TEQUIP:35 == 4
	RETURN 0
;거미집에 걸려 있을 때는 불가
SIF TEQUIP:40 == 7
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;야외면 안 됨
SIF TEQUIP:52
	RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
	RETURN 0
;그밖의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 4
	RETURN 0
;의사 선생님 플레이 중에는 불가
SIF TEQUIP:57
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;코스프레 중에는 불가
SIF TEQUIP:59 > 2
	RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;윤활과 슬라임의 점액에 의해 아픔은 거의 없는＋V경험을 주는 일 없이, 처녀막도 손상시키는 일 없이 쾌V의 주가 들어간다
;반면, 일탈과 불결의 상승수가 높은＋이상 경험이 적으면 반발 필수의 디메리트도 존재
;또, 조교 개시부터 V경험을 1개에서도 넣고 있을까 V감각이 1이상의 경우는 V절정과 쾌V의 값이 일정치를 넘으면(자) 처녀막을 찢어지게 된다
@COM363
PRINTL 슬라임 목욕탕
TSTR:0 = 슬라임 목욕탕

LOCAL:2 = 1
LOCAL:3 = 1
;[남자]
SIF TALENT:120
	LOCAL:2 = 0
;바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:20 || TEQUIP:21
	LOCAL:2 = 0
;애널 바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:25 || TEQUIP:26
	LOCAL:3 = 0

CALL TRAIN_MESSAGE_COM363

IF TEQUIP:55 == 4
	TEQUIP:55 = 0
	TFLAG:155 = 0
ELSE
	TEQUIP:55 = 4
	SOURCE:30 = 15000
	SOURCE:19 = 15000
	SOURCE:31 = 15000
	IF LOCAL:2
		;ABL:V감각을 본다
		IF ABL:1 == 0
			SOURCE:1 = 20
		ELSEIF ABL:1 == 1
			SOURCE:1 = 60
		ELSEIF ABL:1 == 2
			SOURCE:1 = 180
		ELSEIF ABL:1 == 3
			SOURCE:1 = 360
		ELSEIF ABL:1 == 4
			SOURCE:1 = 600
		ELSE
			SOURCE:1 = 900
		ENDIF
		SOURCE:20 += 300
		SOURCE:30 += 3000
		SOURCE:31 += 5000
	ENDIF
	IF LOCAL:3
		;ABL:A감각을 본다
		IF ABL:2 == 0
			SOURCE:2 = 40
			SOURCE:22 = 500
		ELSEIF ABL:2 == 1
			SOURCE:2 = 100
			SOURCE:22 = 600
		ELSEIF ABL:2 == 2
			SOURCE:2 = 200
			SOURCE:22 = 800
		ELSEIF ABL:2 == 3
			SOURCE:2 = 500
			SOURCE:22 = 1100
		ELSEIF ABL:2 == 4
			SOURCE:2 = 800
			SOURCE:22 = 1500
		ELSE
			SOURCE:2 = 1200
			SOURCE:22 = 2000
		ENDIF
		SOURCE:20 += 500
		SOURCE:30 += 4000
		SOURCE:31 += 5000
	ENDIF
	IF LOCAL:2 || LOCAL:3
		;PALAM:윤활을 본다
		IF PALAM:10 < PALAMLV:3
			TIMES SOURCE:1 , 1.00
			TIMES SOURCE:2 , 1.00
			SOURCE:30 += 1200
		ELSEIF PALAM:10 < PALAMLV:4
			TIMES SOURCE:1 , 1.20
			TIMES SOURCE:2 , 1.20
			SOURCE:30 += 1700
		ELSE
			TIMES SOURCE:1 , 1.40
			TIMES SOURCE:2 , 1.40
			SOURCE:30 += 2200
		ENDIF
	ENDIF
	LOCAL:1 = 100
	;EXP:이상 경험을 본다
	IF EXP:50 < EXPLV:1
		TIMES LOCAL:1 , 3.00
	ELSEIF EXP:50 < EXPLV:2
		TIMES LOCAL:1 , 2.50
	ELSEIF EXP:50 < EXPLV:3
		TIMES LOCAL:1 , 2.00
	ELSEIF EXP:50 < EXPLV:4
		TIMES LOCAL:1 , 1.00
	ELSEIF EXP:50 < EXPLV:5
		TIMES LOCAL:1 , 0.80
	ELSE
		TIMES LOCAL:1 , 0.60
	ENDIF
	;ABL:C감각을 본다
	IF ABL:0 == 0
		SOURCE:0 = 50
	ELSEIF ABL:0 == 1
		SOURCE:0 = 150
	ELSEIF ABL:0 == 2
		SOURCE:0 = 800
	ELSEIF ABL:0 == 3
		SOURCE:0 = 1400
	ELSEIF ABL:0 == 4
		SOURCE:0 = 2000
	ELSE
		SOURCE:0 = 2700
	ENDIF
	;ABL:B감각을 본다
	IF ABL:3 == 0
		SOURCE:3 = 40
	ELSEIF ABL:3 == 1
		SOURCE:3 = 100
	ELSEIF ABL:3 == 2
		SOURCE:3 = 250
	ELSEIF ABL:3 == 3
		SOURCE:3 = 800
	ELSEIF ABL:3 == 4
		SOURCE:3 = 1500
	ELSE
		SOURCE:3 = 2300
	ENDIF
	;감정결여
	SIF TALENT:22
		TIMES LOCAL:1 , 0.60
	;작은 몸집 체형
	SIF TALENT:128
		TIMES SOURCE:20 , 1.20

	SOURCE:25 += LOCAL:1 * 8

	;겁쟁이
	SIF TALENT:10
		TIMES LOCAL:1 , 2.00

	DOWNBASE:0 += LOCAL:1 / 4
	DOWNBASE:1 += LOCAL:1 / 3
	SOURCE:31 += LOCAL:1 * 3

	IF LOCAL:3
		TFLAG:155 |= 2
		STAIN:4 = 8
	ENDIF
	IF LOCAL:2
		TFLAG:155 |= 1
		STAIN:3 = 1
	ENDIF
	STAIN:1 = 0
	STAIN:2 = 2
	IF EXP:50 < 3
		;이상 경험이 적으면 반발 각인이 붙지만 그 처리는 TRACHECK.ERB에 맡긴다. 여기에서는 플래그만 세워 둔다.
		IF MARK:9 <= 0 && MARK:1 <= 1
			TFLAG:84 = 1
		ELSEIF MARK:9 <= 1 && TALENT:150 == 0 && TALENT:160 == 0 && TALENT:164 == 0 && TALENT:170 == 0 && (MARK:1 <= 2 || EXP:50 < 5)
			TFLAG:84 = 2
		ELSEIF MARK:9 <= 2 && TALENT:0 == 1 && TALENT:0 && TALENT:150 == 0 && TALENT:160 == 0 && TALENT:164 == 0 && TALENT:170 == 0 && EXP:50 < 7
			TFLAG:84 = 3
		ENDIF
	ENDIF
	IF (CFLAG:23 & 4) == 0
		TCVAR:50 += 1
		CFLAG:23 |= 4
	ENDIF
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;--------------------------------------------------
;슬라임 목욕탕 입욕중
;--------------------------------------------------
@EQUIP_COM363
PRINTL <슬라임 목욕탕 입욕 중>

LOCAL:3 = 1
LOCAL:4 = 1
;[남자]
SIF TALENT:120
	LOCAL:3 = 0
;바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:20 || TEQUIP:21
	LOCAL:3 = 0
;애널 바이브래이터등의 기구를 삽입하고 있다
SIF TEQUIP:25 || TEQUIP:26
	LOCAL:4 = 0

IF LOCAL:3 || TFLAG:155 & 1
	IF TALENT:0 && (ABL:1 || CFLAG:11 & 1) && PALAM:1 >= 6000 && EX:1 > 9
		SIF TALENT:0 == 1
			CFLAG:500 = 1010
		TALENT:0 = 0
		SOURCE:20 += 1000
		PRINTFORMW 자궁에 들어오지 못했던 슬라임들은, 기세가 지나쳐 %CALLNAME:TARGET%의 처녀막을 찢어 버렸다….
		CALL SETCOLOR_TORIKOMODE, "RED", 3
		PRINTW 【처녀 상실】
		RESETCOLOR
	ENDIF
	;ABL:V감각을 본다
	IF ABL:1 == 0
		LOCAL:1 = 10
	ELSEIF ABL:1 == 1
		LOCAL:1 = 30
	ELSEIF ABL:1 == 2
		LOCAL:1 = 60
	ELSEIF ABL:1 == 3
		LOCAL:1 = 120
	ELSEIF ABL:1 == 4
		LOCAL:1 = 240
	ELSE
		LOCAL:1 = 480
	ENDIF
	
	SIF TFLAG:169 == 1 && LOCAL:3 == 0
		TIMES LOCAL:1 , 0.80
	
	SOURCE:1 += LOCAL:1
ENDIF
IF LOCAL:4 || TFLAG:155 & 2
	;ABL:A감각을 본다
	IF ABL:2 == 0
		LOCAL:2 = 15
	ELSEIF ABL:2 == 1
		LOCAL:2 = 45
	ELSEIF ABL:2 == 2
		LOCAL:2 = 90
	ELSEIF ABL:2 == 3
		LOCAL:2 = 180
	ELSEIF ABL:2 == 4
		LOCAL:2 = 260
	ELSE
		LOCAL:2 = 520
	ENDIF
	
	SIF TFLAG:170 == 1 && LOCAL:4 == 0
		TIMES LOCAL:2 , 0.80
	
	SOURCE:2 += LOCAL:2
	SOURCE:22 += LOCAL:2
ENDIF

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 += 25
ELSEIF ABL:0 == 1
	SOURCE:0 += 50
ELSEIF ABL:0 == 2
	SOURCE:0 += 150
ELSEIF ABL:0 == 3
	SOURCE:0 += 300
ELSEIF ABL:0 == 4
	SOURCE:0 += 600
ELSE
	SOURCE:0 += 900
ENDIF
;ABL:B감각을 본다
IF ABL:3 == 0
	SOURCE:3 += 20
ELSEIF ABL:3 == 1
	SOURCE:3 += 40
ELSEIF ABL:3 == 2
	SOURCE:3 += 80
ELSEIF ABL:3 == 3
	SOURCE:3 += 160
ELSEIF ABL:3 == 4
	SOURCE:3 += 320
ELSE
	SOURCE:3 += 640
ENDIF

LOCAL = 100
;EXP:이상 경험을 본다
IF EXP:50 < EXPLV:1
	TIMES LOCAL , 3.00
ELSEIF EXP:50 < EXPLV:2
	TIMES LOCAL , 2.50
ELSEIF EXP:50 < EXPLV:3
	TIMES LOCAL , 2.00
ELSEIF EXP:50 < EXPLV:4
	TIMES LOCAL , 1.00
ELSEIF EXP:50 < EXPLV:5
	TIMES LOCAL , 0.80
ELSE
	TIMES LOCAL , 0.60
ENDIF
;감정결여
SIF TALENT:22
	TIMES LOCAL , 0.60
;소인 체형
SIF TALENT:127
	TIMES LOCAL , 1.80
;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL , 1.20

SOURCE:25 += LOCAL * 5

;겁쟁이
SIF TALENT:10
	TIMES LOCAL , 2.00

DOWNBASE:0 += LOCAL / 2
DOWNBASE:1 += LOCAL / 3

SOURCE:30 += LOCAL * 5
SOURCE:19 += 1000
SOURCE:31 += LOCAL * 2

STAIN:1 = 0
STAIN:2 = 2

TIMES SOURCE:20 , 1.20

RETURN 1

