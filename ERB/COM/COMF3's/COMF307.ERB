;=============================================================================
;조수를 범하게 한다
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE307
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:307 > 0
	RETURN 0
;助手じゃないとダメ
SIF PLAYER != ASSI
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;조교자가 天然처녀であり, 조교 대상이 まだ一度も助手を犯させるを行ったことが無い場合, 
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && TALENT:PLAYER:0 == 1 && (CFLAG:21 & 8) == 0
	RETURN 0
;조교 대상にペニスがない, かつペニスバンドもないとダメ
SIF EXIST_BAR(TARGET) == 0 && ITEM:2 == 0 && NOITEM == 0
	RETURN 0
;조수가 オトコだとダメ
SIF TALENT:PLAYER:120
	RETURN 0
;주인이 [금단의 지식]持ちじゃないとき, 体格差が2段階以内か조교자가 [금단의 지식]持ちじゃないとダメ
SIF TALENT:MASTER:56 == 0 && COMMON_BODYSIZE_CHECK(PLAYER, TARGET) == 0
	RETURN 0
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;오나홀 · 전극 오나홀사용 중에는 불가
SIF TEQUIP:30 == 2 || TEQUIP:30 == 6
	RETURN 0
;거미집에 걸려 있을 때는 불가
SIF TEQUIP:40 == 7
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
	RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;조수가 훈도시 착용 중일 경우 , 助手の기교4이상 필수
SIF ASSI >= 0 && TEQUIP:ASSI:59 == 17 && ABL:ASSI:12 < 4
	RETURN 0
;훈도시 착용 중일 경우 기교4이상 필수
SIF TEQUIP:59 == 17 && ABL:12 < 4
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM307
PRINTL 조수를 범하게 한다

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
IF TFLAG:699 == 0
	CALL COM_ORDER
	;실행할 수 없다
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
TSTR:0 = 조수를 범하게 한다
CALL TRAIN_MESSAGE_COM307

;-------------------------------------------------
;사정 게이지 체크(후타나리의 경우)
;-------------------------------------------------
LOCAL = 0

;ABL:기교를 본다
IF ABL:12 == 0
	LOCAL = 50
ELSEIF ABL:12 == 1
	LOCAL = 300
ELSEIF ABL:12 == 2
	LOCAL = 800
ELSEIF ABL:12 == 3
	LOCAL = 1500
ELSEIF ABL:12 == 4
	LOCAL = 2000
ELSE
	LOCAL = 3200
ENDIF

;순종을 본다
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;봉사 정신을 본다
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;조수의 V감각을 본다
LOCAL = EJACULATIONCHECK_ABL_1_1(ASSI, LOCAL)
;조수의 V경험을 본다
LOCAL = EJACULATIONCHECK_EXP_0_1(ASSI, LOCAL)

;조교 대상의 C감각을 본다
LOCAL = EJACULATIONCHECK_ABL_0_1(TARGET, LOCAL)

SIF TALENT:120 || TALENT:121
	BASE:2 += LOCAL

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 40
DOWNBASE:1 += 220

SOURCE:22 = 1500
SOURCE:31 = 800

;ABL:봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:0 = 800
	SOURCE:15 = 3200
	SOURCE:16 = 400
ELSEIF ABL:13 == 1
	SOURCE:0 = 1400
	SOURCE:15 = 3800
	SOURCE:16 = 800
ELSEIF ABL:13 == 2
	SOURCE:0 = 2000
	SOURCE:15 = 4600
	SOURCE:16 = 1500
ELSEIF ABL:13 == 3
	SOURCE:0 = 2500
	SOURCE:15 = 5400
	SOURCE:16 = 2300
ELSEIF ABL:13 == 4
	SOURCE:0 = 2900
	SOURCE:15 = 6200
	SOURCE:16 = 3500
ELSE
	SOURCE:0 = 3200
	SOURCE:15 = 7000
	SOURCE:16 = 5000
ENDIF

;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:16 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 0.80
	TIMES SOURCE:16 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:16 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
ELSEIF ABL:12 == 4
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 2.50
	TIMES SOURCE:16 , 2.50
ELSE
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:15 , 4.00
	TIMES SOURCE:16 , 4.00
ENDIF

;조수가 처녀
IF TALENT:PLAYER:0
	PRINTL <조수가 처녀>
	CALL LOST_VIRGIN, TARGET, PLAYER, ARG
	IF TALENT:PLAYER:0 == 1
		TIMES SOURCE:22 , 20.00
		TIMES SOURCE:31 , 3.00
		IF (CFLAG:21 & 8) == 0
			TCVAR:50 += 1
			CFLAG:21 |= 8
		ENDIF
		IF ABL:10 < 3
			PRINTFORML 그리고 %ABLNAME:10%이 3이 되었다.
			ABL:10 = 3
		ENDIF
	ELSE
		TIMES SOURCE:22 , 15.00
		TIMES SOURCE:31 , 1.80
	ENDIF
	TFLAG:5 += TALENT:PLAYER:0
	TALENT:PLAYER:0 = 0
	STAIN:PLAYER:3 |= 32
ENDIF
TCVAR:PLAYER:0 += 1

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(TARGET)

;사정시의 처리
IF TFLAG:91 >= 1
	;커맨드 마다 고유의 처리는 이 아래에 쓴다
	TIMES SOURCE:15 , 3.00

	;ABL:정액 중독을 본다
	IF ABL:31 == 0
		SOURCE:24 = 0
		TIMES SOURCE:16 , 2.00
		TIMES SOURCE:22, 2.00
	ELSEIF ABL:31 == 1
		SOURCE:24 = 50
		TIMES SOURCE:16 , 2.50
		TIMES SOURCE:22, 1.60
	ELSEIF ABL:31 == 2
		SOURCE:24 = 150
		TIMES SOURCE:16 , 3.00
		TIMES SOURCE:22, 1.00
	ELSEIF ABL:31 == 3
		SOURCE:24 = 300
		TIMES SOURCE:16 , 4.50
		TIMES SOURCE:22, 0.70
	ELSEIF ABL:31 == 4
		SOURCE:24 = 600
		TIMES SOURCE:16 , 6.00
		TIMES SOURCE:22, 0.40
	ELSE
		SOURCE:24 = 1200
		TIMES SOURCE:16 , 8.00
		TIMES SOURCE:22, 0.10
	ENDIF

	;사정처 타입의 설정
	TFLAG:703 = 8
	;사정처 선택의 가능·불가
	TFLAG:704 = 0
	;노예 사정 판정이 끝난 플래그
	TFLAG:705 = 1
	;범용적인 처리는 이 함수로 실시한다
	;(사정 게이지의 재계산이나 사정 경험의 상승과 공통 부분의 표시)
	CALL SAMEN_SHOOT
ENDIF

;사정에 관계없이 행해지는 처리는 여기로부터
IF TALENT:1 || TALENT:120 || TALENT:121
	;노예의 P⇔조교자의 V의 불결이 이동
	STAIN:2 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:2
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 10
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 10
ENDIF
;봉사 쾌락 경험 입수 판정
TFLAG:100 |= 1

;처녀 상실의 후에 페니스를 뽑고 있을까
TFLAG:101 = (TFLAG:101 == 2) ?  1 # 0
RETURN 1

;--------------------------------------------------
;명령 거부의 메시지
;--------------------------------------------------
@DENIAL_MESSAGE_COM_307
CALL KOJO_DENIAL_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTFORML %CALLNAME:TARGET%에게 범해지라고 %CALLNAME:ASSI%에게 명령했으나 거절당했다.
PRINTW 좀 더 조교가 필요할 것 같다.

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM307
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:68 && (TALENT:120 || TALENT:121)
	SIF PALAM:0 >= (PALAMLV:4 - 1) ||  PALAM:1 >= (PALAMLV:4 - 1) ||  PALAM:2 >= (PALAMLV:4 - 1) ||  PALAM:10 >= (PALAMLV:4 - 1)
		PRINT 당장이라도 가버릴 것 같아하길래, 
	PRINTFORMW %조사선택(CALLNAME:TARGET, "가")% 원하는 대로 하게 했다….
ELSE
	PRINTFORMW %CALLNAME:TARGET%에게 %조사선택(CALLNAME:PLAYER, "를")% 범하도록 명령했다….
ENDIF

;-------------------------------------------------
;고유의 실행 판정
;-------------------------------------------------
@COM_ORDER_307, ARG
LOCAL:99 = ARG
;ABL:욕망
IF ABL:11
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:11
	TFLAG:240 += LOCAL:99 * 2
	RESULTS = %RESULTS% %ABLNAME:11%LV{LOCAL:99}({LOCAL:99 * 2})
ENDIF
;ABL:봉사 정신
IF ABL:13
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:13
	TFLAG:240 += LOCAL:99 * 4
	RESULTS = %RESULTS% %ABLNAME:13%LV{LOCAL:99}({LOCAL:99 * 4})
ENDIF

;쾌락 각인
IF MARK:1
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = MARK:1
	TFLAG:240 += LOCAL:99 * 2
	RESULTS = %RESULTS% %MARKNAME:1%LV{LOCAL:99}({LOCAL:99 * 2})
ENDIF

;PALAM:욕정
GETPALAMLV PALAM:13, 5
LOCAL:2 = RESULT
IF LOCAL:2
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = LOCAL:2 * 2
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %PALAMNAME:13%LV{LOCAL:2}({LOCAL:99})
ENDIF

;부끄럼쟁이
IF TALENT:35
	RESULTS = %RESULTS% - 
	LOCAL:99 = 1
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:35%({LOCAL:99})
ENDIF
;헌신적
IF TALENT:63
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 6
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:63%({LOCAL:99})
ENDIF
;쾌감에 솔직
IF TALENT:70
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 2
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:70%({LOCAL:99})
;쾌감의 부정
ELSEIF TALENT:71
	RESULTS = %RESULTS% - 
	LOCAL:99 = 2
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:71%({LOCAL:99})
ENDIF
;연모
IF TALENT:150 && ASSIPLAY == 0
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 5
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:150%({LOCAL:99})
ENDIF

;조수가 처녀
IF TALENT:PLAYER:0
	RESULTS = %RESULTS% - 
	LOCAL:99 = 15
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% 조수가 처녀({LOCAL:99})
ENDIF

TFLAG:241 = 40
