@COM_ABLE345
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:345 > 0
	RETURN 0
;조교자奉仕 · ウフフ系커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 2048) == 0
	RETURN 0
;主人でないとダメ
SIF ASSIPLAY
	RETURN 0
;吸血鬼や動物耳には순종4がないと使用不可(プレイヤーが새드ならＯＫ)
IF TALENT:205 || TALENT:116
	SIF ABL:10 < 4 && TALENT:PLAYER:81 == 0
		RETURN 0
ENDIF
;순종4미만 · 레즈끼(BL끼)3미만의 ASSI일 경우, 조교 대상이 동성이라면 실행할 수 없다(망신か복종ならＯＫ)
IF ASSIPLAY
	SIF (ABL:ASSI:10 < 4 || ((TALENT:120 == 0 && TALENT:ASSI:120 == 0 && ABL:ASSI:22 < 3) || (TALENT:120 && TALENT:ASSI:120 && ABL:ASSI:23 < 3))) && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
	RETURN 0
ENDIF
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;Ｃ계 장착구 사용 중에는 불가
SIF TEQUIP:30
	RETURN 0
;Ｂ계 장착구 사용 중에는 불가
SIF TEQUIP:35 || TEQUIP:36
	RETURN 0
;전극사용 중에는 불가
SIF TEQUIP:20 == 4 || TEQUIP:25 == 4 || TEQUIP:30 == 4 || TEQUIP:30 == 6 || TEQUIP:32 == 4 || TEQUIP:35 == 4
	RETURN 0
;お風呂中じゃないとダメ
SIF TEQUIP:53 != 1
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;助手のコスプレ中はダメ
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:59 > 2
	RETURN 0
;코스프레 중에는 불가
SIF TEQUIP:59 > 2
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM345
PRINTL 목욕하며 보낸다
TSTR:0 = 목욕하며 보낸다
CALL TRAIN_MESSAGE_COM345

DOWNBASE:0 += 0
DOWNBASE:1 += 20

SOURCE:10 = 50
SOURCE:18 = 50

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	SOURCE:12 = 30
ELSEIF PALAM:13 < PALAMLV:2
	SOURCE:12 = 50
ELSEIF PALAM:13 < PALAMLV:3
	SOURCE:12 = 80
ELSEIF PALAM:13 < PALAMLV:4
	SOURCE:12 = 120
ELSE
	SOURCE:12 = 200
ENDIF

;봉사 정신을 본다
IF ABL:13 == 0
	SOURCE:11 = 10
ELSEIF ABL:13 == 1
	SOURCE:11 = 30
ELSEIF ABL:13 == 2
	SOURCE:11 = 60
ELSEIF ABL:13 == 3
	SOURCE:11 = 100
ELSEIF ABL:13 == 4
	SOURCE:11 = 160
ELSE
	SOURCE:11 = 250
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:11 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:11 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:11 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:11 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:11 , 1.20
ELSE
	TIMES SOURCE:11 , 1.30
ENDIF

;동물귀의 경우
IF TALENT:116
	TIMES SOURCE:31 , 1.60
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:32 , 2.00
ENDIF
;흡혈귀의 경우
IF TALENT:205
	SOURCE:20 = 100
	TIMES SOURCE:20 , 1.60
	TIMES SOURCE:22 , 2.00
	TIMES SOURCE:32 , 1.40
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF

;샤워로 더러움이 리셋
RESET_STAIN TARGET
RESET_STAIN PLAYER

;(할 수 있으면) 윤활이 반에
PALAM:10 /= 2
PALAM:21 /= 2

RETURN 1

