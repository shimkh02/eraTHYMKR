;=============================================================================
;애널 요석
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE438
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:438 > 0
	RETURN 0
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16384) == 0
	RETURN 0
;고문계, 정신조작계커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16) == 0
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;Ａ확장경험이 0일 경우, 이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 && EXP:33 == 0
	RETURN 0
;조교자もしくは조교 대상이 天子でないとダメ
SIF NO:TARGET != 52 && NO:PLAYER != 52
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:25 == 2
	RETURN 1
;ASSI일 경우, 순종3이상이거나 새드, または망신か복종じゃないとできない。
IF ASSIPLAY
	SIF ABL:ASSI:10 < 4 && TALENT:ASSI:81 == 0 && TALENT:ASSI:151 == 0 && TALENT:ASSI:160 == 0
		RETURN 0
ENDIF
;작은 체형なら순종이 4이상かつ마조끼が4이상じゃないと実行できない
IF TALENT:128
	SIF ABL:10 < 4 || ABL:21 < 4
		RETURN 0
ENDIF
;조교 대상이 소인체형일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
	RETURN 0
;일정의 Ａ경험이 필요
SIF EXP:1 < EXPLV:3
	RETURN 0
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
	RETURN 0
;그밖의 Ａ계 장착구 사용 중에는 불가
SIF (TEQUIP:25 && TEQUIP:25 != 2) || TEQUIP:26
	RETURN 0
;쿠스코 삽입 중에는 불가
SIF TEQUIP:21
	RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;通常は弱いアナルバイブでしかないが、抜いたときに大地震が発生
@COM438
PRINTL 애널 요석
TSTR:0 = 애널 요석
CALL TRAIN_MESSAGE_COM438

IF TEQUIP:25 == 2
	DOWNBASE:0 += 500
	DOWNBASE:1 += 300

	;ABL:Ａ감각을 본다
	IF ABL:2 == 0
		SOURCE:2 = 1000
		SOURCE:11 = 10
		SOURCE:22 = 100
	ELSEIF ABL:2 == 1
		SOURCE:2 = 2000
		SOURCE:11 = 30
		SOURCE:22 = 700
	ELSEIF ABL:2 == 2
		SOURCE:2 = 3000
		SOURCE:11 = 100
		SOURCE:22 = 1500
	ELSEIF ABL:2 == 3
		SOURCE:2 = 4500
		SOURCE:11 = 200
		SOURCE:22 = 3000
	ELSEIF ABL:2 == 4
		SOURCE:2 = 6000
		SOURCE:11 = 450
		SOURCE:22 = 5000
	ELSE
		SOURCE:2 = 8000
		SOURCE:11 = 750
		SOURCE:22 = 8000
	ENDIF

	;アナル要石をはめていたターンより快Ａと体力消耗修整
	IF TFLAG:170
		SOURCE:2 = 200 * TFLAG:170
		DOWNBASE:0 += 75 * TFLAG:170
	ENDIF
ELSE
	DOWNBASE:0 += 60
	DOWNBASE:1 += 150

	;ABL:Ａ감각을 본다
	IF ABL:2 == 0
		SOURCE:2 = 80
		SOURCE:22 = 300
	ELSEIF ABL:2 == 1
		SOURCE:2 = 250
		SOURCE:22 = 800
	ELSEIF ABL:2 == 2
		SOURCE:2 = 600
		SOURCE:22 = 1400
	ELSEIF ABL:2 == 3
		SOURCE:2 = 1000
		SOURCE:22 = 1800
	ELSEIF ABL:2 == 4
		SOURCE:2 = 1300
		SOURCE:22 = 2100
	ELSE
		SOURCE:2 = 1700
		SOURCE:22 = 2400
	ENDIF
ENDIF

;EXP:Ａ경험을 본다
IF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:20 = 5000
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:20 = 1800
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.10
	SOURCE:20 = 1000
ELSE
	TIMES SOURCE:2 , 1.20
	SOURCE:20 = 600
ENDIF

SOURCE:12 = 1200
SOURCE:31 = 6200

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 10000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 3600
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 1200
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:20 += 200
ELSE
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSE
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.70
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 0.90
ELSE
	TIMES SOURCE:2 , 1.00
ENDIF

;小柄体型
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;Ａ민감、둔감을 본다
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF
;처녀에다 정조관념
SIF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3

;アナル要石の着脱
IF TEQUIP:25 == 2
	SOURCE:31 *= 2
	TFLAG:170 = 0
	TCVAR:1 += 25
	TCVAR:33 += 3
	TEQUIP:25 = 0
ELSE
	TCVAR:1 += 2
	SIF EXP:33 == 0
		TCVAR:50 += 1
	TCVAR:33 += 1
	TEQUIP:25 = 2
ENDIF

RETURN 1

;-------------------------------------------------
;要石アナル挿入中
;-------------------------------------------------
@EQUIP_COM438
PRINTL <요석 애널 삽입 중>

DOWNBASE:0 += 40
DOWNBASE:1 += 90

;ABL:Ａ감각을 본다
IF ABL:2 == 0
	LOCAL = 20
	LOCAL:1 = 100
ELSEIF ABL:2 == 1
	LOCAL = 60
	LOCAL:1 = 300
ELSEIF ABL:2 == 2
	LOCAL = 150
	LOCAL:1 = 500
ELSEIF ABL:2 == 3
	LOCAL = 250
	LOCAL:1 = 800
ELSEIF ABL:2 == 4
	LOCAL = 400
	LOCAL:1 = 1000
ELSE
	LOCAL = 800
	LOCAL:1 = 1400
ENDIF

;EXP:Ａ경험을 본다
IF EXP:1 < EXPLV:3
	TIMES LOCAL , 1.10
	LOCAL:2 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES LOCAL , 1.30
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL , 0.40
	LOCAL:2 += 3000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL , 0.80
	LOCAL:2 += 1500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 += 1000
ELSEIF PALAM:10 < PALAMLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 += 500
ELSE
	TIMES LOCAL , 1.40
	LOCAL:2 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.70
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 2
	TIMES LOCAL , 0.90
ELSE
	TIMES LOCAL , 1.00
ENDIF

;小柄体型
SIF TALENT:128
	TIMES LOCAL:2 , 2.00

;Ａ민감、둔감을 본다
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀에다 정조관념
SIF TALENT:0 == 1 && TALENT:30
	LOCAL:1 /= 3

SOURCE:2 += LOCAL
SOURCE:22 += LOCAL:1
SOURCE:20 += LOCAL:2

;自慰実行時は経験＋1
IF SELECTCOM == 7 || SELECTCOM == 124 || SELECTCOM == 125 || SELECTCOM == 127 || SELECTCOM == 182 || SELECTCOM == 205 || SELECTCOM == 628
	TCVAR:1 += 2
ELSE
	TCVAR:1 += 1
ENDIF

TFLAG:170 += 1

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM438
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

