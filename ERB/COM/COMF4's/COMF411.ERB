TSTR:0 = 인형 애무

DOWNBASE:0 += 15
DOWNBASE:1 += 75

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------

SOURCE:15 = 60
SOURCE:12 = 200
SOURCE:31 = 800

;여기서 커맨드 사용 회수를 플러스
TFLAG:141 += 1

IF ABL:0 > 0
	LOCAL:1 = 1
	SIF ABL:0 > 3
		LOCAL:1 += 1
ELSEIF ABL:1 > 0
	LOCAL:2 = 1
	SIF ABL:1 > 3
		LOCAL:2 += 1
ELSEIF ABL:2 > 0
	LOCAL:3 = 1
	SIF ABL:2 > 3
		LOCAL:3 += 1
ELSEIF ABL:3 > 0
	LOCAL:4 = 1
	SIF ABL:3 > 3
		LOCAL:4 += 1
ELSE
	LOCAL:5 = 1
ENDIF

SIF LOCAL:2 > 0 || LOCAL:3 > 0
	SOURCE:20 = 600
SIF ABL:3 > 0
	SOURCE:22 = 500

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 0
ELSEIF ABL:0 == 1
	SOURCE:0 = 100
	SOURCE:11 += 60
ELSEIF ABL:0 == 2
	SOURCE:0 = 500
	SOURCE:11 += 120
ELSEIF ABL:0 == 3
	SOURCE:0 = 1100
	SOURCE:11 += 240
ELSEIF ABL:0 == 4
	SOURCE:0 = 1900
	SOURCE:11 += 360
ELSE
	SOURCE:0 = 2700
	SOURCE:11 += 600
ENDIF

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 0
ELSEIF ABL:1 == 1
	SOURCE:1 = 50
	SOURCE:22 += 100
ELSEIF ABL:1 == 2
	SOURCE:1 = 200
	SOURCE:22 += 150
ELSEIF ABL:1 == 3
	SOURCE:1 = 600
	SOURCE:22 += 300
ELSEIF ABL:1 == 4
	SOURCE:1 = 1200
	SOURCE:22 += 600
ELSE
	SOURCE:1 = 1800
	SOURCE:22 += 700
ENDIF

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 0
ELSEIF ABL:2 == 1
	SOURCE:2 = 75
	SOURCE:22 += 100
ELSEIF ABL:2 == 2
	SOURCE:2 = 150
	SOURCE:22 += 250
ELSEIF ABL:2 == 3
	SOURCE:2 = 400
	SOURCE:22 += 500
ELSEIF ABL:2 == 4
	SOURCE:2 = 800
	SOURCE:22 += 800
ELSE
	SOURCE:2 = 1300
	SOURCE:22 += 1200
ENDIF

;ABL:B감각을 본다
IF ABL:3 == 0
	SOURCE:3 = 0
ELSEIF ABL:3 == 1
	SOURCE:3 = 100
	SOURCE:11 += 50
ELSEIF ABL:3 == 2
	SOURCE:3 = 400
	SOURCE:11 += 100
ELSEIF ABL:3 == 3
	SOURCE:3 = 1000
	SOURCE:11 += 200
ELSEIF ABL:3 == 4
	SOURCE:3 = 1700
	SOURCE:11 += 320
ELSE
	SOURCE:3 = 2500
	SOURCE:11 += 450
ENDIF

;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:100
	TIMES SOURCE:0 , 1.50
ELSEIF TALENT:101
	TIMES SOURCE:0 , 0.60
ENDIF
;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:102
	TIMES SOURCE:1 , 1.50
ELSEIF TALENT:103
	TIMES SOURCE:1 , 0.60
ENDIF
;A민감, 둔감을 본다
