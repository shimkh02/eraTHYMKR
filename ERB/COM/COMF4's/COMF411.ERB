@COM_ABLE411
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:411 > 0
	RETURN 0
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16384) == 0
	RETURN 0
;Ｖ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 1
	RETURN 0
;조교자가 ＥＸ化していないアリスでないとダメ
SIF NO:PLAYER != 14 || CFLAG:PLAYER:0
	RETURN 0
;人形愛撫残り使用可能回数オーバー
SIF TFLAG:141 > 3
	RETURN 0
;対象が～感覚を1つも持っていないならダメ
SIF ABL:0 <= 0 && ABL:1 <= 0 && ABL:2 <= 0 && ABL:3 <= 0
	RETURN 0
;대상에게 페니스가 있다면 불가
SIF TALENT:1 || TALENT:120 || TALENT:121
	RETURN 0
;조교자의 기교가 3無いとできない
SIF ABL:PLAYER:12 < 3
	RETURN 0
;Ｖ계 장착구 사용 중에는 불가
SIF TEQUIP:20 || TEQUIP:21
	RETURN 0
;Ａ계 장착구 사용 중에는 불가
SIF TEQUIP:25 || TEQUIP:26
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;알몸 앞치마, 학교수영복 플레이, 코스프레 중에는 불가
SIF TEQUIP:59
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

