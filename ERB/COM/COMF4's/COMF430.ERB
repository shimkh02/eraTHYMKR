;ABL:기교를 본다
IF ABL:12 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:11 , 0.60
ELSEIF ABL:12 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:11 , 0.80
