	ELSEIF ABL:31 == 2
		SOURCE:24 = 1000
	ELSEIF ABL:31 == 3
		SOURCE:24 = 1500
	ELSEIF ABL:31 == 4
		SOURCE:24 = 2200
	ELSE
		SOURCE:24 = 1000 + (ABL:31 * 400)
