	SOURCE:20 += 700
	TIMES SOURCE:20 , 1.00
	SOURCE:12 += 600
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:20 , 0.50
	SOURCE:12 += 80
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:20 , 0.30
ELSEIF PALAM:10 < PALAMLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:20 , 0.20
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:20 , 0.10
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;정조관념
IF TALENT:30
	IF TALENT:0 == 1
		TIMES SOURCE:11 , 0.60
		TIMES SOURCE:31, 15.00
		SOURCE:32 = 17000
	ELSE
		TIMES SOURCE:11 , 0.60
		TIMES SOURCE:31, 15.00
		SOURCE:32 = 1500
	ENDIF
;정조무구애
ELSEIF TALENT:31
	IF TALENT:0 == 1
		TIMES SOURCE:31, 0.50
		SOURCE:32 = 450
	ELSE
		TIMES SOURCE:31, 0.50
	ENDIF
ELSE
	SIF TALENT:0 == 1
		SOURCE:32 = 4500
ENDIF

;PALAM:욕정
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:11 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:11 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:11 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:11 , 1.50
ELSEIF PALAM:13 < PALAMLV:5
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:11 , 1.80
ELSE
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:11 , 2.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:11 , 0.60
	TIMES SOURCE:32 , 2.00
	SOURCE:22 += 1000
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:11 , 0.80
	TIMES SOURCE:32 , 1.50
	SOURCE:22 += 700
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:11 , 1.00
	TIMES SOURCE:32 , 1.00
	SOURCE:22 += 400
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:11 , 1.20
	TIMES SOURCE:32 , 0.80
	SOURCE:22 += 150
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:11 , 1.40
	TIMES SOURCE:32 , 0.60
	SOURCE:22 += 60
