;=============================================================================
;우산에 빨게 한다
;=============================================================================
;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE453
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:453 > 0
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:30 == 9
	RETURN 1
;캐릭터 전용 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 16384) == 0
	RETURN 0
;からかさお化け(混血可)がいて, かつ起きてないとダメ(拘束はＯＫ)
SIF !COM_453_RACE(MASTER) && (!COM_453_RACE(TARGET) || EQUIP:14 || TFLAG:899) && !COM_453_RACE(ASSI)
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;褌着用中もダメ
SIF TEQUIP:59 == 17
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;그밖의 Ｃ계 장착구 사용 중에는 불가
SIF TEQUIP:30 && TEQUIP:30 != 9
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;카라카사 전용. 자신을 빨게 할 수도 있고, 주인·조수의 우산에 조교 대상을 빨게 할 수도 있다
@COM453
VARSET LOCAL
;카라카사의 인원수를 취득해 누구의 우산을 사용하는지를 결정
LOCAL = COM_453_RACE(MASTER) + COM_453_RACE(TARGET) + COM_453_RACE(ASSI)

;코가사 한정 때
IF COM_453_KOGASA_LOOK()
	LOCAL:10 = 1
	CALLF COM_453_UMBRELLA(1, GETCHARA(72))
;카라카사가 복수 있을 때
ELSEIF LOCAL > 1
	LOCAL = 0
	PRINTL 누구의 우산을 사용합니까?
	IF COM_453_RACE(MASTER)
		PRINTFORML [{LOCAL}] %CALLNAME:MASTER%
		LOCAL:1 = LOCAL
		LOCAL ++
	ENDIF
	IF COM_453_RACE(TARGET)
		PRINTFORML [{LOCAL}] %CALLNAME%
		LOCAL:2 = LOCAL
		LOCAL ++
	ENDIF
	IF COM_453_RACE(ASSI)
		PRINTFORML [{LOCAL}] %CALLNAME:ASSI%
		LOCAL:3 = LOCAL
		LOCAL ++
	ENDIF
	$COM453_LOOP
	INPUT
	IF RESULT == LOCAL:1 && COM_453_RACE(MASTER)
		PRINTFORMW %CALLNAME:COM_453_UMBRELLA(1, MASTER)%의 우산으로 %조사처리(CALLNAME,"을")% 빨게 합니다.
	ELSEIF RESULT == LOCAL:2 && COM_453_RACE(TARGET)
		PRINTFORMW %CALLNAME:COM_453_UMBRELLA(1, TARGET)% 자신의 우산으로 %조사처리(CALLNAME,"을")% 빨게 합니다.
	ELSEIF RESULT == LOCAL:3 && COM_453_RACE(ASSI)
		PRINTFORMW %CALLNAME:COM_453_UMBRELLA(1, ASSI)%의 우산에 %조사처리(CALLNAME,"을")% 빨게 합니다.
	ELSE
		GOTO COM453_LOOP
	ENDIF
ELSE
	IF COM_453_RACE(MASTER)
		CALLF COM_453_UMBRELLA(1, MASTER)
	ELSEIF COM_453_RACE(TARGET)
		CALLF COM_453_UMBRELLA(1, TARGET)
	ELSEIF COM_453_RACE(ASSI)
		CALLF COM_453_UMBRELLA(1, ASSI)
	ENDIF
ENDIF

LOCAL = COM_453_UMBRELLA()

PRINTFORML <\@ TEQUIP:50 && TARGET == LOCAL ? 공개 # \@\@ TEQUIP:52 && TARGET == LOCAL ? 야외 # \@\@ TEQUIP:74 && TARGET == LOCAL ? 독심 # \@우산\@ TALENT:120 || TALENT:121 ? 펠라 # 커닐링구스 \@\@ TARGET == LOCAL ? 자위 # \@>

;-------------------------------------------------
;자위라면, 실행할 수 있을까의 판정
;-------------------------------------------------
IF TARGET == LOCAL && !LOCAL:10
	IF TFLAG:699 == 0
		CALL COM_ORDER
		;실행할 수 없다
		SIF RESULT == 0
			RETURN 0
	ENDIF
ENDIF
TSTR:0 = \@ TEQUIP:50 && TARGET == LOCAL ?  공개 # \@\@ TEQUIP:52 && TARGET == LOCAL ?  야외 # \@\@ TEQUIP:74 && TARGET == LOCAL ? 독심 # \@우산\@ TALENT:120 || TALENT:121 ? 펠라 # 커닐링구스 \@\@ TARGET == LOCAL ? 자위 # \@

CALL TRAIN_MESSAGE_COM453

DOWNBASE:0 += 10
DOWNBASE:1 += 80

SOURCE:12 = 120
SOURCE:31 = 70

;ABL:C감각을 본다
;여기는 클리캡과 같지만 혀사용의 탓으로 효과가 높아진다
IF ABL:0 == 0
	SOURCE:0 = 200
ELSEIF ABL:0 == 1
	SOURCE:0 = 400
ELSEIF ABL:0 == 2
	SOURCE:0 = 900
ELSEIF ABL:0 == 3
	SOURCE:0 = 1600
ELSEIF ABL:0 == 4
	SOURCE:0 = 2400
ELSE
	SOURCE:0 = 3000
ENDIF

;코가사의 기교를 본다
IF ABL:LOCAL:12 == 0
	SIF LOCAL == TARGET
		SOURCE:15 = 100
	TIMES SOURCE:0 , 0.30
ELSEIF ABL:LOCAL:12 == 1
	SIF LOCAL == TARGET
		SOURCE:15 = 160
	TIMES SOURCE:0 , 0.70
ELSEIF ABL:LOCAL:12 == 2
	SIF LOCAL == TARGET
		SOURCE:15 = 220
	TIMES SOURCE:0 , 1.00
ELSEIF ABL:LOCAL:12 == 3
	SIF LOCAL == TARGET
		SOURCE:15 = 280
	TIMES SOURCE:0 , 1.20
ELSEIF ABL:LOCAL:12 == 4
	SIF LOCAL == TARGET
		SOURCE:15 = 340
	TIMES SOURCE:0 , 1.40
ELSE
	SIF LOCAL == TARGET
		SOURCE:15 = 400
	TIMES SOURCE:0 , 1.60
ENDIF

;자위라면, 자위 중독을 본다
IF LOCAL == TARGET
	IF ABL:30 == 0
		SOURCE:24 = 0
		TIMES SOURCE:0 , 1.00
	ELSEIF ABL:30 == 1
		SOURCE:24 = 100
		TIMES SOURCE:0 , 1.10
	ELSEIF ABL:30 == 2
		SOURCE:24 = 300
		TIMES SOURCE:0 , 1.20
	ELSEIF ABL:30 == 3
		SOURCE:24 = 800
		TIMES SOURCE:0 , 1.30
	ELSEIF ABL:30 == 4
		SOURCE:24 = 1500
		TIMES SOURCE:0 , 1.50
	ELSE
		SOURCE:24 = 2500
		TIMES SOURCE:0 , 1.70
	ENDIF
ENDIF

;우산의 주의 TALENT:52를 본다
IF TALENT:LOCAL:52
	TIMES SOURCE:0 , 2.00
	SOURCE:17 += SOURCE:0 / 20
ENDIF

;고양이혀은 본체만으로 우산은 무관계라고 봐 고려하지 않는다

;액체 추가
SOURCE:19 = 200

;우산의 착탈
IF TEQUIP:30
	TEQUIP:30 = 0
	TFLAG:178 = 0
ELSE
	TEQUIP:30 = 9
	TFLAG:178 = LOCAL
ENDIF

SIF TALENT:120 || TALENT:121
	TCVAR:LOCAL:22 += 1

IF LOCAL == TARGET
	TCVAR:LOCAL:10 += 1
	TCVAR:LOCAL:11 += 1
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF
SIF LOCAL == MASTER && TEQUIP:30 == 9
	TCVAR:41 += 1

RETURN 1

;-------------------------------------------------
;우산 빨고 안
;-------------------------------------------------
@EQUIP_COM453
LOCAL = TFLAG:178
PRINTFORML <우산 \@ TALENT:120 || TALENT:121 ? 펠라 # 커닐링구스 \@ 중>

SIF TALENT:120 || TALENT:121
	TCVAR:LOCAL:22 += 1

IF LOCAL == TARGET
	TCVAR:LOCAL:10 += 1
	TCVAR:LOCAL:11 += 1
ENDIF

DOWNBASE:0 += 5
DOWNBASE:1 += 20

;ABL:C감각을 본다
IF ABL:0 == 0
	LOCAL:1 = 80
ELSEIF ABL:0 == 1
	LOCAL:1 = 240
ELSEIF ABL:0 == 2
	LOCAL:1 = 500
ELSEIF ABL:0 == 3
	LOCAL:1 = 900
ELSEIF ABL:0 == 4
	LOCAL:1 = 1200
ELSE
	LOCAL:1 = 1500
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL:1 , 1.10
ELSE
	TIMES LOCAL:1 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL:1 , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL:1 , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL:1 , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL:1 , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL:1 , 1.20
ELSE
	TIMES LOCAL:1 , 1.30
ENDIF

SOURCE:0 += LOCAL:1
SOURCE:12 += 50
SOURCE:18 += 50
SOURCE:19 += 100

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM453
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0
LOCAL = COM_453_UMBRELLA()
PRINTL
IF TEQUIP:30
	PRINTFORML %CALLNAME:LOCAL%의 우산은 미끈미끈한 혀로 %CALLNAME%의 \@ TALENT:120 || TALENT:121 ? 페니스 # 클리토리스 \@를 한층 더 격렬하게 빨더니,
	PRINTFORMW 만족했는지, 한 다리 뛰어 %조사처리(CALLNAME,"로")%부터 멀어졌다.
ELSE
	IF TALENT:120 || TALENT:121
		PRINTFORML %CALLNAME:LOCAL%의 우산의 타액이 흐르는 거대한 혀가, %CALLNAME%의 페니스에 감겨
		PRINTW 추잡하게 아래위로 켜댔다….
	ELSE
		PRINTFORML %CALLNAME:LOCAL%의 우산의 타액이 흐르는 거대한 혀가, %CALLNAME%의 비부를 흠뻑 덮어
		PRINTW 커다란 움직임으로 서서히 핥아 올리기 시작했다….
	ENDIF
ENDIF

;-------------------------------------------------
;고유의 실행 판정
;-------------------------------------------------
@COM_ORDER_453, ARG
LOCAL:99 = ARG
;ABL:욕망
IF ABL:11
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:11
	TFLAG:240 += LOCAL:99 * 3
	RESULTS = %RESULTS% %ABLNAME:11%LV{LOCAL:99}({LOCAL:99 * 3})
ENDIF
;ABL:노출벽
IF ABL:14
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:14
	TFLAG:240 += LOCAL:99 * 4
	RESULTS = %RESULTS% %ABLNAME:14%LV{LOCAL:99}({LOCAL:99 * 4})
ENDIF
;ABL:자위 중독
IF ABL:30
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = ABL:30
	TFLAG:240 += LOCAL:99 * 3
	RESULTS = %RESULTS% %ABLNAME:30%LV{LOCAL:99}({LOCAL:99 * 3})
ENDIF

;쾌락 각인
IF MARK:1
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = MARK:1
	TFLAG:240 += LOCAL:99 * 3
	RESULTS = %RESULTS% %MARKNAME:1%LV{LOCAL:99}({LOCAL:99 * 3})
ENDIF

;PALAM:욕정
GETPALAMLV PALAM:13, 5
LOCAL:2 = RESULT
IF LOCAL:2
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = LOCAL:2 * 3
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %PALAMNAME:13%LV{LOCAL:2}({LOCAL:99})
ENDIF

;자제심
IF TALENT:20
	RESULTS = %RESULTS% - 
	LOCAL:99 = 5
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:20%({LOCAL:99})
ENDIF
;부끄럼쟁이
IF TALENT:35
	RESULTS = %RESULTS% - 
	LOCAL:99 = 5
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:35%({LOCAL:99})
;부끄럼없음
ELSEIF TALENT:36
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 2
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:36%({LOCAL:99})
ENDIF
;자위하기 쉽다
IF TALENT:60
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 5
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:60%({LOCAL:99})
ENDIF
;쾌감에 솔직
IF TALENT:70
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 5
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:70%({LOCAL:99})
;쾌감의 부정
ELSEIF TALENT:71
	RESULTS = %RESULTS% - 
	LOCAL:99 = 5
	TFLAG:240 -= LOCAL:99
	RESULTS = %RESULTS% %TALENTNAME:71%({LOCAL:99})
ENDIF

;미약
IF TEQUIP:11
	SIF LOCAL:99
		RESULTS = %RESULTS% + 
	LOCAL:99 = 8
	TFLAG:240 += LOCAL:99
	RESULTS = %RESULTS% %ITEMNAME:61%({LOCAL:99})
ENDIF

;난이도 상승
;공개·독심으로+10
TFLAG:241 = 38
SIF TEQUIP:50
	TFLAG:241 += 10
SIF TEQUIP:52
	TFLAG:241 += 10
SIF TEQUIP:74
	TFLAG:241 += 10

;카라카사의 피가 35%이상 들어가 있을까를 본다
@COM_453_RACE(ARG)
#FUNCTION
RETURNF ARG >= 0 && CFLAG:ARG:3249 >= 35

;누구의 우산이나 기록한다
@COM_453_UMBRELLA(ARG, ARG:1)
#FUNCTION
SIF ARG
	LOCAL = ARG:1
RETURNF LOCAL

;코가사 락(말용)
@COM_453_KOGASA_LOOK(ARG)
#FUNCTION
IF ARG
	LOCAL = 1
ELSE
	LOCAL:1 = LOCAL
	LOCAL = 0
ENDIF
RETURNF LOCAL:1
