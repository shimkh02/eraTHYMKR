@COM_ABLE135
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:135 > 0
	RETURN 0
;Ｃ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 1) == 0
	RETURN 0
;수치 플레이계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 512) == 0
	RETURN 0
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4
	RETURN 0
;남자 또는 후타나리면 불가
SIF EXIST_PENIS(TARGET)
	RETURN 0
;Ｃ계 장착구 사용 중에는 불가
SIF TEQUIP:30 || TEQUIP:31
	RETURN 0
;클리토리스를 성장시키지 않으면 불가
SIF BASE:7 == 0
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;욕실 플레이 중에는 불가
SIF TEQUIP:53
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
	RETURN 0
;알몸 앞치마, 학교수영복 플레이 중에는 불가
SIF TEQUIP:59 == 1 || TEQUIP:59 == 2
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;처리 간략화를 위해[남자],[후타나리]에는 사용할 수 없게
;동시에 지문에 죽어 판정이 발생(향후의 확장성을 전망해 굳이 남기고 있습니다)
@COM135
PRINTL 음핵 측정

;-------------------------------------------------
;실행할 수 있을까의 판정
;-------------------------------------------------
;속박되지 않으면 실행 판정
IF TEQUIP:40 == 0
	IF TFLAG:699 == 0
		CALL COM_ORDER
		;실행할 수 없다
		SIF RESULT == 0
			RETURN 0
	ENDIF
ENDIF

;-------------------------------------------------
;실행 결정
;-------------------------------------------------
TSTR:0 = 음핵 측정
CALL TRAIN_MESSAGE_COM135

;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:1 += 90

SOURCE:10 = 200
SOURCE:12 = 250
SOURCE:22 = 250
SOURCE:31 = 500

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 100
	SOURCE:22 += 200
ELSEIF ABL:0 == 1
	SOURCE:0 = 200
	SOURCE:22 += 300
ELSEIF ABL:0 == 2
	SOURCE:0 = 350
	SOURCE:22 += 600
ELSEIF ABL:0 == 3
	SOURCE:0 = 700
	SOURCE:22 += 900
ELSEIF ABL:0 == 4
	SOURCE:0 = 1400
	SOURCE:22 += 1300
ELSE
	SOURCE:0 = 1800
	SOURCE:22 += 1700
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	SOURCE:12 += 1500
	SOURCE:30 = 2000
ELSEIF ABL:14 == 1
	SOURCE:12 += 1250
	SOURCE:30 = 1000
ELSEIF ABL:14 == 2
	SOURCE:12 += 750
	SOURCE:30 = 500
ELSEIF ABL:14 == 3
	SOURCE:12 += 500
	SOURCE:30 = 200
ELSEIF ABL:14 == 4
	SOURCE:12 += 250
	SOURCE:30 = 100
ELSE
	SOURCE:30 = 50
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:10 , 0.50
	TIMES SOURCE:30 , 1.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:10 , 0.80
	TIMES SOURCE:30 , 1.50
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:10 , 1.20
	TIMES SOURCE:30 , 1.20
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:10 , 1.50
	TIMES SOURCE:30 , 0.80
ELSE
	TIMES SOURCE:10 , 1.80
	TIMES SOURCE:30 , 0.50
ENDIF

;부끄럼쟁이·부끄럼없음
IF TALENT:35
	TIMES SOURCE:30 , 1.20
ELSEIF TALENT:36
	TIMES SOURCE:30 , 0.50
ENDIF
;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:100
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:101
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 1.50
ENDIF

;처녀, 정조관념
IF TALENT:0
	TIMES SOURCE:30 , 1.20
	IF TALENT:0 == 1
		SOURCE:12 += 2000
		TIMES SOURCE:22 , 2.00
		TIMES SOURCE:30 , 2.00
		SIF TALENT:30
			TIMES SOURCE:12 , 2.00
	ENDIF
ENDIF

IF ABL:10 < 3 && MARK:2 > 0 && MARK:9 < 3
	;EXP:자위 경험을 본다
	IF EXP:10 < EXPLV:1
		TIMES SOURCE:30 , 1.50
		TIMES SOURCE:22 , 0.20
	ELSEIF EXP:10 < EXPLV:2
		TIMES SOURCE:30 , 1.20
		TIMES SOURCE:22 , 0.50
	ELSEIF EXP:10 < EXPLV:3
		TIMES SOURCE:30 , 1.00
		TIMES SOURCE:22 , 0.80
	ELSEIF EXP:10 < EXPLV:4
		TIMES SOURCE:30 , 0.80
		TIMES SOURCE:22 , 1.00
	ELSEIF EXP:10 < EXPLV:5
		TIMES SOURCE:30 , 0.50
		TIMES SOURCE:22 , 1.20
	ELSE
		TIMES SOURCE:30 , 0.20
		TIMES SOURCE:22 , 1.50
	ENDIF

	SIF ABL:14 >= 3
		TCVAR:10 += 1
ENDIF
;노예의 V⇔손가락의 불결이 이동
STAIN:3 |= STAIN:1
STAIN:1 |= STAIN:3

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF

SIF PLAYER == MASTER
	TCVAR:41 += 1

CALL PENIS_SMESSAGE

RETURN 1

