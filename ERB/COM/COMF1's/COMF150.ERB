	FLAG:1110 = 8
	CALL NAMECHECK_CUSTOMTOOL
ELSE
	TSTR:8 = %TSTR:7%
	TSTR:9 = %TSTR:7%
ENDIF
PRINTFORML %TSTR:8%
TSTR:0 = 쿠스코
CALL TRAIN_MESSAGE_COM150

DOWNBASE:0 += 50
DOWNBASE:1 += 100

;ABL:V감각을 본다
IF ABL:1 == 0
	SOURCE:1 = 40
ELSEIF ABL:1 == 1
	SOURCE:1 = 130
ELSEIF ABL:1 == 2
	SOURCE:1 = 300
ELSEIF ABL:1 == 3
	SOURCE:1 = 500
ELSEIF ABL:1 == 4
	SOURCE:1 = 800
ELSE
	SOURCE:1 = 1200
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	SOURCE:12 = 300
	SOURCE:19 = 0
	SOURCE:22 = 600
	SOURCE:24 = 0
	SOURCE:31 = 800
ELSEIF ABL:14 == 1
	SOURCE:12 = 500
	SOURCE:19 = 150
	SOURCE:22 = 500
	SOURCE:24 = 150
	SOURCE:31 = 200
ELSEIF ABL:14 == 2
	SOURCE:12 = 500
	SOURCE:19 = 800
	SOURCE:22 = 400
	SOURCE:24 = 300
ELSEIF ABL:14 == 3
	SOURCE:12 = 500
	SOURCE:19 = 600
	SOURCE:22 = 300
	SOURCE:24 = 600
ELSEIF ABL:14 == 4
	SOURCE:12 = 1000
	SOURCE:19 = 1000
	SOURCE:22 = 200
	SOURCE:24 = 1000
ELSE
