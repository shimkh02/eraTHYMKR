	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;프라이드 높다
SIF TALENT:16
	TIMES LOCAL:3 , 1.20
;무관심
IF TALENT:21
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:3 , 0.80
ENDIF
;정조관념
SIF TALENT:30
	TIMES LOCAL:3 , 2.00
;도착적
IF TALENT:80
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:3 , 1.20
ENDIF
;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:2 , 1.50

IF FLAG:1100 & 1
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 8
	FLAG:1111 = 1
	FLAG:1112 = LOCAL
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL = FLAG:1120
	FLAG:1112 = LOCAL:1
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL:1 = FLAG:1120
	FLAG:1111 = 3
	FLAG:1112 = LOCAL:2
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL:2 = FLAG:1120
ENDIF

;비디오 카메라
SIF TEQUIP:50
	TIMES LOCAL:1 , 1.20
;수치 플레이
SIF TEQUIP:56
	TIMES LOCAL:1 , 1.20

SOURCE:1 += LOCAL
SOURCE:12 += LOCAL:1
SOURCE:20 += LOCAL:2
SOURCE:31 += LOCAL:3

TCVAR:0 += 2

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2

RETURN 1

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM150
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0

PRINTL
IF TEQUIP:21
	PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 질로부터 %조사선택(TSTR:7, "를")% 뽑아 냈다….
	IF EXP:0 >= 200 && TALENT:150
		PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 넓혀진 채로 대량의 점액으로 끈적해진 보지를 황홀한 표정으로 응시하고 
	ELSEIF EXP:0 >= 71
		PRINTFORM %CALLNAME:TARGET%의 질구멍은 원래대로 돌아가지 않고, 애액을 흘려 보내고 
	ELSEIF EXP:0 >= 21
		PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 얼굴을 붉혀, 아직도 남아있는 이물감에 떨고 
	ELSE
		PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 수치와 압박감에서 해방되어, 안도의 표정을 짓고 
	ENDIF
	PRINTW 있다….
ELSE
	PRINTFORM %조사선택(CALLNAME:PLAYER, "는")% 
	SIF TEQUIP:50
		PRINT 비디오 카메라의 앞에서 
	SIF TEQUIP:56
		PRINT 거울에 비치는 질 내를 자신에게 과시하듯이 
	PRINTFORML %CALLNAME:TARGET%의 질에 %조사선택(TSTR:9, "를")% 삽입해 벌렸다.
	PRINTL

	IF PALAM:10 >= PALAMLV:5
		PRINTL 벌려진 질 벽에서 대량의 희고 끈적한 꿀이, 눅진눅진 바닥에 떨어지기 시작했다….
	ELSEIF PALAM:10 >= PALAMLV:3
		PRINTFORML 질 벽에서는 점액이 주륵 주륵 %TSTR:7%에 늘어져 떨어지고 있다….
	ELSE
		WAIT
		RETURN 1
	ENDIF

	PRINTFORM %조사선택(CALLNAME:TARGET, "는")% 
	IF EXP:0 >= 200 && (TALENT:150 || TALENT:160 || TALENT:169 || TALENT:170)
		PRINTFORM %CALLNAME:PLAYER% 쪽으로 허리를 밀어 올리며, 더욱 더 넓혀달라고 요구하고 
	ELSEIF EXP:0 >= 71 && (TALENT:150 || TALENT:160 || TALENT:169 || TALENT:170)
		PRINTFORM 천천히 넓혀 가는 %CALLNAME:PLAYER%의 손을, 군침을 흘리며 바라보고 
	ELSEIF EXP:0 >= 71
		PRINTFORM 무엇인가를 포기한 듯한 표정으로, 천천히 넓혀 가는 %CALLNAME:PLAYER%의 손을 바라보고 
	ELSEIF EXP:0 >= 21
		PRINT 질 내를 채우는 차가운 이물감에 뭐라고 말하기 힘든 표정을 하고 
	ELSE
		PRINT 하복부가 찢어지는 것 같은 감각에, 소리 높여 신음하고 
	ENDIF
	PRINTW 있다….
ENDIF
