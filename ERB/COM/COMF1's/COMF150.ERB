	LOCAL = 0
ELSEIF ABL:1 == 1
	LOCAL = 50
ELSEIF ABL:1 == 2
	LOCAL = 100
ELSEIF ABL:1 == 3
	LOCAL = 150
ELSEIF ABL:1 == 4
	LOCAL = 200
ELSE
	LOCAL = 300
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	LOCAL:1 = 300
	LOCAL:3 = 1000
	SOURCE:22 += 600
ELSEIF ABL:14 == 1
	LOCAL:1 = 500
	LOCAL:3 = 500
	SOURCE:22 += 500
ELSEIF ABL:14 == 2
	LOCAL:1 = 500
	LOCAL:3 = 150
	SOURCE:22 += 400
	SOURCE:24 += 300
ELSEIF ABL:14 == 3
	LOCAL:1 = 500
	LOCAL:3 = 50
	SOURCE:22 += 300
	SOURCE:24 += 600
ELSEIF ABL:14 == 4
	LOCAL:1 = 1000
	LOCAL:3 = 0
	SOURCE:22 += 200
	SOURCE:24 += 1000
ELSE
	LOCAL:1 = 1300
	LOCAL:3 = 0
	SOURCE:22 += 100
	SOURCE:24 += 1800
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES LOCAL:1 , 0.00
ELSEIF ABL:21 == 1
	TIMES LOCAL:1 , 1.10
ELSEIF ABL:21 == 2
	TIMES LOCAL:1 , 1.30
ELSEIF ABL:21 == 3
	TIMES LOCAL:1 , 1.50
ELSEIF ABL:21 == 4
	TIMES LOCAL:1 , 1.70
ELSE
	TIMES LOCAL:1 , 2.10
ENDIF

;EXP:V경험을 본다
;처녀는 있을 수 없다
IF EXP:0 < EXPLV:2
	TIMES LOCAL , 0.60
	LOCAL:2 = 150
ELSEIF EXP:0 < EXPLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 = 20
ELSEIF EXP:0 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 0
ELSEIF EXP:0 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL , 0.10
	LOCAL:2 += 400
	TIMES LOCAL:2 , 3.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL , 0.40
	LOCAL:2 += 150
	TIMES LOCAL:2 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL , 1.00
	TIMES LOCAL:2 , 0.50
