	SOURCE:19 = 1800
	SOURCE:22 = 100
	SOURCE:24 = 1800
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES SOURCE:12 , 0.80
ELSEIF ABL:21 == 1
	TIMES SOURCE:12 , 1.10
ELSEIF ABL:21 == 2
	TIMES SOURCE:12 , 1.30
ELSEIF ABL:21 == 3
	TIMES SOURCE:12 , 1.50
ELSEIF ABL:21 == 4
	TIMES SOURCE:12 , 1.70
ELSE
	TIMES SOURCE:12 , 2.10
ENDIF

;EXP:V경험을 본다
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:20 = 6500
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:20 = 2000
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:20 = 500
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:20 = 100
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:20 = 10
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:20 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:20 += 1000
	TIMES SOURCE:20 , 4.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:20 += 400
	TIMES SOURCE:20 , 1.00
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:20 , 0.50
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:20 , 0.20
ELSE
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:20 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:1 , 1.10
ELSE
	TIMES SOURCE:1 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.20
ELSE
	TIMES SOURCE:1 , 1.30
ENDIF

;비디오 카메라
SIF TEQUIP:50
	TIMES SOURCE:12 , 1.20
;수치 플레이
SIF TEQUIP:56
	TIMES SOURCE:12 , 1.20

;정조관념
SIF TALENT:30
	TIMES SOURCE:31 , 4.00
;도착적
IF TALENT:80
	TIMES SOURCE:12 , 1.50
	TIMES SOURCE:31 , 1.20
ENDIF
;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.50

IF FLAG:1100 & 1
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 8
	FLAG:1111 = 1
	FLAG:1112 = SOURCE:1
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:1 = FLAG:1120
	FLAG:1112 = SOURCE:12
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:12 = FLAG:1120
	FLAG:1112 = SOURCE:19
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:19 = FLAG:1120
	FLAG:1112 = SOURCE:22
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:22 = FLAG:1120
	FLAG:1112 = SOURCE:24
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:24 = FLAG:1120
	FLAG:1111 = 3
	FLAG:1112 = SOURCE:20
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:20 = FLAG:1120
ENDIF

;레이센이나 에이린에 의한 사용
IF ASSI >= 0 && ASSIPLAY
	SIF NO:ASSI == 28
		TIMES SOURCE:20 , 0.70
	SIF NO:ASSI == 29
		TIMES SOURCE:20 , 0.50
ENDIF

TCVAR:0 += 3

;커맨드 속성：도구(조교자)
TFLAG:98 = 1

;쿠스코의 착탈
IF TEQUIP:21
	TEQUIP:21 = 0
ELSE
	TEQUIP:21 = 1
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;--------------------------------------------------
;쿠스코 삽입중
;--------------------------------------------------
@EQUIP_COM150
TSTR:7 = 쿠스코
IF FLAG:1100 & 1
	;技師パッチ有効なら特殊な名前チェック
	FLAG:1110 = 8
	CALL NAMECHECK_CUSTOMTOOL
ENDIF
PRINTFORML <%TSTR:7% 삽입 중>

;V경험을 수반하는 커맨드의 플래그(처녀막 재생 대응)
TFLAG:1 = 1

DOWNBASE:0 += 10
DOWNBASE:1 += 50

;ABL:V감각을 본다
