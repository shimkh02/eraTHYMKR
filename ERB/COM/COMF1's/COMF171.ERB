@COM_ABLE171
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:171 > 0
	RETURN 0
;Ａ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4) == 0
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:25 == 4
	RETURN 1
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
	RETURN 0
;아이템을 가지고 있거나, 아이템 없음 설정이 아니면 불가
SIF ITEM:14 == 0 && NOITEM == 0
	RETURN 0
;조교 대상이 소인체형일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
	RETURN 0
;기계면 불가
SIF TALENT:206
	RETURN 0
;쿠스코 삽입 중에는 불가
SIF TEQUIP:21
	RETURN 0
;그밖의 Ａ계 장착구 사용 중에는 불가
SIF (TEQUIP:25 && TEQUIP:25 != 4) || TEQUIP:26
	RETURN 0
;욕실 플레이 중, ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:53 || TEQUIP:55
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM171
TSTR:7 = 전극
IF FLAG:1100 & 1
	;技師パッチ有効なら特殊な名前チェック
	FLAG:1110 = 14
	CALL NAMECHECK_CUSTOMTOOL
	IF TSTR:8 != "전극"
		PRINTFORML 애널 %TSTR:8% 
	ELSE
		PRINTL 애널 전극
	ENDIF 
ELSE
	TSTR:9 = %TSTR:7%
	PRINTL 애널 전극
ENDIF
TSTR:0 = 애널 전극
CALL TRAIN_MESSAGE_COM171

DOWNBASE:0 += 100
DOWNBASE:1 += 150

SOURCE:31 = 1200

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 200
	SOURCE:20 = 1000
	SOURCE:22 = 1000
ELSEIF ABL:2 == 1
	SOURCE:2 = 500
	SOURCE:20 = 1000
	SOURCE:22 = 2000
ELSEIF ABL:2 == 2
	SOURCE:2 = 900
	SOURCE:20 = 1000
	SOURCE:22 = 3000
ELSEIF ABL:2 == 3
	SOURCE:2 = 1700
	SOURCE:20 = 1200
	SOURCE:22 = 5000
ELSEIF ABL:2 == 4
	SOURCE:2 = 2200
	SOURCE:20 = 1500
	SOURCE:22 = 7000
ELSE
	SOURCE:2 = 3400
	SOURCE:20 = 1800
	SOURCE:22 = 11000
ENDIF

;EXP:A경험을 본다
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:20 += 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 300
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:20 += 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:20 += 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:20 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 120
ELSE
	TIMES SOURCE:2 , 1.80
	SOURCE:20 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSE
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF
;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3

IF FLAG:1100 & 1
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 14
	FLAG:1111 = 1
	FLAG:1112 = SOURCE:2
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:2 = FLAG:1120
	FLAG:1111 = 2
	FLAG:1112 = SOURCE:20
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:20 = FLAG:1120
	FLAG:1111 = 1
	FLAG:1112 = SOURCE:22
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:22 = FLAG:1120
ENDIF

TCVAR:1 += 5

;커맨드 속성：도구(조교자)
TFLAG:98 = 1

;전극의 착탈
IF TEQUIP:25
	TEQUIP:25 = 0
ELSE
	TEQUIP:25 = 4
ENDIF
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

