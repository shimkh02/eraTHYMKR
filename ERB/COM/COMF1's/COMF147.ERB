@COM_ABLE147
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:147 > 0
	RETURN 0
;Ｃ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 1) == 0
	RETURN 0
;이상계 조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 4 
	RETURN 0
;요도개발경험이 5이하일 경우 불가
SIF EXP:46 <= 5
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:32 == 5
	RETURN 1
;아이템을 가지고 있거나, 아이템 없음 설정이 아니면 불가
SIF ITEM:38 == 0 && NOITEM == 0
	RETURN 0
;남자일 경우, 기교3이상이 아니면 쓸 수 없다
SIF TALENT:120 && ABL:12 < 3
	RETURN 0
;ASSI일 경우, 기교4이상이 아니면 쓸 수 없다。
IF ASSIPLAY
	SIF ABL:ASSI:12 < 4
		RETURN 0
ENDIF
	;그밖의 Ｕ계 장착구 사용 중에는 불가
SIF (TEQUIP:32 && TEQUIP:32 != 5)
	RETURN 0
;남자일 경우, Ｃ계 장착구 사용 중에는 불가
SIF (TEQUIP:30 || TEQUIP:31) && TALENT:120
	RETURN 0
;욕실 플레이 중에는 플레이 매트가 없으면 불가
SIF TEQUIP:53 && ITEM:42 == 0 && NOITEM == 0
	RETURN 0
;샤워 중에는 불가
SIF TEQUIP:54
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;학교수영복 플레이 중에는 불가
SIF TEQUIP:59 == 2
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM147
PRINTL 요도 비즈
TSTR:0 = 요도비즈
CALL TRAIN_MESSAGE_COM147

;-------------------------------------------------
;체력, 기력의 계산
;-------------------------------------------------
LOCAL:1 = 60

;EXP:U확장 경험을 본다
IF EXP:34 < EXPLV:1
	TIMES LOCAL:1 , 3.00
ELSEIF EXP:34 < EXPLV:2
	TIMES LOCAL:1 , 2.50
ELSEIF EXP:34 < EXPLV:3
	TIMES LOCAL:1 , 2.00
ELSEIF EXP:34 < EXPLV:4
	TIMES LOCAL:1 , 1.00
ELSEIF EXP:34 < EXPLV:5
	TIMES LOCAL:1 , 0.80
ELSE
	TIMES LOCAL:1 , 0.60
ENDIF

;겁쟁이
SIF TALENT:10
	TIMES LOCAL:1 , 2.00
;감정결여
SIF TALENT:22
	TIMES LOCAL:1 , 0.60
;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:1 , 2.00
;요도광
SIF TALENT:175
	TIMES LOCAL:1 , 0.90

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:1 * 2
;-------------------------------------------------
;소스의 계산
;-------------------------------------------------
;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 100
	SOURCE:22 = 600
ELSEIF ABL:0 == 1
	SOURCE:0 = 400
	SOURCE:22 = 900
ELSEIF ABL:0 == 2
	SOURCE:0 = 750
	SOURCE:22 = 1200
ELSEIF ABL:0 == 3
	SOURCE:0 = 1200
	SOURCE:22 = 1800
ELSEIF ABL:0 == 4
	SOURCE:0 = 1500
	SOURCE:22 = 2400
ELSE
	SOURCE:0 = 2000
	SOURCE:22 = 3000
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	SOURCE:20 = 2000
	SOURCE:22 += 400
	SOURCE:30 = 2000
	SOURCE:31 = 1500
	SOURCE:32 = 2000
ELSEIF ABL:21 == 1
	SOURCE:20 = 1600
	SOURCE:22 += 700
	SOURCE:30 = 1500
	SOURCE:31 = 1500
	SOURCE:32 = 1000
ELSEIF ABL:21 == 2
	SOURCE:20 = 1200
	SOURCE:22 += 900
	SOURCE:30 = 1000
	SOURCE:31 = 1000
	SOURCE:32 = 500
ELSEIF ABL:21 == 3
	SOURCE:20 = 800
	SOURCE:22 += 1400
	SOURCE:30 = 500
	SOURCE:31 = 500
	SOURCE:32 = 100
ELSEIF ABL:21 == 4
	SOURCE:20 = 600
	SOURCE:22 += 1800
	SOURCE:30 = 250
	SOURCE:31 = 100
	SOURCE:32 = 0
ELSE
	SOURCE:20 = 400
	SOURCE:22 += 2500
	SOURCE:30 = 100
	SOURCE:31 = 25
	SOURCE:32 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:0 , 0.40
	SOURCE:20 += 2000
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:0 , 0.80
	SOURCE:20 += 1500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	SOURCE:20 += 1000
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:0 , 1.40
	SOURCE:20 += 500
ELSE
	TIMES SOURCE:0 , 1.80
	SOURCE:20 += 250
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:0 , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:0 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:0 , 1.10
ELSE
	TIMES SOURCE:0 , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:0 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:0 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:0 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:0 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:0 , 1.20
ELSE
	TIMES SOURCE:0 , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:128
	TIMES SOURCE:20 , 2.00
;남자 또는 후타나리
SIF (TALENT:120 || TALENT:121)
	TIMES SOURCE:20 , 1.50
;요도광
SIF TALENT:175
	TIMES SOURCE:0 , 1.30

;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:101
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:100
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3

;요도 비즈의 착탈
IF TEQUIP:32 == 5
	TEQUIP:32 = 0
	LOCAL:2 = 2
	IF TEQUIP:12
		LOCAL:2 *= 2
		TEQUIP:12 = 0
	ENDIF
ELSE
	TEQUIP:32 = 5
	LOCAL:2 = 1
ENDIF

;방뇨 레벨 증가
CFLAG:31 += LOCAL:2
SIF BASE:4
	BASE:4 = 0
TCVAR:46 += LOCAL:2
TCVAR:34 += 1
SIF (EXP:46 + TCVAR:46) <= 5
	TCVAR:50 += 1
	
;커맨드 속성：도구(조교자)
TFLAG:98 = 1
;촉수 경험 플래그
TFLAG:90 = 0
RETURN 1

;--------------------------------------------------
;요도비즈 삽입중
;--------------------------------------------------
@EQUIP_COM147
PRINTL <요도 비즈 삽입 중>

LOCAL:3 = 80

;EXP:U확장 경험을 본다
IF EXP:34 < EXPLV:1
	TIMES LOCAL:3 , 3.00
ELSEIF EXP:34 < EXPLV:2
	TIMES LOCAL:3 , 2.50
ELSEIF EXP:34 < EXPLV:3
	TIMES LOCAL:3 , 2.00
ELSEIF EXP:34 < EXPLV:4
	TIMES LOCAL:3 , 1.00
ELSEIF EXP:34 < EXPLV:5
	TIMES LOCAL:3 , 0.80
ELSE
	TIMES LOCAL:3 , 0.60
ENDIF	

;겁쟁이
SIF TALENT:10
	TIMES LOCAL:3 , 2.00
;감정결여
SIF TALENT:22
	TIMES LOCAL:3 , 0.60
;요도광
SIF TALENT:175
	TIMES LOCAL:3 , 0.90

DOWNBASE:0 += LOCAL:3
DOWNBASE:1 += LOCAL:3 * 2

;ABL:C감각을 본다
IF ABL:0 == 0
	LOCAL = 80
	LOCAL:1 = 300
ELSEIF ABL:0 == 1
	LOCAL = 250
	LOCAL:1 = 800
ELSEIF ABL:0 == 2
	LOCAL = 600
	LOCAL:1 = 1400
ELSEIF ABL:0 == 3
	LOCAL = 1000
	LOCAL:1 = 1800
ELSEIF ABL:0 == 4
	LOCAL = 1300
	LOCAL:1 = 2100
ELSE
	LOCAL = 1700
	LOCAL:1 = 2400
ENDIF

;EXP:요도 경험을 본다
IF EXP:46 < EXPLV:1
	TIMES LOCAL , 0.50
	LOCAL:2 = 2000
ELSEIF EXP:46 < EXPLV:2
	TIMES LOCAL , 1.00
	LOCAL:2 = 300
ELSEIF EXP:46 < EXPLV:3
	TIMES LOCAL , 1.10
	LOCAL:2 = 50
ELSEIF EXP:46 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 10
ELSEIF EXP:46 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES LOCAL , 0.40
	LOCAL:2 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES LOCAL , 0.80
	LOCAL:2 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES LOCAL , 1.40
	LOCAL:2 += 120
ELSE
	TIMES LOCAL , 1.80
	LOCAL:2 += 100
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:10 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:10 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:10 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:10 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;남자 또는 후타나리
SIF (TALENT:120 || TALENT:121)
	TIMES LOCAL:2 , 2.50
;작은 몸집 체형
SIF TALENT:128
	TIMES LOCAL:2 , 2.00
;요도광
SIF TALENT:175
	TIMES LOCAL , 1.20

;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:101
	TIMES LOCAL , 1.50
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.50
ELSEIF TALENT:100
	TIMES LOCAL , 0.60
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
ENDIF

;처녀로 정조관념
SIF TALENT:0 == 1 && TALENT:30
	LOCAL:1 /= 3

SOURCE:0 += LOCAL
SOURCE:22 += LOCAL:1
SOURCE:20 += LOCAL:2
SOURCE:31 += LOCAL:1

;요도비즈 삽입중 때는 강제적으로 뇨의를 개최되는 효과도(=방뇨 레벨 증가)
IF TEQUIP:12
	CFLAG:31 += 2
ELSE
	CFLAG:31 += 1
ENDIF

CALL EQUIP_MESSAGE_COM147

TCVAR:46 += 3
TCVAR:34 += 1

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF
;촉수 경험 플래그
SIF TEQUIP:90
	TFLAG:90 += 2
RETURN 1
;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@TRAIN_MESSAGE_COM147
CALL KOJO_TRAIN_MESSAGE_COM
SIF FLAG:10 == 0 || CFLAG:99 == 1 || RESULT != 0
	RETURN 0
	
PRINTL
IF TEQUIP:32 == 5
	PRINTFORM %CALLNAME:TARGET%의 요도를 채운 비즈를 힘차게 뽑자, 뜨거운 물보라\@((TALENT:120 || TALENT:121) && NOWEX:11 > 0) ? 가 # 와 백탁의 점액이\@ 포물선을 그리며 뿜어 나왔다.
	PRINTL

	IF ABL:14 >= 5 && TALENT:45
		PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 격렬한 쾌감과 해방감에 몸을 뒤로 젖힌 채 부들부들 떨고 있었다.
		IF (TALENT:120 || TALENT:121)
			PRINTFORMW %CALLNAME:TARGET%의 페니스는 요도를 유린당한 쾌감에 부들부들 떨면서도 마치 더 괴롭혀 달라는듯 그 구멍을 헤~ 벌리고 있었다….
		ELSE
			PRINTFORMW %CALLNAME:TARGET%의 요도는 한동안 제 모습을 되찾지 못한 채 벌려져 그저 되는대로 오줌을 흘리고 있었다….
		ENDIF
	ELSEIF ABL:14 >= 4
		PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 비즈가 뽑힌 쾌감에 넋을 잃은 표정으로, 아직도 퓩퓩 하고 오줌을 싸고 있는 자신의 요도구를 응시하고 있다.
	ELSEIF ABL:14 == 3
		PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 부끄러움보다 해방감 쪽이 컸는지, 안심한 표정을 짓고 있다.
	ELSEIF ABL:14 >= 1
		PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 몸을 기역자로 꺾은 채로, 아직 남아있는 잔뇨감에 번민하고 있다….
	ELSE
		PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 고통에 어금니를 꽉 깨물고 치욕감에 빨개진 얼굴을 돌려버렸다.
	ENDIF
ELSE
	PRINTFORML %CALLNAME:TARGET%의 요도구에 실로 이어진 구슬들를 정성스레 삽입했다.
	PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 요도를 가득 채우는 비즈의 감촉에 절규하고 있다….
ENDIF
