ENDIF

SOURCE:12 += LOCAL
SOURCE:17 += LOCAL /2
SOURCE:25 += LOCAL:1 /2
SOURCE:31 += LOCAL:1

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 1
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 1
ENDIF

;노출 쾌락 경험 입수 판정
TFLAG:100 |= 2

$PASSOUT_103
;주인 경험 플래그
SIF ASSIPLAY == 0 && ABL:14 >= 3
	TFLAG:50 += 1

RETURN 1

