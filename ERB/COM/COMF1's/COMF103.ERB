;………………………………………………
;실행 판정
;………………………………………………
@COM_ABLE103
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:103 > 0
	RETURN 0
;수치 플레이계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 512) == 0
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:56
	RETURN 1
;조교자가 에이키 님でないか, キャラ확장커맨드OFFの場合には아이템필수
IF NO:PLAYER != 36 || (FLAG:15 & 16384) == 0
	;욕실 플레이中なら아이템「風呂場用大鏡」が필요
	SIF TEQUIP:53 && ITEM:49 == 0 && NOITEM == 0
		RETURN 0
	;욕실 플레이中でなければ아이템｢大鏡｣を持っているか, 아이템なし設定でないとダメ
	SIF TEQUIP:53 == 0 && ITEM:40 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;순종이 1이상 없으면 안 됨
SIF ABL:10 < 1
	RETURN 0
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;蜘蛛の巣にかけているときは移動できない
SIF TEQUIP:40 == 7
	RETURN 0
;야외 플레이 중에는 불가
SIF TEQUIP:52
	RETURN 0
;○○목욕탕 입욕 중에는 불가
SIF TEQUIP:55
	RETURN 0
;의사 선생님 플레이 중에는 불가
SIF TEQUIP:57
	RETURN 0
;알몸 앞치마 중에는 불가
SIF TEQUIP:59 == 1
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM103
;조교자가 에이키 님으로 콘피그의 설정 「정파리의 거울」이 유효 또는 정파리의 거울 사용중의 경우, 정파리의 거울에 변화.
SIF (NO:PLAYER == 36 && FLAG:15 & 16384) || TEQUIP:56 == 2
	JUMP COM650

PRINTL 수치 플레이
TSTR:0 = 수치 플레이
CALL TRAIN_MESSAGE_COM103

;거울세트
;종료시는 수정 없음
IF TEQUIP:56
	TEQUIP:56 = 0
	RETURN 1
ENDIF

DOWNBASE:0 += 0
DOWNBASE:1 += 200

LOCAL:1 = 500
LOCAL:2 = 500

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL:1 , 1.10
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL:1 , 1.20
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL:1 , 1.30
ELSE
	TIMES LOCAL:1 , 1.40
ENDIF

;ABL:노출벽을 본다
IF ABL:14 == 0
	SOURCE:24 += 0
	SOURCE:19 += 0
	SOURCE:22 += 600
	SOURCE:31 += 1000
	SOURCE:32 += 1000
	TIMES LOCAL:1 , 0.60
ELSEIF ABL:14 == 1
	SOURCE:24 += 150
	SOURCE:19 += 150
	SOURCE:22 += 500
	TIMES LOCAL:1 , 1.00
ELSEIF ABL:14 == 2
	SOURCE:24 += 300
	SOURCE:19 += 300
	SOURCE:22 += 400
	TIMES LOCAL:1 , 1.60
ELSEIF ABL:14 == 3
	SOURCE:24 += 600
	SOURCE:19 += 600
	SOURCE:22 += 300
	TIMES LOCAL:1 , 2.00
ELSEIF ABL:14 == 4
	SOURCE:24 += 1000
	SOURCE:19 += 1000
	SOURCE:22 += 200
	TIMES LOCAL:1 , 2.60
ELSE
	SOURCE:24 += 1800
	SOURCE:19 += 1800
	SOURCE:22 += 100
	TIMES LOCAL:1 , 3.80
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	TIMES LOCAL:1 , 1.00
ELSEIF ABL:21 == 1
	TIMES LOCAL:1 , 1.20
ELSEIF ABL:21 == 2
	TIMES LOCAL:1 , 1.40
ELSEIF ABL:21 == 3
	TIMES LOCAL:1 , 1.60
ELSEIF ABL:21 == 4
	TIMES LOCAL:1 , 1.80
ELSE
	TIMES LOCAL:1 , 2.00
ENDIF

;겁쟁이
SIF TALENT:10
	TIMES LOCAL:2 , 1.50
;무관심
SIF TALENT:21
	TIMES LOCAL:1 , 0.80
;감정결여
SIF TALENT:22
	TIMES LOCAL:1 , 0.80
;호기심
SIF TALENT:23
	TIMES LOCAL:1 , 2.50
;해방
SIF TALENT:33
	TIMES LOCAL:1 , 1.50
;부끄럼쟁이
SIF TALENT:35
	TIMES LOCAL:2 , 3.00
;도착적
IF TALENT:80
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.20
ENDIF
;매력
IF TALENT:90
	SOURCE:11 += 500
	SOURCE:17 += 500
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.20
ENDIF

SOURCE:12 += LOCAL:1
SOURCE:17 += LOCAL:1 /2
SOURCE:25 += LOCAL:2 /2
SOURCE:28 += LOCAL:1 /2
SOURCE:31 += LOCAL:2
TEQUIP:56 = 1
RETURN 1

