;빨래집게(유두)
;-------------------------------------------------
@EQUIP_COM195
PRINTL <빨래집게(유두)>

DOWNBASE:0 += 40
DOWNBASE:1 += 10

;ABL:B감각을 본다
IF ABL:3 == 0
	LOCAL = 500
ELSEIF ABL:3 == 1
	LOCAL = 700
ELSEIF ABL:3 == 2
	LOCAL = 900
ELSEIF ABL:3 == 3
	LOCAL = 1100
ELSEIF ABL:3 == 4
	LOCAL = 1300
ELSE
	LOCAL = 1800
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	LOCAL:1 = 0
ELSEIF ABL:21 == 1
	LOCAL:1 = 140
ELSEIF ABL:21 == 2
	LOCAL:1 = 280
ELSEIF ABL:21 == 3
	LOCAL:1 = 560
ELSEIF ABL:21 == 4
	LOCAL:1 = 1120
ELSE
	LOCAL:1 = 2240
ENDIF

;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES LOCAL , 1.40
	TIMES LOCAL:1 , 0.20
ELSEIF PALAM:13 < PALAMLV:2
	TIMES LOCAL , 1.10
	TIMES LOCAL:1 , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES LOCAL , 1.00
	TIMES LOCAL:1 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES LOCAL , 0.90
	TIMES LOCAL:1 , 1.10
ELSE
	TIMES LOCAL , 0.70
	TIMES LOCAL:1 , 1.30
ENDIF

;도착적
IF TALENT:80
	TIMES LOCAL , 0.80
	TIMES LOCAL:1 , 1.50
ENDIF
;마조
SIF TALENT:82
	TIMES LOCAL:1 , 1.20
;B민감
IF TALENT:106
	TIMES LOCAL , 2.00
;B둔감
ELSEIF TALENT:107
	TIMES LOCAL , 0.75
ENDIF
;왕M
SIF TALENT:178
	TIMES LOCAL:1 , 1.50

IF FLAG:1100 & 1
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 36
	FLAG:1111 = 3
	FLAG:1112 = LOCAL
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL = FLAG:1120
	FLAG:1111 = 1
	FLAG:1112 = LOCAL:1
	CALL SOURCECHECK_CUSTOMTOOL
	LOCAL:1 = FLAG:1120
ENDIF

SOURCE:3 += (LOCAL+LOCAL:1)/10
SOURCE:18 += LOCAL:1
SOURCE:20 += LOCAL
SOURCE:22 += (LOCAL+LOCAL:1)/2
SOURCE:31 += LOCAL+LOCAL:1

RETURN 1

