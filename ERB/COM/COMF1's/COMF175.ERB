@COM_ABLE175
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:175 > 0
	RETURN 0
;Ａ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4) == 0
	RETURN 0
;Ａ조교 필터가 오프되어 있지 않으면 불가
SIF FLAG:2 & 2
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용 중일 경우는 안 됨
SIF TEQUIP:59 == 17
	RETURN 0
;촉수 조교 중에는 불가
SIF TEQUIP:90
	RETURN 0
;해제는 언제든 가능
SIF TEQUIP:25 == 8
	RETURN 1
;아이템을 가지고 있거나, 아이템 없음 설정이 아니면 불가
SIF ITEM:12 == 0 && NOITEM == 0
	RETURN 0
;조교 대상이 소인체형일 경우에는 주인이 금단의 지식 보유자가 아니면 불가
SIF TALENT:127 && TALENT:MASTER:56 == 0
	RETURN 0
;기계면 불가
SIF TALENT:206
	RETURN 0
;Ａ경험이 50미만이면 못 함
SIF EXP:1 < 50
	RETURN 0
;노출증, 욕망, 순종의 합계가 10 안 되면 못 함
SIF ABL:10 + ABL:11 + ABL:14 < 10
	RETURN 0
;쿠스코 삽입 중에는 불가
SIF TEQUIP:21
	RETURN 0
;그밖의 Ａ계 장착구 사용 중에는 불가
SIF (TEQUIP:25 && TEQUIP:25 != 8) || TEQUIP:26
	RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;시체는 안됨
SIF TALENT:226 == 1
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
@COM175
PRINTL 공기 관장
TSTR:0 = 공기 관장
CALL TRAIN_MESSAGE_COM175

DOWNBASE:0 += 200
DOWNBASE:1 += 300

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 0
	SOURCE:22 = 300
ELSEIF ABL:2 == 1
	SOURCE:2 = 10
	SOURCE:22 = 800
ELSEIF ABL:2 == 2
	SOURCE:2 = 450
	SOURCE:22 = 1400
ELSEIF ABL:2 == 3
	SOURCE:2 = 80
	SOURCE:22 = 1800
ELSEIF ABL:2 == 4
	SOURCE:2 = 120
	SOURCE:22 = 2100
ELSE
	SOURCE:2 = 160
	SOURCE:22 = 2400
ENDIF

;ABL:마조끼를 본다
IF ABL:21 == 0
	SOURCE:20 = 2000
	SOURCE:22 += 200
	SOURCE:25 = 8000
	SOURCE:30 = 8000
	SOURCE:31 = 1000
	SOURCE:32 = 2000
ELSEIF ABL:21 == 1
	SOURCE:20 = 2000
	SOURCE:22 += 500
	SOURCE:25 = 5000
	SOURCE:30 = 6000
	SOURCE:31 = 1000
	SOURCE:32 = 1000
ELSEIF ABL:21 == 2
	SOURCE:20 = 2000
	SOURCE:22 += 800
	SOURCE:25 = 3000
	SOURCE:30 = 5000
	SOURCE:31 = 1000
	SOURCE:32 = 500
ELSEIF ABL:21 == 3
	SOURCE:20 = 1800
	SOURCE:22 += 1200
	SOURCE:25 = 1200
	SOURCE:30 = 4000
	SOURCE:31 = 1000
	SOURCE:32 = 100
ELSEIF ABL:21 == 4
	SOURCE:20 = 1800
	SOURCE:22 += 1500
	SOURCE:25 = 500
	SOURCE:30 = 3000
	SOURCE:31 = 1000
	SOURCE:32 = 0
ELSE
	SOURCE:20 = 1800
	SOURCE:22 += 2000
	SOURCE:25 = 200
	SOURCE:30 = 2000
	SOURCE:31 = 1000
	SOURCE:32 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:20 += 800
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:20 += 500
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:20 += 300
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:20 += 120
ELSE
	TIMES SOURCE:2 , 1.80
	SOURCE:20 += 100
ENDIF

;ABL:순종을 본다
IF ABL:10 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:10 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:10 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:10 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

IF FLAG:1100 & 1
	;技師パッチ有効なら道具使用時ソースチェックを呼ぶ
	;FLAG 1110:道具番号 1111:改善タイプ 1112:SOURCEの実値 
	FLAG:1110 = 12
	;ここまでにNAMECHECKしてないため開発者未定。代わりにCHECK_DEALER_CUSTOMTOOLを呼ぶ
	CALL CHECK_DEALER_CUSTOMTOOL
	FLAG:1111 = 1
	FLAG:1112 = SOURCE:2
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:2 = FLAG:1120
	FLAG:1112 = SOURCE:22
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:22 = FLAG:1120
	FLAG:1112 = SOURCE:31
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:31 = FLAG:1120
	FLAG:1111 = 2
	FLAG:1112 = SOURCE:20
	CALL SOURCECHECK_CUSTOMTOOL
	SOURCE:20 = FLAG:1120
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

;관장 플러그의 착탈
IF TEQUIP:25
	SOURCE:22 += 1000
	SOURCE:28 += 2000
	TEQUIP:25 = 0
ELSE
	SOURCE:20 += 1500
	SOURCE:22 += 2500
	TEQUIP:25 = 8
ENDIF

TCVAR:1 += 1

;커맨드 속성：도구(조교자)
TFLAG:98 = 1

RETURN 1

