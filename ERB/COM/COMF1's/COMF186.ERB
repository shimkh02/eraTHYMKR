@COM_ABLE186
;개별 설정 커맨드 필터
SIF DITEMTYPE:999:186 > 0
	RETURN 0
;Ａ계 확장 커맨드를 유효로 하지 않으면 불가
SIF (FLAG:15 & 4) == 0
	RETURN 0
;그 조교 중에 관장 플레이＋배설이 있었을 경우, 조교자가 汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:61 == 0 && TALENT:PLAYER:64 == 0
	RETURN 0
;수면 중에는 불가
SIF EQUIP:14
	RETURN 0
;실신 중에는 불가
SIF TFLAG:899 > 0
	RETURN 0
;승마 중에는 불가
SIF TEQUIP:43
	RETURN 0
;한증탕과 암반욕 외의 ○○ 욕탕 입욕 중에는 불가
SIF TEQUIP:55 && TEQUIP:55 != 12 && TEQUIP:55 != 15
	RETURN 0
;기저귀 플레이 중에는 불가
SIF TEQUIP:58
	RETURN 0
;훈도시 착용 중일 경우 조교자의 기교4이상 필수
SIF TEQUIP:59 == 17 && ABL:PLAYER:12 < 4
	RETURN 0
;나체정식 중에는 불가
SIF TEQUIP:60
	RETURN 0
;시간정지 중에는 무리
SIF TEQUIP:71
	RETURN 0
;촉수 조교중일 시 주인의 촉수사역Lv2 이하는 안 됨
SIF TEQUIP:90 && ABL:MASTER:98 <= 2
	RETURN 0
RETURN 1

;--------------------------------------------------
;커맨드 본체
;--------------------------------------------------
;애널 목적이 아니라 어디까지나 엉덩이를 어루만지고 돌린다
@COM186
PRINTL 엉덩이 애무
TSTR:0 = 엉덩이 애무
CALL TRAIN_MESSAGE_COM186

DOWNBASE:0 += 15
DOWNBASE:1 += 80

SOURCE:12 = 700
SOURCE:15 = 30
SOURCE:31 = 300
SOURCE:10 = 300

;ABL:C감각을 본다
IF ABL:0 == 0
	SOURCE:0 = 10
	SOURCE:11 = 10
ELSEIF ABL:0 == 1
	SOURCE:0 = 50
	SOURCE:11 = 20
ELSEIF ABL:0 == 2
	SOURCE:0 = 200
	SOURCE:11 = 32
ELSEIF ABL:0 == 3
	SOURCE:0 = 400
	SOURCE:11 = 40
ELSEIF ABL:0 == 4
	SOURCE:0 = 650
	SOURCE:11 = 46
ELSE
	SOURCE:0 = 950
	SOURCE:11 = 50
ENDIF

;ABL:A감각을 본다
IF ABL:2 == 0
	SOURCE:2 = 10
	SOURCE:22 = 200
ELSEIF ABL:2 == 1
	SOURCE:2 = 50
	SOURCE:22 = 250
ELSEIF ABL:2 == 2
	SOURCE:2 = 200
	SOURCE:22 = 300
ELSEIF ABL:2 == 3
	SOURCE:2 = 400
	SOURCE:22 = 550
ELSEIF ABL:2 == 4
	SOURCE:2 = 650
	SOURCE:22 = 900
ELSE
	SOURCE:2 = 950
	SOURCE:22 = 1400
ENDIF


;PALAM:욕정을 본다
IF PALAM:13 < PALAMLV:1
	TIMES SOURCE:2 , 0.30
	TIMES SOURCE:22 , 0.30
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:22 , 0.60
ELSEIF PALAM:13 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:22 , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:22 , 1.30
ELSE
	TIMES SOURCE:2 , 1.60
	TIMES SOURCE:22 , 1.60
ENDIF

;A민감, 둔감을 본다
;쾌A자체의 체크는 다음에 정리해 준다
IF TALENT:105
	TIMES SOURCE:20 , 1.50
	TIMES SOURCE:22 , 1.50
	TIMES SOURCE:31 , 1.50
ELSEIF TALENT:104
	TIMES SOURCE:20 , 0.60
	TIMES SOURCE:22 , 0.60
	TIMES SOURCE:31 , 0.60
ENDIF

IF TEQUIP:59 == 2
	;학교수영복 장착중
	TIMES SOURCE:10 , 3.00
	TIMES SOURCE:12 , 0.80
ENDIF


;처녀로 정조관념
IF TALENT:0 == 1 && TALENT:30
	SOURCE:22 /= 3
ELSEIF TALENT:0 && TALENT:30
	SOURCE:22 /= 2
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	TCVAR:12 += 5
ELSEIF TALENT:120 && TALENT:PLAYER:120
	TCVAR:13 += 5
ENDIF
RETURN 1

