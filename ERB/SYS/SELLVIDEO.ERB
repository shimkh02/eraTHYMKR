;ビデオのボーナス
IF FLAG:15 & 128
	LOCAL = VIDEO_EDIT_BONUS(LOCAL)
ELSE
	TIMES LOCAL , 1.50
ENDIF

;토리코모드 보정
IF FLAG:5000 & 2
	TIMES LOCAL , 0.2
	LOCAL = MIN(LOCAL, 700)
ENDIF


;HARD 모드 이상
SIF FLAG:3 >= 3 && LOCAL > 75000
	LOCAL = 75000
