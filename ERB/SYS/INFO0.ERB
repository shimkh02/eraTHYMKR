;-------------------------------------------------
;주인 · 노예 · 조수의 사정 게이지 계산 처리
;-------------------------------------------------
@CALC_GAUGE2_COMMON(ARG)
#FUNCTION
LOCAL = (TALENT:ARG:120) ? 10000 # 20000
IF TALENT:ARG:120 && TALENT:ARG:132
	LOCAL /= 2
ELSEIF TALENT:ARG:132
	TIMES LOCAL , 0.75
ELSEIF TALENT:ARG:133
	LOCAL *= 2
ENDIF
SIF TALENT:ARG:171
	LOCAL /= 2
RETURNF LOCAL

