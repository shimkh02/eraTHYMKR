	TFLAG:202 = 0
	PRINTFORML %CALLNAME:TARGET%의 사진을 팔았다.
	MONEY:1 = TFLAG:124
	CALL SHOW_MONEY_INCREACE
	;--------------------------------------------------
	;사진으로 번 최고 금액 판정 처리
	SIF MONEY:1 > MONEY:971
