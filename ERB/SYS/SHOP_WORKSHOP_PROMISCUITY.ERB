;互いの技巧に応じて双方の絶頂回数を変動
;挿入される側はＡ感覚でも変動
SELECTCASE ABL:(ARG:1):12
	CASE 0 TO 1
		;技巧不足、絶頂に届かない
		LOCAL:0 = ABL:(ARG:0):2 / 2
		LOCAL:1 = 0
	CASE 2 TO 3
		LOCAL:0 = ABL:(ARG:0):2 + 1
		LOCAL:1 = 1
	CASEELSE
		LOCAL:0 = ABL:(ARG:0):2 + 1
		TIMES LOCAL:0, 1.50
		LOCAL:1 = 2
ENDSELECT
SELECTCASE ABL:(ARG:0):12
	CASE 0 TO 1
		;技巧不足、絶頂に届かない
		LOCAL:0 += 0
		LOCAL:1 += ABL:(ARG:1):0 / 2
	CASE 2 TO 3
		LOCAL:0 += 1
		LOCAL:1 += ABL:(ARG:1):0 + 1
	CASEELSE
		LOCAL:0 += 2
		LOCAL:1 += ABL:(ARG:1):0 + 1
		TIMES LOCAL:1, 1.50
ENDSELECT
IF TALENT:(ARG:1):120 || TALENT:(ARG:1):121
	;挿入側が射精可能であれば射精中毒・遅漏・早漏にも応じて挿入側の射精経験と絶頂経験を同数増加
	SELECTCASE ABL:(ARG:1):37
		CASE 0
			;中毒じゃない以上変動もなし
			LOCAL:1 += 0
		CASE 1 TO 2
			;1回保障
			LOCAL:1 += 1
		CASE 3 TO 4
			;1回保障のうえで1.5倍
			LOCAL:1 += 1
			TIMES LOCAL:1, 1.50
		CASEELSE
			;2回保障のうえで1.5倍
			LOCAL:1 += 2
			TIMES LOCAL:1, 1.50
	ENDSELECT
	;早漏・遅漏
	IF TALENT:(ARG:1):132
		;さらに1.5倍(中毒じゃなく技巧不足なら0のまま)
		TIMES LOCAL:1, 1.50
	ELSEIF TALENT:(ARG:1):133
		;射精回数1割減
		TIMES LOCAL:1, 0.90
	ENDIF
	LOCAL@VALUE_WORKSHOP_EXP_03:(ARG:1) += LOCAL:1
	;精液経験は射精回数の3倍
	LOCAL@VALUE_WORKSHOP_EXP_04:(ARG:0) += LOCAL:1 * 3
ELSE
	;肥大陰核なら射精による絶頂限界がない分絶頂回数が増加
	TIMES LOCAL:1, 1.50
ENDIF
LOCAL@VALUE_WORKSHOP_EXP_02:(ARG:1) += LOCAL:1
;Ａ経験は挿入する側の絶頂回数に等しい
LOCAL@VALUE_WORKSHOP_EXP_01:(ARG:0) += LOCAL:1
;挿入される側の絶頂回数に応じて挿入する側の習得の珠と挿入される側の快Ａの珠が増加
SELECTCASE LOCAL:0
	CASE 0
