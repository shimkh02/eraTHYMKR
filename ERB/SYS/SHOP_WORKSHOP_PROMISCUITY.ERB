	IF LOCAL@VALUE_WORKSHOP_RESULT_PROMISCUITY:(ARG:0) == -1
		LOCAL@VALUE_WORKSHOP_RESULT_PROMISCUITY:(ARG:0) = ARG:1
	ELSEIF LOCAL@VALUE_WORKSHOP_RESULT_PROMISCUITY:(ARG:0) == ARG:1
