;タイプ1 主人棒あり＞相手Ｖあり
;タイプ2 主人棒あり＞相手Ｖなし
;タイプ3 主人棒なしＶあり＞相手棒あり
;タイプ4 主人棒なしＶあり＞相手棒なしＶあり
@SELF_CHECK2
VARSET LOCAL, 0
;前提条件
;恋慕がないとダメ
SIF TALENT:150 == 0
	RETURN 0
;体力必要
SIF BASE:0 < 500
	RETURN 0
;時間も必要
SIF TFLAG:107 > 100
	RETURN 0
;ここでタイプ分け
IF TALENT:MASTER:120 || TALENT:MASTER:121
	;タイプ1
	IF TALENT:120 == 0
		;非処女じゃないとダメ
		SIF TALENT:0 || EXP:0 <= 0
			RETURN 0

		;Ｖ感覚
		IF ABL:1 == 0
			LOCAL = 0
		ELSEIF ABL:1 == 1
			LOCAL = 1
		ELSEIF ABL:1 <= 3
			LOCAL = 2
		ELSE
			LOCAL = 3
		ENDIF

		;欲望と奉仕精神によるボーナス
		IF ABL:13 >= 4 && PALAM:13 >= PALAMLV:4
			IF ABL:11 >= 5
				LOCAL += 2
			ELSEIF ABL:11 == 4
				LOCAL += 1
			ELSEIF ABL:11 == 3 && ABL:13 >= 5
				LOCAL += 1
			ENDIF
		ENDIF
		;快感に素直, 快感の否定
		IF TALENT:70 && LOCAL
			LOCAL += 1
		ELSEIF TALENT:71 && LOCAL
			LOCAL -= 2
		ENDIF
		;淫乱
		SIF TALENT:170 && LOCAL
			LOCAL += 1
		;ある程度時間が経過している
		IF TFLAG:107 > 50 && LOCAL
			LOCAL -= 1
		;殆ど時間が経過していない
		ELSEIF TFLAG:107 < 10 && LOCAL
			LOCAL += 1
		ENDIF

		;条件を満たさない場合は処理をここで終了させる
		SIF LOCAL < 1
			RETURN 0

		;表現のみ制限(処理は変わらず)
		TFLAG:226 = LOCAL
		TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
		TFLAG:226 = MIN(TFLAG:226, 10)

		DRAWLINE
		;地の文章カット
		IF CFLAG:99 == 0
			PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
			PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
		ENDIF

		;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
		CALL SELF_KOJO, TARGET, 150, 1

		;Ｖ経験
		CALL COMMON_UP_EXP,TARGET, 0, TFLAG:226, 0
		CFLAG:173 += TFLAG:226
		;Ｖ調律経験
		CALL COMMON_UP_EXP,TARGET, 42, TFLAG:226, 0
		;快Ｖ의 구슬
		CALL COMMON_UP_JUEL, TARGET, 1, (LOCAL*200), 0
		;恭順의 구슬
		CALL COMMON_UP_JUEL, TARGET, 12, (LOCAL*100), 0
		;欲情의 구슬
		CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*250), 1
		;Ｖ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
		IF (ABL:1 + ABL:10 + ABL:13) >= 13 && CFLAG:99 == 0
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 아직 미련이 남은 듯 옷자락을 잡아끌었다.
			PRINTFORMW 하지만 %조사처리(CALLNAME:MASTER,"는")% 그 손을 뿌리치고, 방을 뒤로 했다….
		ENDIF
	;タイプ2
	ELSE
		;Ａ経験ないとダメ
		SIF EXP:1 <= 0
			RETURN 0

		;Ａ感覚
		IF ABL:2 == 0
			LOCAL = 0
		ELSEIF ABL:2 == 1
			LOCAL = 1
		ELSEIF ABL:2 <= 3
			LOCAL = 2
		ELSE
			LOCAL = 3
		ENDIF

		;欲望と奉仕精神によるボーナス
		IF ABL:13 >= 4 && PALAM:13 >= PALAMLV:4
			IF ABL:11 >= 5
				LOCAL += 2
			ELSEIF ABL:11 == 4
				LOCAL += 1
			ELSEIF ABL:11 == 3 && ABL:13 >= 5
				LOCAL += 1
			ENDIF
		ENDIF
		;快感に素直, 快感の否定
		IF TALENT:70 && LOCAL
			LOCAL += 1
		ELSEIF TALENT:71 && LOCAL
			LOCAL -= 2
		ENDIF
		;淫乱
		SIF TALENT:170 && LOCAL
			LOCAL += 1
		;ある程度時間が経過している
		IF TFLAG:107 > 50 && LOCAL
			LOCAL -= 1
		;殆ど時間が経過していない
		ELSEIF TFLAG:107 < 10 && LOCAL
			LOCAL += 1
		ENDIF

		;条件を満たさない場合は処理をここで終了させる
		SIF LOCAL < 1
			RETURN 0

		;表現のみ制限(処理は変わらず)
		TFLAG:226 = LOCAL
		TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
		TFLAG:226 = MIN(TFLAG:226, 10)

		DRAWLINE
		;地の文章カット
		IF CFLAG:99 == 0
			PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
			PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
		ENDIF

		;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
		CALL SELF_KOJO, TARGET, 150, 2

		;Ａ経験
		CALL COMMON_UP_EXP,TARGET, 1, TFLAG:226, 0
		CFLAG:174 += TFLAG:226
		;快Ａ의 구슬
		CALL COMMON_UP_JUEL, TARGET, 2, (LOCAL*200), 0
		;恭順의 구슬
		CALL COMMON_UP_JUEL, TARGET, 12, (LOCAL*100), 0
		;欲情의 구슬
		CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*250), 1
		;Ａ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
		IF (ABL:2 + ABL:10 + ABL:13) >= 13 && CFLAG:99 == 0
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 아직 미련이 남은 듯 옷자락을 잡아끌었다.
			PRINTFORMW 하지만 %조사처리(CALLNAME:MASTER,"는")% 그 손을 뿌리치고, 방을 뒤로 했다….
		ENDIF
	ENDIF
;タイプ3
ELSEIF TALENT:120 || TALENT:121
	;主人が非処女じゃないとダメ
	SIF TALENT:MASTER:0 || EXP:MASTER:0 <= 0
		RETURN 0

	;Ｃ感覚
	IF ABL:0 == 0
		LOCAL = 0
	ELSEIF ABL:0 == 1
		LOCAL = 1
	ELSEIF ABL:0 <= 3
		LOCAL = 2
	ELSE
		LOCAL = 3
	ENDIF

	;欲望と奉仕精神によるボーナス
	IF ABL:13 >= 4 && PALAM:13 >= PALAMLV:4
		IF ABL:11 >= 5
			LOCAL += 2
		ELSEIF ABL:11 == 4
			LOCAL += 1
		ELSEIF ABL:11 == 3 && ABL:13 >= 5
			LOCAL += 1
		ENDIF
	ENDIF
	;快感に素直, 快感の否定
	IF TALENT:70 && LOCAL
		LOCAL += 1
	ELSEIF TALENT:71 && LOCAL
		LOCAL -= 2
	ENDIF
	;淫乱
	SIF TALENT:170 && LOCAL
		LOCAL += 1
	;ある程度時間が経過している
	IF TFLAG:107 > 50 && LOCAL
		LOCAL -= 1
	;殆ど時間が経過していない
	ELSEIF TFLAG:107 < 10 && LOCAL
		LOCAL += 1
	ENDIF

	;条件を満たさない場合は処理をここで終了させる
	SIF LOCAL < 1
		RETURN 0

	;表現のみ制限(処理は変わらず)
	TFLAG:226 = LOCAL
	TFLAG:226 = TABLE_CALC(TFLAG:226, 5, 2)
	TFLAG:226 = MIN(TFLAG:226, 10)

	DRAWLINE
	;地の文章カット
	IF CFLAG:99 == 0
		PRINTFORM %조사처리(CALLNAME:MASTER,"과")% %조사처리(CALLNAME:TARGET,"는")% 아직 식지 않은 흥분을 억누르지 못하고, 
		PRINTFORML 다시 {TFLAG:226}번 침대 위에서 뒹굴었다….
	ENDIF

	;調教後セックス口上(TFLAG:99によるタイプわけをしてください)
	CALL SELF_KOJO, TARGET, 150, 3

	;主人のＶ経験
	CALL COMMON_UP_EXP,MASTER, 0, TFLAG:226, 0
	SIF TALENT:121 || TALENT:1
		CFLAG:175 += TFLAG:226
	;レズ経験
	SIF TALENT:121 || TALENT:1
		CALL COMMON_UP_EXP,TARGET, 12, TFLAG:226, 0
	;快Ｃ의 구슬
	CALL COMMON_UP_JUEL, TARGET, 0, (LOCAL*150), 0
	;恭順의 구슬
	CALL COMMON_UP_JUEL, TARGET, 12, (LOCAL*100), 0
	;欲情의 구슬
	CALL COMMON_UP_JUEL, TARGET, 13, (LOCAL*250), 1
	;Ｃ感覚＋従順＋奉仕精神が大きい奴隷は名残を惜しむ描写を地の文章表示のときにはおこなう
	IF (ABL:0 + ABL:10 + ABL:13) >= 13 && CFLAG:99 == 0
