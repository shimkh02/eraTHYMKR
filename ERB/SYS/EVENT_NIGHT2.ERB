	SIF TALENT:(LOCAL:2):120 || TALENT:(LOCAL:2):121
		LOCAL:11 += 1
	SIF TALENT:(LOCAL:3):120 || TALENT:(LOCAL:3):121
		LOCAL:11 += 1
	SIF TALENT:(LOCAL:4):120 || TALENT:(LOCAL:4):121
		LOCAL:11 += 1
