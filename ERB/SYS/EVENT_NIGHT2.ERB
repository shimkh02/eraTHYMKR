		LOCAL:13 += 1
	SIF TALENT:(LOCAL:3):120
		LOCAL:13 += 1
	SIF TALENT:(LOCAL:4):120
		LOCAL:13 += 1
	SIF TALENT:(LOCAL:5):120
		LOCAL:13 += 1

	;경험処理開始.
