		CALL COMMON_UP_EXP, LOCAL:1, 20, (ARG*(LOCAL+LOCAL:11)*ABL:(LOCAL:1):12)/LOCAL, 0
	ELSE
		CALL COMMON_UP_EXP, LOCAL:1, 20, (ARG*(LOCAL+LOCAL:11)*ABL:(LOCAL:1):13)/LOCAL, 0
	ENDIF
	;22펠라경험
	CALL COMMON_UP_EXP, LOCAL:1, 22, (ARG*LOCAL:11)/(LOCAL*3), 0
	;23키스경험
	LOCAL:12 = ARG*2
