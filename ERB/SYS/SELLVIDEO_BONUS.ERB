;--------------------------------------------------
;타이틀 결정 판별용
;--------------------------------------------------
@JUDGE_VIDEOTITLE
#FUNCTION
LOCAL:1 = 99
IF TFLAG:120
	LOCAL:1 = 98
ELSEIF GET_GENRE(1) >= 8 && GET_GENRE(2) >= 8
	LOCAL:1 = 97
ELSEIF GET_GENRE(14) >= 8 && GET_GENRE(15) >= 8
	LOCAL:1 = 96
ELSE
	FOR LOCAL, 0, 16
		LOCAL:2 = GET_GENRE(LOCAL)
		SIF LOCAL:2 >= 8 && LOCAL:2 >= GET_GENRE(LOCAL:1)
				LOCAL:1 = LOCAL
	NEXT
ENDIF
RETURNF LOCAL:1

