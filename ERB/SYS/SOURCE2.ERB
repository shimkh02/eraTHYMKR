ELSEIF (TALENT:152 || TALENT:153) && ((SELECTCOM >= 40 && SELECTCOM < 60) || (SELECTCOM >= 303 && SELECTCOM < 307) || (SELECTCOM >= 601 && SELECTCOM < 605) || SELECTCOM == 629)
	TIMES UP:11 , 0.75
