	;ARG:1 = TABLE_CALC(ARG:1, 1000000, 10)
	;ARG:1 = TABLE_CALC(ARG:1, 5000000, 20)

	;토리코모드
	ARG:1 = 0

	LOCALS = 유명
ENDIF
LOCAL -= ARG:1
SIF LOCAL > 0
	PRINTFORML %NAME:ARG%：%LOCALS% 캐릭터 －{ARG:1}

