;MARK:8をみる(苦痛のみ)
IF ARG:1 == 16
	IF MARK:ARG:8 == 0
		TIMES ARG:2 , 0.50
	ELSEIF MARK:ARG:8 == 1
		TIMES ARG:2 , 0.65
	ELSEIF MARK:ARG:8 == 2
		TIMES ARG:2 , 0.80
	ELSE
		TIMES ARG:2 , 1.00
	ENDIF
ENDIF
;MARK:9をみる
IF MARK:ARG:9 == 0
	TIMES ARG:2 , 1.00
ELSEIF MARK:ARG:9 == 1
	TIMES ARG:2 , 0.75
ELSEIF MARK:ARG:9 == 2
	TIMES ARG:2 , 0.50
ELSE
	TIMES ARG:2 , 0.50
ENDIF

;珠の種類により個々の補正を掛ける
SELECTCASE ARG:1
	CASE 0
	CASE 1
	CASE 2
	CASE 3
	CASE 11
		ARG:2 = CALC_POSITIVE_DAILYJUEL_11(ARG, ARG:2)
	CASE 12
		ARG:2 = CALC_POSITIVE_DAILYJUEL_12(ARG, ARG:2)
	CASE 13
		ARG:2 = CALC_POSITIVE_DAILYJUEL_13(ARG, ARG:2)
	CASE 14
		ARG:2 = CALC_POSITIVE_DAILYJUEL_14(ARG, ARG:2)
	CASE 15
		ARG:2 = CALC_POSITIVE_DAILYJUEL_15(ARG, ARG:2)
	CASE 16
		ARG:2 = CALC_POSITIVE_DAILYJUEL_16(ARG, ARG:2)
	CASE 17
		ARG:2 = CALC_POSITIVE_DAILYJUEL_17(ARG, ARG:2)
	CASE 32
		ARG:2 = CALC_POSITIVE_DAILYJUEL_32(ARG, ARG:2)
ENDSELECT

;CFLAG:好感度をみる
IF CFLAG:ARG:2 <= 0
	TIMES ARG:2 , 0.50
ELSEIF CFLAG:ARG:2 < 100
	TIMES ARG:2 , 0.55
ELSEIF CFLAG:ARG:2 < 250
	TIMES ARG:2 , 0.60
ELSEIF CFLAG:ARG:2 < 500
	TIMES ARG:2 , 0.65
ELSEIF CFLAG:ARG:2 < 750
	TIMES ARG:2 , 0.70
ELSEIF CFLAG:ARG:2 < 1000
	TIMES ARG:2 , 0.75
ELSE
	TIMES ARG:2 , 1.00
ENDIF

