;-------------------------------------------------
;肯定的珠の日常増加時の補正処理
;-------------------------------------------------
@CALC_POSITIVE_DAILYJUEL(ARG, ARG:1, ARG:2)
#FUNCTION
;MARK:0をみる(恐怖のみ)
IF ARG:1 == 16
	IF MARK:ARG:1 == 0
		TIMES ARG:2 , 0.50
	ELSEIF MARK:ARG:1 == 1
		TIMES ARG:2 , 0.65
	ELSEIF MARK:ARG:1 == 2
		TIMES ARG:2 , 0.80
	ELSE
		TIMES ARG:2 , 1.00
	ENDIF
ENDIF
;MARK:1をみる(快C～快Bのみ)
IF ARG:1 >= 0 && ARG:1 <= 9
	IF MARK:ARG:1 == 0
		TIMES ARG:2 , 0.25
	ELSEIF MARK:ARG:1 == 1
		TIMES ARG:2 , 0.50
	ELSEIF MARK:ARG:1 == 2
		TIMES ARG:2 , 0.75
	ELSE
		TIMES ARG:2 , 1.00
	ENDIF
ENDIF
;MARK:2をみる(屈服・恥情・苦痛・恐怖のみ)
IF ARG:1 >= 14 && ARG:1 <= 17
	IF MARK:ARG:2 == 0
		TIMES ARG:2 , 0.50
	ELSEIF MARK:ARG:2 == 1
		TIMES ARG:2 , 0.65
	ELSEIF MARK:ARG:2 == 2
		TIMES ARG:2 , 0.80
	ELSE
		TIMES ARG:2 , 1.00
	ENDIF
ENDIF
;MARK:4をみる(欲情・屈服・恥情のみ)
IF ARG:1 >= 13 && ARG:1 <= 15
	IF MARK:ARG:4 == 0
		TIMES ARG:2 , 0.50
	ELSEIF MARK:ARG:4 == 1
		TIMES ARG:2 , 0.65
	ELSEIF MARK:ARG:4 == 2
		TIMES ARG:2 , 0.80
	ELSE
		TIMES ARG:2 , 1.00
	ENDIF
