	;コマンドが存在しているかどうか
	STRLENS TRAINNAME:COUNT
	SIF RESULT < 1
		CONTINUE
	CALLF SET_LIST, COUNT, 1
	LOCAL:3 += 1
	SIF (LOCAL:3%60) == 1 && LOCAL:3 > 60
