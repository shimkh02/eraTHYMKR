	SIF LOCAL:2 > (LOCAL * 60) && LOCAL:2 <= ((LOCAL + 1) * 60)
		PRINTFORMLC [{COUNT, 3}]%ITEMNAME:(COUNT+100)%
REND
PRINTL 
DRAWLINE
PRINTFORMLC \@(LOCAL <= 0) ?  %" " * 16% # [1001] 앞페이지\@
PRINTLC [1000] 돌아가기
PRINTFORMLC \@(LOCAL >= LOCAL:1) ?  %" " * 16% # [1009] 다음페이지\@

