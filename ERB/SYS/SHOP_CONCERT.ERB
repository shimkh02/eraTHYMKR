	TIMES LOCAL , 0.90
;감정결여
SIF TALENT:22
	TIMES LOCAL , 0.90
;호기심
SIF TALENT:23
	TIMES LOCAL , 1.10
;억압
SIF TALENT:32
	TIMES LOCAL , 0.80
;해방
SIF TALENT:33
	TIMES LOCAL , 1.20
;부끄럼쟁이
SIF TALENT:35
	TIMES LOCAL , 0.90
;부끄럼없음
SIF TALENT:36
