;조교 중의 추가 처리 및 서브 이벤트
;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡
;=============================================================================
;조교 경험 관련의 처리
;=============================================================================
;-------------------------------------------------
;주인조교경험 · 호감도
;-------------------------------------------------
;호감도는 100만 이상으로 상승시키기란 불가능
@MASTER_FLAG_CHECK
;절정(에 여기서 조교 대상의 사정과 분유와 방뇨를 더하고 있음)
TFLAG:51 += TFLAG:30 + TFLAG:31 + TFLAG:32

LOCAL:1 = 0
;이번 조교 턴에서의 Ｃ절정, Ｖ절정, Ａ절정, Ｂ절정
REPEAT 4
	SIF NOWEX:COUNT > 0
		LOCAL:1 += 1
REND
;이번 조교 턴에서의 조교 대상의 사정
SIF TFLAG:30 > 0
	LOCAL:1 += 1
;이번 조교 턴에서의 조교 대상의 분유
SIF TFLAG:31 > 0
	LOCAL:1 += 1
;이번 조교 턴에서의 조교 대상의 방뇨
SIF TFLAG:32 > 0
	LOCAL:1 += 1
;주인경험에 절정＋이것저것 추가된 것을 더하고 있음
TFLAG:50 += TFLAG:51 * LOCAL:1

;사정
FOR COUNT, 8, 25
	SIF TFLAG:COUNT > 0 && (EXP:4 + TCVAR:4) >= EXPLV:3
		TFLAG:50 += TFLAG:COUNT
NEXT

;주인(아나타)와 대상의 능력을 계산
;ABL:순종을 본다
TFLAG:97 = ABL:10

;주인과 대상이 동성
IF !(TALENT:MASTER:120 ^^ TALENT:120)
	;男ならＢＬっ気, 女ならレズっ気を見る
	LOCAL = 22 + TALENT:120
	IF ABL:LOCAL == 0
		TFLAG:97 -= 1
	ELSEIF ABL:LOCAL < 3
		TFLAG:97 += 0
	ELSEIF ABL:LOCAL < 5
		TFLAG:97 += 1
	ELSE
		TFLAG:97 += 2
	ENDIF
	;男ならＢＬ中毒, 女ならレズ中毒を見る
	LOCAL = 32 + TALENT:120
	IF ABL:LOCAL > 4
		TFLAG:97 += 2
	ELSEIF ABL:LOCAL > 2
		TFLAG:97 += 1
	ENDIF
	;両刀
	SIF TALENT:83
		TFLAG:97 += 1
ENDIF
;素質による増減
;反抗的
SIF TALENT:11
	TFLAG:97 -= 1
;素直
SIF TALENT:13 && TFLAG:97 > 0
	TFLAG:97 += 1
;자제심
SIF TALENT:20
	TFLAG:97 -= 1
;感情乏しい
SIF TALENT:22
	TFLAG:97 -= 1
;抵抗
SIF TALENT:34
	TFLAG:97 -= 1
;헌신적
SIF TALENT:63 && TFLAG:97 > 0
	TFLAG:97 += 1
;쾌감에 솔직
IF TALENT:70
	TFLAG:97 += 1
;쾌감을 부정
ELSEIF TALENT:71
	TFLAG:97 -= 1
ENDIF
;男嫌い
IF TALENT:37 && TALENT:MASTER:120
	TFLAG:97 -= 1
;女嫌い
ELSEIF TALENT:38 && TALENT:MASTER:120 == 0
	TFLAG:97 -= 1
ENDIF
;연모
SIF TALENT:150
	TFLAG:97 += 2
;망신・상애
SIF TALENT:151 || TALENT:153
	TFLAG:97 += 2
;친애・괴조인격
SIF TALENT:152 || TALENT:169
	TFLAG:97 += 3
;服従
SIF TALENT:160
	TFLAG:97 += 2
;烙印
SIF TALENT:161
	TFLAG:97 += 1
;隷属
SIF TALENT:162
	TFLAG:97 += 3

;주인의 매력
SIF TALENT:MASTER:90
	TFLAG:97 += 1
;주인의 매혹
SIF TALENT:MASTER:91
	TFLAG:97 += 1
;주인의 수수께끼의 매력
SIF TALENT:MASTER:92
	TFLAG:97 += 1
;주인의 인기
SIF TALENT:MASTER:94
	TFLAG:97 += 1
;YASAI 모드
SIF TALENT:MASTER:831 && TEQUIP:78
	TFLAG:97 += 1

;촉수 임플란트
SIF CFLAG:41
	CALL TENTACLE_INPLANT

TFLAG:97 += TFLAG:50 / 2
;상성에 따른 수정
LOCAL:1 = TRAIN_RELATION_CALC(MASTER)
IF LOCAL:1 > 0 && LOCAL:1 != 100 && TFLAG:97 > 0
	TFLAG:97 *= LOCAL:1
	TFLAG:97 /= 100
ENDIF

;반발각인 보정
TFLAG:97 -= (FLAG:3 < 3) ? 2 * MARK:9 # 5 * MARK:9

;非陥落なら, 恐怖각인 보정(難易度HARD 이상のみ)
SIF FLAG:3 > 2 && TALENT:150 == 0 && TALENT:160 == 0 && TALENT:170 == 0
	TFLAG:97 -= 2 * MARK:8

;ふぉーゆーっ！がしつこすぎると嫌われる
IF TFLAG:143 > 50
	TFLAG:97 -= 10
ELSEIF TFLAG:143 >= 5
	TFLAG:97 -= TFLAG:143 / 5
ENDIF

;神霊の場合, 主人が[수수께끼의 매력]を持っていないとなびきにくい
SIF TALENT:222 && TALENT:MASTER:92 == 0 && TFLAG:97 > 0
	TIMES TFLAG:97 , 0.50
;半神の場合も主人が[수수께끼의 매력]または[매혹]を持っていないとなびきにくい
SIF TALENT:223 && TALENT:MASTER:91 == 0 && TALENT:MASTER:92 == 0 && TFLAG:97 > 0
	TIMES TFLAG:97 , 0.75
;死神の場合, 主人が[仙人]ならばなびきにくい
SIF TALENT:224 && TALENT:MASTER:225
	TIMES TFLAG:97 , 0.75
;仙人の場合, 主人が[死神]ならばなびきにくい
SIF TALENT:225 && TALENT:MASTER:224
	TIMES TFLAG:97 , 0.75

;結果が一定数 미만の場合
IF MARK:9 >= 3 && TFLAG:97 < -3
	TFLAG:97 = -3
ELSEIF MARK:9 == 2 && TFLAG:97 < -2
	TFLAG:97 = -2
ELSEIF MARK:9 == 1 && TFLAG:97 < -1
	TFLAG:97 = -1
ELSEIF TFLAG:97 < 0
	TFLAG:97 = 0
ENDIF

;정신붕괴の場合, 호감도가 変化しない
IF TALENT:167
	TFLAG:97 = 0
;괴뢰の場合, 호감도가 上昇しにくい
ELSEIF TALENT:168
	TFLAG:97 /= 4
;無関心の場合, 연모がないと호감도가 上がりにくい
ELSEIF TALENT:21 && TALENT:150 == 0
	IF TFLAG:97 > 1
		TFLAG:97 /= 2
	ELSEIF TFLAG:97 > 0
		TFLAG:97 = 1
	ENDIF
	SIF ABL:10 >= 4
		TFLAG:97 += 1
ENDIF
;お香の効果(ムスク・フランキンセンス)
SIF (TEQUIP:89 == 5 || TEQUIP:89 == 6) && TEQUIP:52 == 0 && TEQUIP:53 == 0 && TEQUIP:55 == 0
	TFLAG:97 += RAND:3

;何もしないを使用した場合は上限を0にする
SIF TFLAG:97 > 0 && SELECTCOM == 9
	TFLAG:97 = 0
;ふぉーゆーっ！がしつこすぎると嫌われる
SIF TFLAG:143 > 10
	TFLAG:97 -= 1

;睡眠中・時止め中・실신 중は変動なし
IF EQUIP:14 || TEQUIP:71 || TFLAG:899 > 0
	TFLAG:97 = 0
;獣姦계 커맨드は호감도の上昇が最大で0
ELSEIF TFLAG:98 == 9 && TFLAG:97 > 0
	TFLAG:97 = 0
;촉수プレイ中は호감도減少(촉수中毒が大きいと減少値は小さくなる)
ELSEIF TEQUIP:90
	TFLAG:97 -= (5 - ABL:35)
;裸エプロン装着中は호감도가 2 이상アップすると愛情経験が＋1
ELSEIF TFLAG:97 > 1 && TEQUIP:59 == 1
	TCVAR:21 += 1
;상애持ちの場合ウェディングドレスコスプレ中は호감도가 5 이상アップすると愛情経験が＋1（助手はだめ）
ELSEIF TFLAG:97 > 4 && TALENT:153 && TEQUIP:59 == 19
	TCVAR:21 += 1
ENDIF

CFLAG:2 += TFLAG:97
TFLAG:238 += TFLAG:97
;호감도는 0 이하로도 100만 이상으로도 되지 않음
CFLAG:2 = LIMIT(CFLAG:2, 0, 999999)

;フラグが立ってなく, 촉수プレイ中以外は主人調教経験も追加
SIF TEQUIP:90 == 0 && TFLAG:59 == 0
	TCVAR:98 += 1

;호감도 표시
SIF FLAG:12 & 4 && TFLAG:97 != 0
	PRINTFORML \@(TFLAG:97 >= 0) ? 호감도＋{TFLAG:97} # －호감도{-TFLAG:97}\@

;-------------------------------------------------
;주인이 하는 조교의 보너스
;-------------------------------------------------
@MASTER_SKILL_CHECK
;질투
IF TALENT:157 && ASSI < 0
	TIMES SOURCE:11 , 1.30
	TIMES SOURCE:24 , 1.50
	TIMES SOURCE:31 , 0.50
	TIMES SOURCE:32 , 0.50
ENDIF

;주인이 하는 조교가 아니라면 돌아감
SIF ASSIPLAY
	RETURN 0

;주인이 하는 조교경험
IF EXP:98 >= 500
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:11 , 1.30
	TIMES SOURCE:30 , 0.70
	TIMES SOURCE:31 , 0.70
ELSEIF EXP:98 >= 300
	TIMES SOURCE:0 , 1.10
	TIMES SOURCE:1 , 1.10
	TIMES SOURCE:2 , 1.10
	TIMES SOURCE:3 , 1.10
	TIMES SOURCE:11 , 1.20
	TIMES SOURCE:30 , 0.80
	TIMES SOURCE:31 , 0.80
ELSEIF EXP:98 >= 100
	TIMES SOURCE:11 , 1.10
	TIMES SOURCE:30 , 0.90
	TIMES SOURCE:31 , 0.90
ENDIF

;음란, 연모, 친애는 주인경험과의 중복 전제로 계산
;음란
IF TALENT:170
	TIMES SOURCE:0 , 1.80
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:2 , 1.80
	TIMES SOURCE:3 , 1.80
ENDIF

;친애
IF TALENT:152
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:2 , 1.50
	TIMES SOURCE:3 , 1.50
	TIMES SOURCE:11 , 2.00
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:16 , 1.50
	TIMES SOURCE:24 , 1.20
;연모
ELSEIF TALENT:150
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:2 , 1.30
	TIMES SOURCE:3 , 1.30
	TIMES SOURCE:11 , 1.80
ENDIF
;[연모]か[친애]持ちが処女(乙女ではなく)を今回の調教で散らせた場合
IF (TALENT:150 || TALENT:152) && TALENT:0 == 1 && TFLAG:2
	LOCAL:1 = 0
	LOCAL = NO:PLAYER
	;主人との相性が100% 이상で痛みのソースが2000 미만で反感追加のソースが3000以下
	SIF (RELATION:LOCAL == 0 || RELATION:LOCAL >= 100) && SOURCE:20 < 2000 && SOURCE:32 <= 3000
		LOCAL:1 += 1
	;Ｖ系装着器具, Ａ系装着器具, Ｃ系装着器具（コンドーム除く）, Ｕ系装着器具, Ｂ系装着器具, サラシを装着していない
	SIF TEQUIP:20 == 0 && TEQUIP:21 == 0 && TEQUIP:25 == 0 && TEQUIP:26 == 0 && TEQUIP:30 == 0 && TEQUIP:32 == 0 && TEQUIP:35 == 0 && TEQUIP:36 == 0
		LOCAL:1 += 1
	;緊縛しておらず, 目隠しせず, 口封じせず, 木馬に乗せず, ビデオを撮影してなくて, ビデオを鑑賞してなくて, 야외 플레이でなくて, シャワー中でなかったり, 特殊な風呂に入ってなくて, 女体盛り中でなくて, 촉수プレイでない場合
	SIF TEQUIP:40 == 0 && TEQUIP:41 == 0 && TEQUIP:42 == 0 && TEQUIP:43 == 0 && TEQUIP:50 == 0 && TEQUIP:51 == 0 && TEQUIP:52 == 0 && TEQUIP:54 == 0 && TEQUIP:55 == 0 && TEQUIP:60 == 0 && TEQUIP:90 == 0
		LOCAL:1 += 1
	;上の３条件を満たしてなおかつ調教テキストを表示する設定であるならば幸福を感じていることを表示し, なおかつ반발각인回避フラグを立てる
	IF LOCAL:1 == 3 && FLAG:10
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%에게 처녀를 바칠 수 있어서 행복해하고 있다.
		TFLAG:105 = 1
	ENDIF
	;情愛のソース倍増
	TIMES SOURCE:11 , 2.00
	;친애なら反感追加のソースが1/10に
	IF TALENT:152
		TIMES SOURCE:32 , 0.10
	;친애なくても反感追加のソースは30%に
	ELSE
		TIMES SOURCE:32 , 0.30
	ENDIF
ENDIF

;-------------------------------------------------
;조수와의 조교경험의 카운트(임신 패치용)
;-------------------------------------------------
@ASSI_EXP_COUNT
;이 경험의 많고 적음으로 임신 발각시의 분기가 결정된다
;밸런스 조정 필요
LOCAL = 100
LOCAL:1 = 100

LOCAL:2 = NO:ASSI + 1000
LOCAL:3 = NO:TARGET + 1000

;상성에 따른 보정
IF RELATION:(NO:ASSI) != 0
	LOCAL *= RELATION:(NO:ASSI)
	LOCAL /= 100
ENDIF
IF RELATION:ASSI:(NO:TARGET) != 0
	LOCAL:1 *= RELATION:ASSI:(NO:TARGET)
	LOCAL:1 /= 100
ENDIF

;공통 부분
;これまでの経験がかなり多ければプラス補正
LOCAL += CFLAG:(LOCAL:2) / 100
LOCAL:1 += CFLAG:ASSI:(LOCAL:3) / 100
;欲情で補正
IF ABL:11 > 0
	LOCAL *= (100 + ABL:11 * ABL:11 * 4)
	LOCAL /= 100
ENDIF
IF ABL:ASSI:11 > 0
	LOCAL:1 *= (100 + ABL:ASSI:11 * ABL:ASSI:11 * 4)
	LOCAL:1 /= 100
ENDIF
;자제심で補正
SIF TALENT:20
	LOCAL /= 2
SIF TALENT:ASSI:20
	LOCAL:1 /= 2
;無関心で補正
SIF TALENT:21
	TIMES LOCAL, 0.80
SIF TALENT:ASSI:21
	TIMES LOCAL:1, 0.80
;感情乏しいで補正
SIF TALENT:22
	LOCAL /= 4
SIF TALENT:ASSI:22
	LOCAL:1 /= 4
;一線越えないで補正
SIF TALENT:27
	LOCAL /= 2
SIF TALENT:ASSI:27
	LOCAL:1 /= 2
;쾌감을 부정で補正
SIF TALENT:71
	LOCAL /= 3
SIF TALENT:ASSI:71
	LOCAL:1 /= 3
;素直で補正
SIF TALENT:13
	TIMES LOCAL, 1.2
SIF TALENT:ASSI:13
	TIMES LOCAL:1, 1.2
;好奇心で補正
SIF TALENT:23
	TIMES LOCAL, 1.2
SIF TALENT:ASSI:23
	TIMES LOCAL:1 , 1.2
;헌신적で補正
SIF TALENT:63
	TIMES LOCAL, 1.25
SIF TALENT:ASSI:63
	TIMES LOCAL:1, 1.25
;쾌감에 솔직で補正
SIF TALENT:70
	LOCAL *= 2
SIF TALENT:ASSI:70
	LOCAL:1 *= 2
;倒錯的で補正
SIF TALENT:80
	TIMES LOCAL, 1.5
SIF TALENT:ASSI:80
	TIMES LOCAL:1, 1.5
;매력で補正
SIF TALENT:90
	TIMES LOCAL:1, 1.2
SIF TALENT:ASSI:90
	TIMES LOCAL, 1.2
;매혹で補正
SIF TALENT:91
	TIMES LOCAL:1, 1.2
SIF TALENT:ASSI:91
	TIMES LOCAL, 1.2
;수수께끼의 매력で補正
SIF TALENT:92
	TIMES LOCAL:1, 1.3
SIF TALENT:ASSI:92
	TIMES LOCAL, 1.3
;소악마で補正
SIF TALENT:93
	TIMES LOCAL:1, 1.1
SIF TALENT:ASSI:93
	TIMES LOCAL, 1.1
;인기로 보정
SIF TALENT:94
	TIMES LOCAL:1, 1.25
SIF TALENT:ASSI:94
	TIMES LOCAL, 1.25

;소질 의존의 부분(남자 관련)
IF TALENT:120 == 0 && TALENT:ASSI:120 == 0
	;女嫌いならマイナス補正（両方）
	SIF TALENT:38
		LOCAL /= 2
	SIF TALENT:ASSI:38
		LOCAL:1 /= 2
	;両刀ならプラス補正
	SIF TALENT:83
		LOCAL *= 2
	SIF TALENT:ASSI:83
		LOCAL:1 *= 2
	;レズっ気の補正
	IF ABL:22 > 0
		LOCAL *= (100 + ABL:22 * 10)
		LOCAL /= 100
	ENDIF
	IF ABL:ASSI:22 > 0
		LOCAL:1 *= (100 + ABL:ASSI:22 * 10)
		LOCAL:1 /= 100
	ENDIF
	;レズ中毒の補正
	IF ABL:32 > 0
		LOCAL *= (100 + ABL:32 * 25)
		LOCAL /= 100
	ENDIF
	IF ABL:ASSI:32 > 0
		LOCAL:1 *= (100 + ABL:ASSI:32 * 25)
		LOCAL:1 /= 100
	ENDIF
ELSEIF TALENT:120 && TALENT:ASSI:120 == 0
	;女嫌いならマイナス補正（奴隷）
	SIF TALENT:38
		LOCAL /= 2
	;男嫌いならマイナス補正（助手）
	SIF TALENT:37
		LOCAL:1 /= 2
ELSEIF TALENT:120 == 0 && TALENT:ASSI:120
	;男嫌いならマイナス補正（奴隷）
	SIF TALENT:37
		LOCAL:1 /= 2
	;女嫌いならマイナス補正（助手）
	SIF TALENT:38
		LOCAL /= 2
ELSE
	;男嫌いならマイナス補正（両方）
	SIF TALENT:37
		LOCAL /= 2
	SIF TALENT:ASSI:37
		LOCAL:1 /= 2
	;両刀ならプラス補正
	SIF TALENT:83
		LOCAL *= 2
	SIF TALENT:ASSI:83
		LOCAL:1 *= 2
ENDIF

;조수조교경험 가산 처리(100000까지)
CFLAG:(LOCAL:2) = MIN(CFLAG:(LOCAL:2) + LOCAL / 100, 100000)
CFLAG:ASSI:(LOCAL:3) = MIN(CFLAG:ASSI:(LOCAL:3) + LOCAL:1 / 100, 100000)

;조수 조교 중에는 조수경험도 추가
TCVAR:ASSI:99 += 1

