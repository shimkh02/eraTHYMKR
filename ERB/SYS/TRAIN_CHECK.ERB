@ALCOHOL_DRUNK_CHECK, ARG
;조교 대상만 ○절정で酔いの深度が微弱上昇
IF ARG == TARGET
	LOCAL = 0
	LOCAL:1 = 0
	REPEAT 4
		IF NOWEX:COUNT
			LOCAL += NOWEX:COUNT
			LOCAL:1 += 1
		ENDIF
	REND
	IF LOCAL:1 == 0
		LOCAL *= 1
	ELSEIF LOCAL:1 == 1
		LOCAL *= 2
	ELSEIF LOCAL:1 == 2
		LOCAL *= 4
	ELSEIF LOCAL:1 == 3
		LOCAL *= 7
	ELSE
		LOCAL *= 10
	ENDIF
	BASE:ARG:8 += LOCAL
