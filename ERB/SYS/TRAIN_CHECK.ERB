IF (UP:16 >= 7500 || UP:16 + LOCAL:1 >= 15000) && TFLAG:899 < 1 && RAND:10 < 5
	IF TFLAG:895 == 0
		TFLAG:895 = 2
		TFLAG:898 = 2
		TCVAR:55 += 1
		PRINTL 실신
	ELSEIF TFLAG:895 == 1
		TFLAG:895 = 4
		TFLAG:898 = 2
	ENDIF
ENDIF

