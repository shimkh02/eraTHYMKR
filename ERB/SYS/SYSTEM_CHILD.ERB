TALENT:110 = 0
TALENT:111 = 0
TALENT:112 = 0
TALENT:113 = 0
IF TALENT:120 == 0
	IF TALENT:(TFLAG:652):110 || TALENT:(TFLAG:652):111
		TALENT:110 = 1
	ELSEIF TALENT:(TFLAG:652):113
		TALENT:112 = 1
