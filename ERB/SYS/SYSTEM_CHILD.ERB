	;말할 수 있는 인형은 유전(LOCAL:2に退避)
	SIF COUNT == 208
		LOCAL:2 = TALENT:COUNT
	;[구문지][황혼의 괴물][신령][반신][사신]은 초기화하지 않음
	SIF COUNT == 216 || COUNT == 221 || COUNT == 222 || COUNT == 223 || COUNT == 224
		CONTINUE
	TALENT:COUNT = 0
NEXT

