;-------------------------------------------------
;EVENTEND関係（32をキャラ番号に置換）
;調教終了時のセリフ
;-------------------------------------------------
@LES_EVENTEND_K0_M32
SIF FLAG:11 == 0
	RETURN 0
SIF NO:TARGET != 0
	RETURN 0
SIF NO:MASTER != 32
	RETURN 0
;--------------------------------------------------
;調教終了時のセリフ CFLAG 211～220を使用
;--------------------------------------------------
DRAWLINE
;髪に汚れ有り
IF STAIN:7
	PRINTFORMW 「今日はこれで終わりですけど……」
	PRINTFORMW 「ちょっと来てください」
	PRINTFORMW 「髪、洗いますよ？」
	PRINTL  
	PRINTW ………
	PRINTW ……
	PRINTW …
	;反発刻印Lv3
	IF MARK:9 == 3
		PRINTFORMW 「こらこら暴れない」
		IF TALENT:120
			PRINTFORMW 「キレイにしてあげますから、大人しく……」
			PRINTFORMW 「あぁ、もう……」
		ELSE
			PRINTFORMW 「キレイな髪まで失いたいんですか？」
			PRINTFORMW 「……そうです。最初からそうしてれば良いんです」
		ENDIF
	;恋慕
	ELSEIF TALENT:150
		PRINTFORMW 「ここまで気持ちよさそうにされると、嬉しいですね」
		IF BASE:0 < 500
			PRINTFORMW 「……さ、終わりましたよ」
			PRINTFORMW 「後はゆっくり休んでください」
		ELSE
			PRINTFORMW 「……さ、終わりましたけど……」
			PRINTFORMW 「ええ、ちょっと我慢できませんね」
			PRINTFORMW 「頂きます」
		ENDIF
	;屈服刻印LV2以上
	ELSEIF MARK:2 >= 2
		PRINTFORMW 「はい、キレイになりましたよ」
		PRINTFORMW 「うーん、やっぱりあなたの髪は触っていて気持ちいいです」
		PRINTFORMW 「もうちょっとだけ、じっとしていて……」
		IF MARK:2 >= 3
			PRINTFORMW 「ややっ、ありがたいです、感謝です」
			PRINTFORMW 「……あー、いけませんねぇ。こうして弄っていると、また掛けたく……」
			PRINTFORMW 「まあ、二度手間は避けたいので我慢です」
		ELSE
			PRINTFORMW 「あ、イヤですか。仕方ないですね」
			PRINTFORMW 「次を待つとしましょう」
		ENDIF
	ELSE
		PRINTFORMW 「はい、キレイになりましたよ」
		PRINTFORMW 「……と、もうちょっと触らせてくれたって良いじゃないですかぁ」
	ENDIF
	;髪洗ったフラグ
	CFLAG:5202 |= 2
;髪に汚れ無し
ELSE
	;反発刻印Lv3
	IF MARK:9 == 3
		PRINTFORMW 「その虚勢がいつまで続くか見物ですね」
		RETURN 1
	;屈服刻印Lv1以下
	ELSEIF MARK:2 <= 1
		PRINTFORMW 「今回はこれくらいにしておきましょう」
		RETURN 1
	;屈服刻印Lv2
	ELSEIF MARK:2 == 2
		PRINTFORMW 「今回はこれくらいにしておきましょう」
		PRINTFORMW 「この次は……まあ、皆まで言うのは止めておきましょうか」
		RETURN 1
	;屈服刻印Lv3
	ELSEIF MARK:2 >= 3 && TALENT:150 == 0
		PRINTFORMW 「もう少し抵抗してもらっても構いませんよ？」
		RETURN 1
	;恋慕＋体力500未満
	ELSEIF TALENT:150 && BASE:0 < 500
		PRINTFORMW 「倒れられても困るので、ちゃんと休んでください」
		PRINTFORMW 「……あ、それ以上の意味はないので、勘違いされても困りますよ？」
		RETURN 1
	;恋慕
	ELSEIF TALENT:150
		PRINTFORMW 「今日はこれでおしまいです」
		PRINTFORMW 「……あんまりワガママ言ってると売っちゃいますよ？」
		RETURN 1
	ENDIF
ENDIF
RETURN 0

