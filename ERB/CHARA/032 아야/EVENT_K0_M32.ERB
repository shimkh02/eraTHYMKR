;-------------------------------------------------
LOSEBASE:0 += 10
LOSEBASE:1 += 30

;ABL:0をみる
IF ABL:0 == 0
	A = 20
	B = 40
ELSEIF ABL:0 == 1
	A = 70
	B = 100
ELSEIF ABL:0 == 2
	A = 150
	B = 160
ELSEIF ABL:0 == 3
	A = 350
	B = 370
ELSEIF ABL:0 == 4
	A = 550
	B = 600
ELSE
	A = 700
	B = 800
ENDIF

;ABL:12をみる
IF ABL:PLAYER:12 == 0
	TIMES A , 0.50
	TIMES B , 0.50
ELSEIF ABL:PLAYER:12 == 1
	TIMES A , 0.80
	TIMES B , 0.80
ELSEIF ABL:PLAYER:12 == 2
	TIMES A , 1.00
	TIMES B , 1.00
ELSEIF ABL:PLAYER:12 == 3
	TIMES A , 1.50
	TIMES B , 1.50
ELSEIF ABL:PLAYER:12 == 4
	TIMES A , 2.50
	TIMES B , 2.50
ELSE
	TIMES A , 4.00
	TIMES B , 4.00
ENDIF

;PALAM:13をみる
IF PALAM:13 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:13 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:13 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:13 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;ABL:10をみる
IF ABL:10 == 0
	TIMES A , 0.80
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES A , 0.90
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES A , 1.00
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES A , 1.10
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES A , 1.20
	TIMES B , 1.20
ELSE
	TIMES A , 1.30
	TIMES B , 1.30
ENDIF

SOURCE:0 += A
SOURCE:11 += B

EXP:PLAYER:0 += 1

IF TALENT:121 || TALENT:121 || TALENT:120
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:2 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:2
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	PRINTFORML 레즈경험＋4
	EXP:12 += 4
ENDIF



;-------------------------------------------------
;同フェラ
;Pが立ってるとパイズリ
;-------------------------------------------------
@COMBOPLAY_F
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;パイズリ
IF P
	;ABL:12をみる
	IF ABL:12 == 0
		B = 100
	ELSEIF ABL:12 == 1
		B = 210
	ELSEIF ABL:12 == 2
		B = 350
	ELSEIF ABL:12 == 3
		B = 500
	ELSEIF ABL:12 == 4
		B = 600
	ELSE
		B = 800
	ENDIF
;フェラ
ELSE
	;ABL:12をみる
	IF ABL:12 == 0
		B = 80
	ELSEIF ABL:12 == 1
		B = 190
	ELSEIF ABL:12 == 2
		B = 300
	ELSEIF ABL:12 == 3
		B = 420
	ELSEIF ABL:12 == 4
		B = 550
	ELSE
		B = 700
	ENDIF
ENDIF


;ABL:10をみる
IF ABL:10 == 0
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:13をみる
IF ABL:13 == 0
	TIMES B , 0.50
ELSEIF ABL:13 == 1
	TIMES B , 0.80
ELSEIF ABL:13 == 2
	TIMES B , 1.20
ELSEIF ABL:13 == 3
	TIMES B , 1.50
ELSEIF ABL:13 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;ABL:31をみる
IF ABL:31 == 0
	TIMES B , 1.00
ELSEIF ABL:31 == 1
	TIMES B , 1.20
ELSEIF ABL:31 == 2
	TIMES B , 1.30
ELSEIF ABL:31 == 3
	TIMES B , 1.50
ELSEIF ABL:31 == 4
	TIMES B , 1.70
ELSE
	TIMES B , 2.00
ENDIF

;TALENT:舌使いをみる
SIF TALENT:52
	TIMES B , 2.00

;TALENT:猫舌をみる
SIF TALENT:54
	TIMES B , 0.70

;プレイヤーのABL:0をみる
IF ABL:PLAYER:0 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:0 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:0 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:0 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:0 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:120
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

LOSEBASE:0 += 10
LOSEBASE:1 += 150

SOURCE:12 = 1500
SOURCE:22 = 500

;上のほうで計算した汚れデータ
SOURCE:24 = Y*20 + 50

;ABL:13をみる
IF ABL:13 == 0
	A = 200
	B = 80
	TIMES SOURCE:24 , 4.00
ELSEIF ABL:13 == 1
	A = 240
	B = 150
	TIMES SOURCE:24 , 2.50
ELSEIF ABL:13 == 2
	A = 300
	B = 300
	TIMES SOURCE:24 , 1.50
ELSEIF ABL:13 == 3
	A = 330
	B = 450
	TIMES SOURCE:24 , 1.00
ELSEIF ABL:13 == 4
	A = 370
	B = 750
	TIMES SOURCE:24 , 0.50
ELSE
	A = 400
	B = 1000
	TIMES SOURCE:24 , 0.10
ENDIF


;ABL:12をみる
IF ABL:12 == 0
	TIMES A , 0.50
	TIMES B , 0.50
ELSEIF ABL:12 == 1
	TIMES A , 0.80
	TIMES B , 0.80
ELSEIF ABL:12 == 2
	TIMES A , 1.00
	TIMES B , 1.00
ELSEIF ABL:12 == 3
	TIMES A , 1.20
	TIMES B , 1.20
ELSEIF ABL:12 == 4
	TIMES A , 1.50
	TIMES B , 1.50
ELSE
	TIMES A , 2.00
	TIMES B , 2.00
ENDIF

SOURCE:11 += A
SOURCE:15 += B

IF P
	;ABL:3をみる
	IF ABL:3 == 0
		A = 100
	ELSEIF ABL:3 == 1
		A = 200
	ELSEIF ABL:14== 2
		A = 400
	ELSEIF ABL:3 == 3
		A = 800
	ELSEIF ABL:3 == 4
		A = 1200
	ELSE
		A = 1500
	ENDIF
	
	IF TALENT:174
		TIMES A , 1.20
	ENDIF
	IF TALENT:112
		TIMES A , 1.20
	ENDIF
	IF TALENT:106
		TIMES A , 1.20
	ELSEIF TALENT:107
		TIMES A , 0.70
	ENDIF
	
	SOURCE:17 += A
ENDIF

;-------------------------------------------------
;사정 체크
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:11 , 3.00

	;ABL:31をみる
	IF ABL:31 == 0
		SOURCE:20 = 0
		TIMES SOURCE:15 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:31 == 1
		SOURCE:20 = 500
		TIMES SOURCE:15 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:31 == 2
		SOURCE:20 = 1200
		TIMES SOURCE:15 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:31 == 3
		SOURCE:20 = 3000
		TIMES SOURCE:15 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:31 == 4
		SOURCE:20 = 6000
		TIMES SOURCE:15 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:20 = 12000
		TIMES SOURCE:15 , 15.00
		TIMES SOURCE:13, 1.00
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:20 , 2.00
	TIMES SOURCE:15 , 1.50

	EXP:PLAYER:3 += 2
	EXP:4 += 9
	PRINTL 대량사정
	PRINTL 정액경험＋９
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:11 = 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:4 += 3
	PRINTL 사정
	PRINTL 정액경험＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で射精させたフラグ
	TFLAG:11 = 1
ENDIF

PRINTFORML 펠라경험＋１
EXP:22 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

IF P
	;奴隷のＢ⇔調教者のＰの汚れが移動
	STAIN:5 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:5
	
	;奴隷の口⇔調教者のＰの汚れが移動
	STAIN:0 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:0
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:13 >= 2 && ABL:12 >= 2
	STAIN:PLAYER:2 = 2
	SIF E >= 1
		TFLAG:21 = 1
ENDIF

IF TALENT:120 == 0 && TALENT:PLAYER:120 == 0
	PRINT 레즈경험
	PRINTL +7
	EXP:12 += 7
ELSEIF TALENT:120 == 1 && TALENT:PLAYER:120 == 1
	PRINT ＢＬ경험
	PRINTL +7
	EXP:13 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:50 += 1

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:12 /= 2
ENDIF



