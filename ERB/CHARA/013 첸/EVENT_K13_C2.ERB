;-------------------------------------------------
;콘돔
;-------------------------------------------------
@K13_MESSAGE_COM_290
SELECTCASE TFLAG:99
	;양자 장착 가능해 조교자가 착탈
	CASE 0
		$COM290_K13_0
		IF TEQUIP:PLAYER:31 == 0
			;PRINTFORMW 「조교자 콘돔 해제시의 말입니다」
		ELSE
			;PRINTFORMW 「조교자 콘돔 장착시의 말입니다」
		ENDIF
	;양자 장착 가능해 노예가 착탈
	CASE 1
		$COM290_K13_1
		IF TEQUIP:31 == 0
			;PRINTFORMW 「노예 콘돔 해제시의 말입니다」
		ELSE
			;PRINTFORMW 「노예 콘돔 장착시의 말입니다」
		ENDIF
	;조교자만 착탈가능 또는 노예만 착탈가능의 경우
	CASE 2
		;조교자가 착탈
		IF TALENT:PLAYER:120 || TALENT:PLAYER:121
			GOTO COM290_K13_0
		;노예가 착탈
		ELSE
			GOTO COM290_K13_1
		ENDIF
ENDSELECT
