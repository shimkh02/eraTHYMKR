CALL CALC_MISSION_REWARD_TALENT_TORIKOMODE, LOCAL:1
LOCAL:5 = RESULT:0
CALL CALC_MISSION_REWARD_ABILITY_TORIKOMODE, LOCAL:4, LOCAL:3
LOCAL:6 = RESULT:0
CALL CALC_MISSION_REWARD_DAY_TORIKOMODE, LOCAL:0
LOCAL:7 = RESULT:0
LOCAL:9 = ((LOCAL:5*3) + LOCAL:6) * LOCAL:7 /100

;;디버그
