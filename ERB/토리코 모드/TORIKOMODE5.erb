ENDIF

PRINTFORML (%CALLNAME:TARGET%의 %CALLNAME:ASSI%에 대한 상성 : {LOCAL:0}퍼센트)
IF LOCAL:0 + RAND:100 >= 150
	;승인
	RESULT:0 = 0
ELSE
