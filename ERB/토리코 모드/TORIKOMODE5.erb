				PRINTL
			ENDIF
		;1턴 남았다면. 끝내버리자.
		ELSEIF CFLAG:ASSI:4007 == 1 && CFLAG:ASSI:4009 == 1	
			;지난 턴의 누적 체력기력 소모량 초기화
			CALL RESET_DOWNARG_TORIKOMODE
			CFLAG:TARGET:4007 -= 1
			;종료표시
			PRINTL
			PRINTFORMW 「후우...」
