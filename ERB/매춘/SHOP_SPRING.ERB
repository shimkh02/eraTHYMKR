FLAG:3000 |= 1048576
;１週目と, 二週目以降で初期人気が異なる.
IF FLAG:6 > 1
	FLAG:3005 = FLAG:30
	IF FLAG:3005 > 0
		IF FLAG:3005 < 50
			FLAG:3005 /= 2
		ELSEIF FLAG:3005 >= 50 && FLAG:3005 < 150
			FLAG:3005 = 25 + ((FLAG:3005 - 50) / 4)
		ELSEIF FLAG:3005 >= 150 && FLAG:3005 < 2650
			FLAG:3005 = 50 + ((FLAG:3005 - 150) / 10)
		ELSEIF FLAG:3005 >= 2650 && FLAG:3005 < 7650
			FLAG:3005 = 75 + ((FLAG:3005 - 2650) / 20)
		ELSEIF FLAG:3005 >= 7650
			FLAG:3005 = 100
		ENDIF
	ENDIF
ELSE
	FLAG:3005 = 30
	FLAG:3005 += FLAG:30
	FLAG:3005 += FLAG:30
	SIF FLAG:3005 > 100
		FLAG:3005 = 100
ENDIF

