	LOCAL@SPRING_EXP:4 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:77 = 10
	LOCAL@SPRING_DUMMY:5 = 5
ELSEIF RAND:2 == 0
	;기본이 되는 정상정도.
	PRINTFORML %조사처리(CALLNAME:COUNT,"는")% 고객과 정상위로 연결된 채, 몇 번이고 유린당했다.
	TFLAG:200 = 1402
	;V3, 봉사 정신
	LOCAL@SPRING_DUMMY:1 = ABL:COUNT:1
		SIF LOCAL@SPRING_DUMMY:1 >= 5 && (FLAG:3000 & 8388608) == 0
		LOCAL@SPRING_DUMMY:1 = 5
	LOCAL@SPRING_DUMMY:3 += LOCAL@SPRING_DUMMY:1 * 3
	LOCAL@SPRING_DUMMY:1 = ABL:COUNT:13
		SIF LOCAL@SPRING_DUMMY:1 >= 5 && (FLAG:3000 & 8388608) == 0
		LOCAL@SPRING_DUMMY:1 = 5
	LOCAL@SPRING_DUMMY:3 += LOCAL@SPRING_DUMMY:1
	IF TALENT:COUNT:0
		PRINTFORMW 결합부에 맺힌 처녀의 증거에, 고객도 더욱 흥분해 몇번이고 허리를 부딪쳐왔다.
		LOCAL@SPRING_DUMMY:3 += 50
		LOCAL@SPRING_DUMMY:4 |= 4
	ENDIF
	CALL SPRING_HAPPY
	
	IF LOCAL@SPRING_DUMMY:0 >= 3
		PRINTFORMW %조사처리(CALLNAME:COUNT,"는")% 적극적으로 고객의 움직임에 답해, 동시에 절정에 도달했다.
		LOCAL@SPRING_EXP:2 = LOCAL@SPRING_DUMMY:6
	ELSEIF LOCAL@SPRING_DUMMY:0 < 2
		PRINTW 하지만 유감스럽게도 고객들의 욕구를 만족시킬 수는 없었다.
	ENDIF
	SIF LOCAL@SPRING_DUMMY:4 & 1
		LOCAL@SPRING_EXP:1 = LOCAL@SPRING_DUMMY:6
	SIF LOCAL@SPRING_DUMMY:4 & 2
		LOCAL@SPRING_EXP:0 = LOCAL@SPRING_DUMMY:6
	SIF LOCAL@SPRING_DUMMY:4 & 16
		LOCAL@SPRING_EXP:72 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:20 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:4 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:77 = 9
	LOCAL@SPRING_DUMMY:5 = 4
	
ELSE
	;신사인 내용
	PRINTFORML %조사처리(CALLNAME:COUNT,"는")% 고객에게 연결된 채로, 느긋한 이야기를 나누며 즐겼다.
	TFLAG:200 = 1401
	;순종, V2, 봉사 정신
	LOCAL@SPRING_DUMMY:1 = ABL:COUNT:10
		SIF LOCAL@SPRING_DUMMY:1 >= 5 && (FLAG:3000 & 8388608) == 0
		LOCAL@SPRING_DUMMY:1 = 5
	LOCAL@SPRING_DUMMY:3 += LOCAL@SPRING_DUMMY:1
	LOCAL@SPRING_DUMMY:1 = ABL:COUNT:1
		SIF LOCAL@SPRING_DUMMY:1 >= 5 && (FLAG:3000 & 8388608) == 0
		LOCAL@SPRING_DUMMY:1 = 5
	LOCAL@SPRING_DUMMY:3 += LOCAL@SPRING_DUMMY:1 * 2
	LOCAL@SPRING_DUMMY:1 = ABL:COUNT:13
		SIF LOCAL@SPRING_DUMMY:1 >= 5 && (FLAG:3000 & 8388608) == 0
		LOCAL@SPRING_DUMMY:1 = 5
	LOCAL@SPRING_DUMMY:3 += LOCAL@SPRING_DUMMY:1
	;더러운 무시, 쾌감에 솔직해 보너스 2
	SIF TALENT:COUNT:64 || TALENT:COUNT:70
		LOCAL@SPRING_DUMMY:3 += 2
	IF TALENT:COUNT:0
		PRINTFORMW %조사처리(CALLNAME:COUNT,"는")% 처녀였었지만, 여유 있는 성교 덕분인지 아픔은 그다지 없었던 모양이다.
		LOCAL@SPRING_DUMMY:3 += 50
		LOCAL@SPRING_DUMMY:4 |= 4
	ENDIF
	CALL SPRING_HAPPY
	
	IF LOCAL@SPRING_DUMMY:0 >= 3
		PRINTW 연결된 채로 나눈 이야기들에, 고객은 상당히 치유된 모양이다.
	ELSEIF LOCAL@SPRING_DUMMY:0 < 2
		PRINTW 하지만 유감스럽게도 고객들의 욕구를 만족시킬 수는 없었다.
	ENDIF
	LOCAL@SPRING_EXP:0 = LOCAL@SPRING_DUMMY:6
	SIF LOCAL@SPRING_DUMMY:4 & 16
		LOCAL@SPRING_EXP:72 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:20 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:4 = LOCAL@SPRING_DUMMY:6
	LOCAL@SPRING_EXP:77 = 8
	LOCAL@SPRING_DUMMY:5 = 3


ENDIF


